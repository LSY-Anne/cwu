 retrieve="  SELECT 	A.CODE_GB 						AS CLS_CD,   
              	A.CODE 							AS CD,   
              	A.FNAME 						AS CD_NM,
              	NVL(A.ORDER_SEQ, 9999) 	AS SEQ ,
				B.AREA_GB						AS REL_CD,
              	A.ENAME  						AS ENAME,
				A.ETC_CD1 						AS ECD1,
				A.ETC_CD2						AS ECD2,
				A.ETC_CD3 						AS ECD3,
				A.ETC_CD4						AS ECD4,
				A.ETC_CD5 						AS ECD5,
				A.ETC_QTY1					AS EQTY1,
				A.ETC_QTY2					AS EQTY2,
				A.ETC_AMT1					AS EAMT1,
				A.ETC_AMT2 					AS EAMT2
     FROM 	CDDB.KCH102D A, CDDB.KCH101M B
  WHERE 	B.CODE_GB = A.CODE_GB
       AND 	NVL(B.USE_YN,'N') = 'Y' 
       AND 	NVL(A.USE_YN,'N') = 'Y'
UNION ALL
SELECT		'%'								AS CLS_CD,   
             		NULL 								AS CD,   
              	NULL								AS CD_NM,
              	1   								AS SEQ,
              	''  									AS REL_CD, 
              	''  									AS ENAME,
				'' 									AS ECD1,
				''									AS ECD2,
				'' 									AS ECD3,
				''									AS ECD4,
				'' 									AS ECD5,
				0									AS EQTY1,
				0									AS EQTY2,
				0									AS EAMT1,
				0 									AS EAMT2
FROM			DUAL
UNION ALL
SELECT		'%'								AS CLS_CD,   
             		'%' 								AS CD,   
              	'전체'							AS CD_NM,
              	1   								AS SEQ,
              	''  									AS REL_CD, 
              	''  									AS ENAME,
				'' 									AS ECD1,
				''									AS ECD2,
				'' 									AS ECD3,
				''									AS ECD4,
				'' 									AS ECD5,
				0									AS EQTY1,
				0									AS EQTY2,
				0									AS EAMT1,
				0 									AS EAMT2
FROM			DUAL
ORDER BY 1, 4, 2" )
