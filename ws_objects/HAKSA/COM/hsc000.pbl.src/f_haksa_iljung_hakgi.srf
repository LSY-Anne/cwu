global function string f_haksa_iljung_hakgi ();string	ls_hakgi

SELECT	HAKSA_ILJUNG.HAKGI  
INTO		:ls_hakgi  
FROM		HAKSA.HAKSA_ILJUNG  
WHERE		HAKSA_ILJUNG.SIJUM_FLAG = 'Y'
;

return ls_hakgi
end function

