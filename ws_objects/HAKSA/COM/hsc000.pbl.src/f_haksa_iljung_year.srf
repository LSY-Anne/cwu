global function string f_haksa_iljung_year ();string	ls_year

SELECT	HAKSA_ILJUNG.YEAR  
INTO		:ls_year  
FROM		HAKSA.HAKSA_ILJUNG  
WHERE		HAKSA_ILJUNG.SIJUM_FLAG = 'Y'
;

return ls_year
end function

