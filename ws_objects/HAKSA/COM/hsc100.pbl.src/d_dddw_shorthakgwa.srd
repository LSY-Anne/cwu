 retrieve="SELECT  		A.GWA ,
				A.SNAME,
				A.ORDER_SEQ
FROM			(	SELECT  	KCH003M.GWA ,
								KCH003M.SNAME,
								KCH003M.ORDER_SEQ     
					FROM 		CDDB.KCH003M 
					WHERE		KCH003M.GWA_GUBUN = '1'
					AND		LENGTH(TRIM(GWA)) = 4 
					AND		USE_GUBUN = 'Y'
					AND		SUBSTR(GWA,1,2) NOT IN ('AM','AH')
					
					UNION
					
					SELECT  	TRIM(KCH003M.GWA) ,
								KCH003M.FNAME,
								KCH003M.ORDER_SEQ    
					FROM 		CDDB.KCH003M 
					WHERE		KCH003M.GWA	=	'TAA'
					AND		USE_GUBUN = 'Y' 
				)	A
ORDER BY 	A.ORDER_SEQ


" )
