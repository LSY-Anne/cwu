 retrieve="SELECT	B.GWA_ID													AS	GWA_ID,
			SUM(DECODE(A.DUNG_YN, '1', 1, 0))				AS	DR_INWON,
			SUM(A.IPHAK_N)	+ NVL(SUM(I_JANGHAK), 0)		AS	IPHAK,
			NVL(SUM(A.DUNGROK_N), 0)	+ NVL(SUM(D_JANGHAK), 0)	AS	DUNGROK,
			SUM(I_JANGHAK)	AS	I_JANG,
			SUM(D_JANGHAK)	AS	D_JANG,
			NVL(SUM(A.IPHAK_N), 0) + NVL(SUM(A.DUNGROK_N), 0)	AS	TOT
FROM	HAKSA.D_DUNGROK	A,
		HAKSA.D_HAKJUK		B
WHERE	A.HAKBUN	=	B.HAKBUN
AND	A.YEAR	=	:as_year
AND	A.HAKGI	=	:as_hakgi
AND	A.NAPBU_DATE	BETWEEN	:as_start	AND	:as_end
GROUP BY	B.GWA_ID" arguments=(("as_year", string),("as_hakgi", string),("as_start", string),("as_end", string)) )
