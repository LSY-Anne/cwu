 retrieve="SELECT	B.GWA_ID			AS	GWA_ID,
			A.HAKBUN			AS	HAKBUN,
			B.D_HAKGICHA	AS	HAKGICHA,
			B.HNAME			AS	HNAME,
			A.IPHAK			AS	IPHAK,
			A.DUNGROK		AS	DUNGROK,
			A.DUNGROK - A.DUNGROK_N	AS	MINAP,
			DECODE(A.WAN_YN, '1', '완납', '')	AS	GUBUN
FROM	HAKSA.D_DUNGROK	A,
		HAKSA.D_HAKJUK		B
WHERE	A.HAKBUN	=	B.HAKBUN
AND	A.YEAR	=	:as_year
AND	A.HAKGI	=	:as_hakgi
AND	A.NAPBU_DATE	BETWEEN	:as_start	AND	:as_end
AND	A.WAN_YN	like	:as_wan
AND	A.BUN_YN	=	'1'
	" arguments=(("as_year", string),("as_hakgi", string),("as_start", string),("as_end", string),("as_wan", string)) )
