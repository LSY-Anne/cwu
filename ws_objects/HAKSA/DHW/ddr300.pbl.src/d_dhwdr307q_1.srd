 retrieve="SELECT	A.YEAR			AS	YEAR,
			A.HAKGI			AS	HAKGI,
             B.GYEYUL_ID     AS GYEYUL_ID,
			B.GWA_ID			AS	GWA_ID,
			B.JUNGONG_ID	AS	JUNGONG_ID,
			B.HAKBUN			AS	HAKBUN,
			B.D_HAKGICHA	AS	HAKGICHA,
			B.S_HAKGICHA	AS	S_HAKGICHA,
			DECODE(:as_gubun, 'Y', B.ENAME, B.HNAME)	AS	HNAME,
			D.GASANG_NO		AS	GASANG_NO,
			A.HAKJUM			AS	HAKJUM,
			A.IPHAK			AS	IPHAK,
			A.I_JANGHAK		AS	I_JANG,
			A.IPHAK - NVL(A.I_JANGHAK, 0)	AS	I_SIL,
			A.DUNGROK					AS	DUNGROK,
			A.D_JANGHAK					AS	D_JANG,
			A.DUNGROK - NVL(A.D_JANGHAK, 0)	AS	D_SIL,
			(NVL(A.IPHAK, 0) - NVL(A.I_JANGHAK, 0)) + (NVL(A.DUNGROK, 0) - NVL(A.D_JANGHAK, 0))	AS	SIL_NAP,
			A.WONWOO	AS	WONWOO,
			B.JUSO	AS	JUSO,
			B.ZIP_ID	AS	ZIPCODE,
			C.NAPBU_GIGAN	AS	N_GIGAN,
			C.NAPBUCHEO		AS	N_CHEO,
			C.GAE_ILSI		AS	GAE_ILSI,
			C.GITA1			AS	GITA1,
			C.GITA2			AS	GITA2,
			C.GITA3			AS	GITA3,
			C.GITA4			AS	GITA4,
			C.GITA5			AS	GITA5
FROM		HAKSA.D_DUNGROK		A,
			HAKSA.D_HAKJUK			B,
			HAKSA.D_GONGJI			C,
			(	SELECT	Z.YEAR,
							Z.HAKGI,
							Z.HAKBUN,
							Z.GASANG_NO
				FROM 		HAKSA.D_GASANG	Z) D
WHERE		A.HAKBUN		=	B.HAKBUN
AND		A.YEAR		=	C.YEAR(+)
AND		A.HAKGI		=	C.HAKGI(+)
AND		A.YEAR		=	D.YEAR(+)
AND		A.HAKGI		=	D.HAKGI(+)
AND		A.HAKBUN		=	D.HAKBUN(+)	
AND		A.YEAR		=	:as_year
AND		A.HAKGI		=	:as_hakgi
AND		A.HAKBUN	like	:as_hakbun
AND       B.HNAME     LIKE   '%' || :as_hname || '%'" arguments=(("as_year", string),("as_hakgi", string),("as_hakbun", string),("as_hname", string),("as_gubun", string))  sort="d_hakjuk_gwa_id A d_hakjuk_jungong_id A d_hakjuk_hakgicha A d_hakjuk_hakbun A " )
