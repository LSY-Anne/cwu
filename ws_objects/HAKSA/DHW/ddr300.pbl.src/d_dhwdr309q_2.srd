 retrieve="SELECT	B.GWA_ID		AS	GWA_ID,
			NVL(SUM(A.IPHAK_N), 0)		AS	IPHAK,
			NVL(SUM(A.DUNGROK_N), 0)	AS	DUNGROK,
			NVL(SUM(A.WONWOO_N), 0)		AS	WONWOO,
			SUM(NVL(A.IPHAK_N, 0) + NVL(A.DUNGROK_N, 0) + NVL(A.WONWOO_N, 0))	AS	TOT,
			COUNT(A.HAKBUN)	AS	INWON
FROM	HAKSA.D_DUNGROK	A,
		HAKSA.D_HAKJUK		B
WHERE	A.HAKBUN		=	B.HAKBUN
AND	A.YEAR		=	:as_year
AND	A.HAKGI		=	:as_hakgi
AND	A.HWAN_YN	=	'1'
AND	A.NAPBU_DATE	BETWEEN	:as_start	AND	:as_end
GROUP BY B.GWA_ID" arguments=(("as_year", string),("as_hakgi", string),("as_start", string),("as_end", string))  sort="d_hakjuk_gwa_id A " )
