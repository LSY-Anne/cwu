 retrieve="SELECT	X.YEAR,
			X.HAKGI,
			X.HAKBUN,
			Z.GWA_ID,
			Z.HNAME,
			Z.S_HAKGICHA,
			NVL(X.HJ, 0)	   AS HAKJUM,
			NVL(X.IP, 0)		AS	T_IP,
			NVL(X.DR, 0)	   AS	T_DR,
			NVL(X.I_JANG, 0)	AS	I_JANG,
			NVL(X.D_JANG, 0)	AS	D_JANG,
			NVL(X.IP_N, 0) 	AS	IPHAK,
			NVL(X.DR_N, 0)		AS	DUNGROK,
			X.DR - (	NVL(X.I_JANG, 0) + NVL(X.D_JANG, 0) + NVL(X.DR_N, 0) )	AS CHAYEAK
FROM		(	SELECT	YEAR,
							HAKGI,
							HAKBUN,
							SUM(HAKJUM) 			AS HJ,
							SUM(IPHAK)				AS IP,
							SUM(DUNGROK)			AS DR,
							SUM(I_JANGHAK) 		AS I_JANG,
							SUM(D_JANGHAK) 		AS D_JANG,
							SUM(IPHAK_N)			AS IP_N,
							NVL(SUM(DUNGROK_N), 0)	AS DR_N
				FROM		HAKSA.D_DUNGROK
				WHERE		YEAR		=	:as_year
				AND		HAKGI		=	:as_hakgi
				GROUP BY YEAR,
							HAKGI,
							HAKBUN
			) X,
			(
				SELECT	DISTINCT
							YEAR,
							HAKGI,
							HAKBUN
				FROM	HAKSA.D_DUNGROK
				WHERE	YEAR	=	:as_year
				AND	HAKGI	=	:as_hakgi
				AND	HWAN_YN	=	'1'
				AND	WAN_YN	=	'0'
			)	Y,
			HAKSA.D_HAKJUK Z
WHERE		X.YEAR 		= Y.YEAR
AND		X.HAKGI 		= Y.HAKGI
AND		X.HAKBUN 	= Y.HAKBUN
AND		X.HAKBUN		= Z.HAKBUN
AND		Z.GWA_ID		LIKE :as_gwa
" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string))  sort="jaehak_hakjuk_hakbun A " )
