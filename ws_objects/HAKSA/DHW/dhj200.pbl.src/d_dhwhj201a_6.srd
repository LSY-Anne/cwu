 retrieve="SELECT A.HAKBUN,   
         A.YEAR,   
         A.HAKGI,   
         A.CHASU,   
         A.HAKJUM,   
         A.IPHAK,   
         A.DUNGROK,   
         A.WONWOO,   
         A.D_JANGHAK,   
         A.IPHAK_N,   
         A.DUNGROK_N,   
         A.WONWOO_N,   
         A.WAN_YN,   
         A.DUNG_YN,   
         A.BUN_YN,   
         A.CHU_YN,   
         B.GWAJUNG_ID,  
         B.GYEYUL_ID,  
         B.GWA_ID,   
         B.HNAME,   
         B.JUNGONG_ID,   
         A.HWAN_YN,   
         A.BANK_ID,   
         A.NAPBU_DATE,   
         A.I_JANGHAK,
         D.JANGHAK_NAME
    FROM HAKSA.D_DUNGROK A,   
              HAKSA.D_HAKJUK    B,
              HAKSA.D_JANGHAK  C,
              HAKSA.D_JANGHAK_CODE D
   WHERE  A.HAKBUN  = B.HAKBUN
        AND A.HAKBUN  = C.HAKBUN(+)
        AND A.YEAR      = C.YEAR(+)
        AND A.HAKGI     = C.HAKGI(+)
        AND C.JANGHAK_ID = D.JANGHAK_ID(+)
        AND  A.HAKBUN = :as_hakbun    
" arguments=(("as_hakbun", string))  sort="d_dungrok_year A d_dungrok_hakgi A " )
