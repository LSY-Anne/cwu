 retrieve="  SELECT HAKSA.D_SANGBUL.SERIAL,   
         HAKSA.D_SANGBUL.HAKBUN,   
         HAKSA.D_SANGBUL.SANGBUL_ID,   
         HAKSA.D_SANGBUL.S_ILJA,   
         HAKSA.D_SANGBUL.E_ILJA,   
         HAKSA.D_SANGBUL.YEAR,   
         HAKSA.D_SANGBUL.HAKGI,   
         HAKSA.D_SANGBUL.HAKGICHA,   
         HAKSA.D_SANGBUL.SAYU,   
         HAKSA.D_HAKJUK.HNAME,   
         HAKSA.D_SANGBUL.WORKER,   
         HAKSA.D_SANGBUL.IPADDR,   
         HAKSA.D_SANGBUL.WORK_DATE,   
         HAKSA.D_SANGBUL.JOB_UID,   
         HAKSA.D_SANGBUL.JOB_ADD,   
         HAKSA.D_SANGBUL.JOB_DATE  
    FROM HAKSA.D_SANGBUL,   
         HAKSA.D_HAKJUK  
   WHERE ( HAKSA.D_SANGBUL.HAKBUN = HAKSA.D_HAKJUK.HAKBUN ) and  
         ( ( HAKSA.D_SANGBUL.HAKBUN like :as_hakbun ) AND  
         ( HAKSA.D_SANGBUL.SANGBUL_ID like :as_sangbul ) )    
