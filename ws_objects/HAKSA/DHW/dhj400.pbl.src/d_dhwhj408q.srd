 retrieve="SELECT 	HAKSA.D_NONMUN.TITLE_HNAME,   
			HAKSA.D_JOLUP_SAJUNG.JUNG_NO,   
			HAKSA.D_JOLUP_SAJUNG.HAKWI_NO,   
			HAKSA.D_JOLUP_SAJUNG.HAKWI_ID,   
			HAKSA.D_JOLUP_SAJUNG.HNAME  
FROM 	HAKSA.D_JOLUP_SAJUNG,   
			HAKSA.D_NONMUN
WHERE	HAKSA.D_JOLUP_SAJUNG.HAKBUN = HAKSA.D_NONMUN.HAKBUN 
and  		HAKSA.D_JOLUP_SAJUNG.PANJUNG = '1'
AND 		HAKSA.D_JOLUP_SAJUNG.YEAR = :as_year 
AND  		HAKSA.D_JOLUP_SAJUNG.HAKGI = :as_hakgi 
AND  		HAKSA.D_JOLUP_SAJUNG.HAKBUN like :as_hakbun 
AND       HAKSA.D_JOLUP_SAJUNG.HNAME LIKE '%' || :as_hname || '%'
  
" arguments=(("as_year", string),("as_hakgi", string),("as_hakbun", string),("as_hname", string)) )
