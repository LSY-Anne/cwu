 retrieve="SELECT	'      '||
			RPAD(A.GWA_ID, 6, ' ')||
			RPAD(A.SUHUM_NO, 12, ' ')||
			DECODE(A.JONGBYUL_ID, '0', '1', '2')||
			RPAD(A.HNAME, 16, ' ')||
			' 6'||
			LPAD(B.IPHAK, 7, ' ')||
			LPAD(B.DUNGROK, 7, ' ')||
			DECODE(B.D_JANGHAK, NULL, '0000000', LPAD(NVL(B.I_JANGHAK, 0) + NVL(B.D_JANGHAK, 0), 7, ' '))||
			LPAD(B.IPHAK + B.DUNGROK - (NVL(B.I_JANGHAK, 0) + NVL(B.D_JANGHAK, 0)), 7, ' ')||
			LPAD(B.WONWOO, 7, ' ')||
			LPAD(B.WONWOO, 7, ' ')||
			LPAD(B.IPHAK + B.DUNGROK - (NVL(B.I_JANGHAK, 0) + NVL(B.D_JANGHAK, 0)) + B.WONWOO, 7, ' ')
			||'        '	AS	KOOKMIN
FROM		DIPSI.DI_WONSEO	A,
			DIPSI.DI_DUNGROK	B
WHERE		A.YEAR		=	B.YEAR
AND		A.HAKGI		=	B.HAKGI
AND		A.SUHUM_NO	=	B.SUHUM_NO
AND		B.YEAR		=	:as_year
AND		B.HAKGI		=	:as_hakgi
ORDER BY	A.SUHUM_NO" arguments=(("as_year", string),("as_hakgi", string)) )
