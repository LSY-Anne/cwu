 retrieve="SELECT	B.GWA_ID													AS	GWA_ID,
			SUM(DECODE(A.DUNG_YN, '1', 1, 0))				AS	DR_INWON,
			NVL(SUM(A.IPHAK_N), 0)	+ NVL(SUM(I_JANGHAK), 0)		AS	IPHAK,
			NVL(SUM(A.DUNGROK_N), 0)	+ NVL(SUM(D_JANGHAK), 0)	AS	DUNGROK,
			NVL(SUM(I_JANGHAK), 0)	AS	I_JANG,
			NVL(SUM(D_JANGHAK), 0)	AS	D_JANG,
			NVL(SUM(A.IPHAK_N), 0)	+ NVL(SUM(I_JANGHAK), 0) + NVL(SUM(A.DUNGROK_N), 0)	+ NVL(SUM(D_JANGHAK), 0) AS	TOT_D,
			NVL(SUM(I_JANGHAK), 0) + NVL(SUM(D_JANGHAK), 0)		AS	TOT_J,
			NVL(SUM(A.IPHAK_N), 0) + NVL(SUM(A.DUNGROK_N), 0)	AS TOT_SIL
FROM	DIPSI.DI_DUNGROK	A,
		DIPSI.DI_WONSEO	B
WHERE	A.YEAR		=	B.YEAR
AND	A.HAKGI		=	B.HAKGI
AND	A.SUHUM_NO	=	B.SUHUM_NO
AND	A.YEAR		=	:as_year
AND	A.HAKGI		=	:as_hakgi
AND	( A.WAN_YN	=	'1' OR A.BUN_YN = '1' )
AND	A.NAPBU_DATE	BETWEEN	:as_start	AND	:as_end
GROUP BY	B.GWA_ID" arguments=(("as_year", string),("as_hakgi", string),("as_start", string),("as_end", string)) )
