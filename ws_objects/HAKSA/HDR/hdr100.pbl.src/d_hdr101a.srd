 retrieve="SELECT  	A.HAKBUN ,
			A.YEAR ,
			A.HAKGI ,
			A.CHASU ,
			A.HAKJUM ,
			A.IPHAK ,
			A.DUNGROK ,
			A.HAKSENGWHE,
			A.GYOJAE,
			A.ALBUM,
			A.HAKWHE,
			A.MEMORIAL,
			A.DONGCHANGWHE,
			A.I_JANGHAK,
			A.D_JANGHAK ,
			A.IPHAK_N ,
			A.DUNGROK_N ,
			A.HAKSENGWHE_N,
			A.GYOJAE_N,
			A.ALBUM_N,
			A.HAKWHE_N,
			A.MEMORIAL_N,
			A.DONGCHANGWHE_N,
			A.WAN_YN ,
			A.DUNG_YN ,
			A.BUN_YN ,
			A.CHU_YN ,
			A.WORKER ,
			A.IPADDR ,
			A.WORK_DATE ,
			A.JOB_UID ,
			A.JOB_ADD ,
			A.JOB_DATE ,
			B.GWA ,
			B.HNAME ,
			A.SU_HAKYUN,
			A.HWAN_YN ,
			A.BANK_ID,
			A.DUNGROK_GUBUN,
			A.JANGHAK_ID,
			C.ORDER_SEQ     
FROM		HAKSA.DUNGROK_GWANRI A ,
			HAKSA.JAEHAK_HAKJUK  B ,
			HAKSA.GWA_SYM 			C 
WHERE 	B.GWA			=	C.GWA
AND		A.HAKBUN 	= B.HAKBUN 
and      A.YEAR 		= :as_year  
And     	A.HAKGI		= :as_hakgi   
And     	B.GWA 	like :as_gwa 

   
" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string))  sort="gwa_sym_order_seq A jaehak_hakjuk_su_hakyun A " )
