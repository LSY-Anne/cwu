              and c.chasu   = d.chasu) c,
			HAKSA.GASANG			D
WHERE		A.HAKBUN		=	B.HAKBUN
AND		B.YEAR		=	D.YEAR(+)
AND		B.HAKGI		=	D.HAKGI(+)
AND		B.HAKBUN		=	D.HAKBUN(+)
AND		B.YEAR		=	:as_year
AND		B.HAKGI		=	:as_hakgi
AND		TO_CHAR(B.WORK_DATE, 'YYYYMMDD') LIKE :as_ilja
