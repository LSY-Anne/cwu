AND		B.HWAN_YN	= 	'N'
AND      B.BUN_YN    =  'Y'
AND		B.HAKJUM	IS NOT NULL
AND		B.HAKJUM	<> 0	
and      b.hakbun    = c.hakbun (+)
AND    	((:as_bunnap  = 'Y') OR (:as_bunnap = 'N' AND nvl(c.BUN_YN, 'N') = 'N'))" arguments=(("as_year", string),("as_hakgi", string),("as_ilja", string),("as_bunnap", string)) )
