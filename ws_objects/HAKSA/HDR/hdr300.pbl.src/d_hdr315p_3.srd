 retrieve="SELECT	A.GWA							AS	GWA,
			A.HAKBUN						AS	HAKBUN,
			A.HNAME						AS	HNAME,
			NVL(SUM(C.DUNGROK),0)	AS	DUNGROK,
			NVL(SUM(C.HAKJUM),0)		AS	HAKJUM,
			NVL(SUM(C.D_JANGHAK),0)	AS	JANGHAK,
			NVL(SUM(C.DUNGROK_N),0)	AS	DUNGROK_N,
			NVL(SUM(C.D_JANGHAK),0) + NVL(SUM(C.DUNGROK_N),0)	AS	TOT,
			MAX(NAPBU_DATE)	AS	DR_ILJA,
			B.ORDER_SEQ
FROM		HAKSA.JAEHAK_HAKJUK	A,			
			HAKSA.GWA_SYM B,
			HAKSA.DUNGROK_GWANRI	C,
			HAKSA.JANGHAK_GWANRI	D
WHERE		A.GWA		=	B.GWA
AND		A.HAKBUN	=	D.HAKBUN
AND		D.HAKBUN	=	C.HAKBUN(+)
AND		D.YEAR	=	C.YEAR(+)
AND		D.HAKGI	=	C.HAKGI(+)
AND		D.YEAR	=	:as_year
AND		D.HAKGI	=	:as_hakgi
AND		C.NAPBU_DATE	BETWEEN	:as_start	AND	:as_end
AND		C.WAN_YN	=	'Y'
AND		D.JANGHAK_ID	IN	('I01','I02','I03','I04','I50','I51','I52','I53','I54','I55','I56','I57')
GROUP BY	A.GWA,
			A.HAKBUN,
			A.HNAME,
			B.ORDER_SEQ
" arguments=(("as_year", string),("as_hakgi", string),("as_start", string),("as_end", string)) )
