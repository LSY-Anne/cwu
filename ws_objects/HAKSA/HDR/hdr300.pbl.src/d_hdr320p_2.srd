 retrieve="SELECT	B.GWA										AS	GWA_ID,
			B.HAKBUN									AS	HAKBUN,
			B.SU_HAKYUN								AS	HAKGICHA,
			B.HNAME									AS	HNAME,
			B.ADDR									AS	JUSO,
			B.ZIP_ID									AS	ZIPCODE,
			C.NAPBU_GIGAN							AS	N_GIGAN,
			C.NAPBUCHEO								AS	N_CHEO,
			C.GAE_ILSI								AS	GAE_ILSI,
			C.GITA									AS	GITA,
			C.GITA1									AS GITA1,
			C.GITA2									AS	GITA2,
			C.GITA3									AS	GITA3,
			C.GITA4									AS	GITA4,
			D.SINCHUNG_HAKJUM						AS	S_HAKJUM,
			D.CHIDK_HAKJUM							AS	C_HAKJUM,
			D.TOTAL_PYENGJUM						AS	P_TOT,
			D.AVG_PYENGJUM							AS	P_AVG,
			D.HAKYUN									AS DR_HAKYUN,
			D.YEAR									AS DR_YEAR,
			D.HAKGI									AS DR_HAKGI
FROM		HAKSA.JAEHAK_HAKJUK		B,
			HAKSA.GONGJI				C,
			(	SELECT	:as_year N_YEAR,
							:as_hakgi N_HAKGI,
							YEAR,
							HAKGI,
							HAKBUN,
							HAKYUN,
							SINCHUNG_HAKJUM,
							CHIDK_HAKJUM,
							TOTAL_PYENGJUM,
							AVG_PYENGJUM
				FROM		HAKSA.SUNGJUKGYE
				WHERE		YEAR  = :as_year
				AND		HAKGI	= :as_hakgi
			) D
WHERE		B.HAKBUN	=	D.HAKBUN
AND		D.YEAR	=	C.YEAR(+)
AND		D.HAKGI	=	C.HAKGI(+)
AND		D.YEAR	=	:as_year
AND		D.HAKGI	=	:as_hakgi
AND		B.GWA		like	:as_gwa
AND		D.HAKBUN	like	:as_hakbun





