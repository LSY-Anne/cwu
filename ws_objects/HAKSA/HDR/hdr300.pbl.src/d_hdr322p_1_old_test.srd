 retrieve="SELECT	A.GWA,
			A.HNAME,
			A.HAKBUN,
			D.SAYU_ID,
			D.HJMOD_SIJUM,
			A.HJMOD_DATE,
		  	DECODE(D.DUNGROK_INJUNG, 'Y', '학점추가분', '전액') NAPBU_GUBUN,
			DECODE(A.TH, 0, '미수강', '수강') SUGANG_GUBUN,
			A.TH,
			A.TD,
		  	D.HAKJUM  T_HAKJUM,
		  	D.DUNGROK T_DUNGROK
FROM    	(	SELECT	Y.HAKBUN,
							Y.GWA,
							Y.HNAME,
							Y.HUHAK_DATE,
							Y.HJMOD_DATE,
							NVL(SUM(Z.HAKJUM),0) TH,
							NVL(SUM(Z.DUNGROK),0) TD
				FROM		HAKSA.HAKJUKBYENDONG  X,
							HAKSA.JAEHAK_HAKJUK Y,
							HAKSA.DUNGROK_GWANRI Z
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HAKBUN = Z.HAKBUN(+)
				AND		X.YEAR 	= Z.YEAR(+)
				AND		X.HAKGI 	= Z.HAKGI(+)
				AND		X.YEAR 	= :as_year
				AND		X.HAKGI 	= :as_hakgi
				AND		X.HJMOD_ID = 'C'
				GROUP BY Y.HAKBUN,
							Y.GWA,
							Y.HNAME,
							Y.HUHAK_DATE,
							Y.HJMOD_DATE
			) A,
			(	SELECT 	X2.HAKBUN,
							Y2.HJMOD_ID,
							Y2.SAYU_ID,
							Y2.HJMOD_SIJUM,
							Y2.DUNGROK_INJUNG,
							MAX(Z2.WAN_YN) WAN,
							SUM(Z2.HAKJUM) HAKJUM,
							SUM(Z2.DUNGROK_N) DUNGROK
				FROM		(	SELECT	X1.HAKBUN,
											SUBSTR(MIN(X1.YEAR||X1.HAKGI),1,4) YEAR,
											SUBSTR(MIN(X1.YEAR||X1.HAKGI),5,2) HAKGI,
											MIN(X1.HJMOD_SIJUM) SIJUM,
											X1.HJMOD_ID
								FROM		HAKSA.HAKJUKBYENDONG X1,
											HAKSA.JAEHAK_HAKJUK Y1
								WHERE		X1.HAKBUN = Y1.HAKBUN
								AND		Y1.HJMOD_ID 	= 'C'
								AND		X1.HJMOD_ID 	= 'B'
								AND		X1.HJMOD_SIJUM	> NVL((	SELECT	MAX(A.HJMOD_SIJUM) HJMOD_SIJUM
																			FROM		HAKSA.HAKJUKBYENDONG A
																			WHERE		A.HJMOD_ID		= 'C'
																			AND		A.HAKBUN			= X1.HAKBUN
																			AND		A.HJMOD_SIJUM	< (	SELECT 	MAX(B.HJMOD_SIJUM) HJMOD_SIJUM
																													FROM		HAKSA.HAKJUKBYENDONG B
																													WHERE		B.HJMOD_ID = 'C'
																													AND		B.HAKBUN		= A.HAKBUN)), '00000000')
								GROUP BY X1.HAKBUN,
											X1.HJMOD_ID
											) X2,
											HAKSA.HAKJUKBYENDONG Y2,
											HAKSA.DUNGROK_GWANRI Z2
								WHERE		X2.HAKBUN 	= Y2.HAKBUN
								AND		X2.YEAR 	= Y2.YEAR
								AND		X2.HAKGI	= Y2.HAKGI
								AND		X2.SIJUM	= Y2.HJMOD_SIJUM
								AND 		X2.HJMOD_ID = Y2.HJMOD_ID
								AND		X2.HAKBUN	= Z2.HAKBUN
								AND		X2.YEAR		= Z2.YEAR
								AND		X2.HAKGI	= Z2.HAKGI
								GROUP BY X2.HAKBUN,
											Y2.HJMOD_ID,
											Y2.SAYU_ID,
											Y2.HJMOD_SIJUM,
											Y2.DUNGROK_INJUNG
		   	) D
WHERE		A.HAKBUN = D.HAKBUN" arguments=(("as_year", string),("as_hakgi", string)) )
