 retrieve="SELECT	A.GWA,
			A.HNAME,
			A.HAKBUN,
			A.HJMOD_DATE,
		  	DECODE(B.DUNGROK_INJUNG, 'Y', B.HAKJUM, NULL) T_HAKJUM,
		  	DECODE(B.DUNGROK_INJUNG, 'Y', B.DUNGROK, NULL) T_DUNGROK,
		  	DECODE(B.DUNGROK_INJUNG, 'Y', '학점추가분', '전액') NAPBU_GUBUN,
			NVL(A.TD, 0)	TD,
			NVL(A.TDN, 0)	TDN,
			A.NAPBU_DATE,
			A.BOHUN_GUBUN,
			D.ORDER_SEQ
FROM    	(	SELECT	Y.HAKBUN,
							Y.GWA,
							Y.HNAME,
							Y.HUHAK_DATE,
							Y.HJMOD_DATE,
							NVL(SUM(Z.DUNGROK),0)	AS TD,
							NVL(SUM(Z.DUNGROK_N),0)	AS TDN,
							MAX(DECODE(Z.DUNG_YN, 'Y', Z.NAPBU_DATE, ''))	AS	NAPBU_DATE,
							DECODE(NVL(Y.BOHUN_GUBUN, '0'), '1', '보훈', '2', '보훈', '0', '')  AS BOHUN_GUBUN
				FROM		HAKSA.HAKJUKBYENDONG  X,
							HAKSA.JAEHAK_HAKJUK Y,
							HAKSA.DUNGROK_GWANRI Z
				WHERE		X.HAKBUN	= Y.HAKBUN
				AND		X.HAKBUN	= Z.HAKBUN(+)
				AND		X.YEAR	= Z.YEAR(+)
				AND		X.HAKGI	= Z.HAKGI(+)
				AND		X.YEAR	= :as_year
				AND		X.HAKGI	= :as_hakgi
				AND		X.HJMOD_ID = 'C'
				AND		Z.NAPBU_DATE BETWEEN :as_start AND :as_end
				GROUP BY Y.HAKBUN,
							Y.GWA,
							Y.HNAME,
							Y.HUHAK_DATE,
							Y.HJMOD_DATE,
							Y.BOHUN_GUBUN
			) A,
			(	SELECT 	X2.HAKBUN,
							Y2.HJMOD_ID,
							Y2.SAYU_ID,
							Y2.HJMOD_SIJUM,
							Y2.DUNGROK_INJUNG,
							MAX(Z2.WAN_YN) WAN,
							NVL(SUM(Z2.HAKJUM),0) HAKJUM,
							NVL(SUM(Z2.DUNGROK_N),0) DUNGROK
				FROM		HAKSA.JAEHAK_HAKJUK	X2,
							HAKSA.HAKJUKBYENDONG Y2,
							HAKSA.DUNGROK_GWANRI Z2
				WHERE		X2.HAKBUN	 	=	Y2.HAKBUN
				AND		X2.HUHAK_DATE	=	Y2.HJMOD_SIJUM
				AND		X2.HAKBUN		=	Z2.HAKBUN
				AND		Y2.YEAR			=	Z2.YEAR
				AND		Y2.HAKGI			=	Z2.HAKGI
				AND		X2.HJMOD_ID		=	'C'
				AND		Y2.HJMOD_ID		=	'B'
				GROUP BY X2.HAKBUN,
							Y2.HJMOD_ID,
							Y2.SAYU_ID,
							Y2.HJMOD_SIJUM,
							Y2.DUNGROK_INJUNG
		   ) B,
			(	SELECT	DISTINCT	HAKBUN
				FROM		HAKSA.DUNGROK_GWANRI
				WHERE		YEAR		=	:as_year
				AND		HAKGI		=	:as_hakgi
				AND		DUNG_YN	=	'Y'
			)	C,			
			HAKSA.GWA_SYM D
WHERE		A.GWA		=	D.GWA
AND		A.HAKBUN =	B.HAKBUN(+)
AND		A.HAKBUN	=	C.HAKBUN







