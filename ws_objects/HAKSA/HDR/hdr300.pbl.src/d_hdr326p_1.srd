 retrieve="SELECT	JH.GWA			AS	HAKGWA,
			DG.SU_HAKYUN	AS HAKYUN,
			JH.HAKBUN		AS HAKBUN,
			JH.HNAME			AS HNAME,
			JH.SANGTAE		AS	SANGTAE,
			DG.ALBUM			AS	ALBUM,
			DG.ALBUM_N		AS	ALBUM_N,
			D.ORDER_SEQ
FROM		HAKSA.JAEHAK_HAKJUK	JH,
			HAKSA.DUNGROK_GWANRI	DG,			
			HAKSA.GWA_SYM D
WHERE		JH.GWA		=	D.GWA
AND		JH.HAKBUN	= DG.HAKBUN
AND		DG.YEAR		= :as_year
AND		DG.HAKGI		= :as_hakgi
AND		DG.ALBUM		= DG.ALBUM_N
AND		DG.ALBUM		> 0
ORDER BY D.ORDER_SEQ,
			DG.SU_HAKYUN,
			JH.HAKBUN

" arguments=(("as_year", string),("as_hakgi", string)) )
