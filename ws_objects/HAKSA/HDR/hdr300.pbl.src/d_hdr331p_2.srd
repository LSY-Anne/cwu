 retrieve="SELECT 	C.HAKBUN,   
			C.GWA,   
			D.SU_HAKYUN,   
			C.HNAME,   
			B.YEAR,   
			B.HAKGI,   
			B.CHASU,   
			F.GASANG_NO,
         nvl(B.IPHAK, 0) iphak_01,
			(	SELECT 	NVL(Z.DUNGROK, 0)
				FROM 		haksa.BUNNAP_GWANRI	Z
				WHERE 	Z.YEAR	=	B.YEAR
				AND		Z.HAKGI	=	B.HAKGI
				AND		Z.HAKBUN	=	B.HAKBUN
				AND		Z.CHASU	=	1
				AND		Z.hakbun like :as_hakbun 
				AND 		nvl(Z.dungrok, 0) <> 0)	DUNGROK_01, 
			(	SELECT 	NVL(Z.DUNGROK, 0)
				FROM 		haksa.BUNNAP_GWANRI	Z
				WHERE 	Z.YEAR	=	B.YEAR
				AND		Z.HAKGI	=	B.HAKGI
				AND		Z.HAKBUN	=	B.HAKBUN
				AND		Z.CHASU	=	2
				AND		Z.hakbun like :as_hakbun 
				AND 		nvl(Z.dungrok, 0) <> 0)	DUNGROK_02,     
			(	SELECT 	NVL(Z.DUNGROK, 0)
				FROM 		haksa.BUNNAP_GWANRI	Z
				WHERE 	Z.YEAR	=	B.YEAR
				AND		Z.HAKGI	=	B.HAKGI
				AND		Z.HAKBUN	=	B.HAKBUN
				AND		Z.CHASU	=	3
				AND		Z.hakbun like :as_hakbun 
				AND 		nvl(Z.dungrok, 0) <> 0)	DUNGROK_03,
			case when D.iphak_n   = 0 then nvl(B.IPHAK, 0)   else D.iphak   - nvl(B.iphak, 0)   end iphak,   
			case when D.dungrok_n = 0 then nvl(B.DUNGROK, 0) else D.dungrok - nvl(B.dungrok, 0) end dungrok,   
			D.HAKSENGWHE   - nvl(D.HAKSENGWHE_N, 0)   HAKSENGWHE,
			D.GYOJAE       - nvl(D.GYOJAE_N, 0)       GYOJAE,
			D.ALBUM        - nvl(D.ALBUM_N, 0)        ALBUM,   
			D.MEMORIAL     - nvl(D.MEMORIAL_N, 0)     MEMORIAL,   
			D.DONGCHANGWHE - nvl(D.DONGCHANGWHE_N, 0) DONGCHANGWHE,   
         D.IPHAK_N,
         D.DUNGROK_N,
         D.HAKSENGWHE_N,
         D.GYOJAE_N,      
         D.ALBUM_N,       
         D.MEMORIAL_N,    
         D.DONGCHANGWHE_N,
         D.i_janghak,
         D.d_janghak,
			B.BUNNAP_TOTAL,
			B.YAEJUNG_DATE,   
			D.IPHAK,   
			D.DUNGROK,   
			D.HAKSENGWHE,   
			D.GYOJAE,   
			D.ALBUM,   
			D.MEMORIAL,   
			D.DONGCHANGWHE,   
			D.HAKJUM,
			E.BUNNAP_GITA1, 
			E.BUNNAP_GITA2,
			E.BUNNAP_GITA3,
			E.BUNNAP_GITA4,
			E.BUNNAP_GITA5,
			E.NAPBU_GIGAN1 napbu_gigan,
			E.NAPBUCHEO,
			E.GAE_ILSI,
			E.BUNNAP_GITA1 || ' '  || E.BUNNAP_GITA2 || ' '  || E.BUNNAP_GITA3 || ' '  || E.BUNNAP_GITA4 || ' '  || E.BUNNAP_GITA5 gita,
         C.zip_id,
         C.addr
FROM 		HAKSA.BUNNAP_GWANRI		B,   
			HAKSA.JAEHAK_HAKJUK		C,   
			HAKSA.DUNGROK_GWANRI		D,
			HAKSA.GONGJI				E,
			(	SELECT	Z.YEAR,
							Z.HAKGI,
							Z.HAKBUN,
							Z.GASANG_NO
				FROM 		HAKSA.GASANG	Z) F
WHERE 	B.HAKBUN 	= C.HAKBUN 	
and		B.HAKBUN 	= D.HAKBUN	
and		B.YEAR 		= D.YEAR 	 
and  		B.HAKGI 		= D.HAKGI 
and 		B.YEAR		= E.YEAR				
and		B.HAKGI		= E.HAKGI				
and		B.YEAR 		= :as_year  
AND  		B.HAKGI 		= :as_hakgi 
AND 		C.GWA 	like :as_gwa 
AND 		B.HAKBUN like :as_hakbun 
AND		B.CHASU		= :as_chasu   
and   	((( D.iphak   - D.i_janghak - D.iphak_n )   > 0 )
or     	(( D.dungrok - D.d_janghak - D.dungrok_n ) > 0 )
or     	(( D.HAKSENGWHE   - D.HAKSENGWHE_N) > 0 )
or     	(( D.GYOJAE       - D.GYOJAE_N) > 0 )
or     	(( D.ALBUM        - D.ALBUM_N) > 0 )
or     	(( D.MEMORIAL     - D.MEMORIAL_N) > 0 )
or     	(( D.DONGCHANGWHE - D.DONGCHANGWHE_N) > 0 ))
AND		B.HAKBUN		=	F.HAKBUN(+)
AND		B.YEAR		=	F.YEAR(+)
AND		B.HAKGI		=	F.HAKGI(+)
	" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_hakbun", string),("as_chasu", number)) )
