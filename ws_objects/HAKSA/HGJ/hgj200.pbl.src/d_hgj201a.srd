			HAKSA.JAEHAK_HAKJUK.HNAME,   
			HAKSA.JAEHAK_HAKJUK.GWA  
FROM 		HAKSA.GJ_SINCHUNG,   
			HAKSA.JAEHAK_HAKJUK  
WHERE 	HAKSA.GJ_SINCHUNG.HAKBUN = HAKSA.JAEHAK_HAKJUK.HAKBUN 
