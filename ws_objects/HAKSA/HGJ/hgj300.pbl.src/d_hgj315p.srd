         ( ( HAKSA.GJ_SAJUNG.JAGUK_ID = :jaguk ) AND  
         ( HAKSA.GJ_SAJUNG.HAPGYUK_YN = 'Y' ) )    
" arguments=(("jaguk", string)) )
