			JANGHAK_MODEL.HAKGI,
			JANGHAK_MODEL.JANGHAK_ID ,
			JANGHAK_CODE.JANGHAK_NAME ,
			JANGHAK_MODEL.IPHAKGUM_RAT ,
			JANGHAK_MODEL.DUNGROK_RAT ,
