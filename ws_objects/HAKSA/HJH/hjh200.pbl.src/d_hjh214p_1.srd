 column=(type=decimal(0) updatewhereclause=yes name=gwa_sym_order_seq dbname="jaehak_hakjuk.order_seq" )
 retrieve="SELECT	D.GWA,
			D.SU_HAKYUN,
			D.JUYA_GUBUN,
			D.HAKBUN,
			D.HNAME,
			E.JANGHAK_ID,
			E.GUMYEAK,
			B.ORDER_SEQ	
FROM		HAKSA.JAEHAK_HAKJUK D,
			(	SELECT  	A.JANGHAK_ID,
							A.HAKBUN,
							SUM(B.D_JANGHAK)GUMYEAK
				FROM		HAKSA.JANGHAK_GWANRI  A,
							HAKSA.DUNGROK_GWANRI  B
				WHERE		A.YEAR	= 	B.YEAR
				AND		A.HAKGI 	= 	B.HAKGI
				AND		A.HAKBUN =	B.HAKBUN
				AND		A.JANGHAK_ID IN ( 'I01','I02','I03','I04')
				AND		A.YEAR = :as_year
				AND		A.HAKGI = :as_hakgi
				GROUP BY A.JANGHAK_ID,
							A.HAKBUN
			) E,
			HAKSA.HAKJUKBYENDONG F,
			HAKSA.GWA_SYM B
WHERE		d.GWA		=	B.GWA
AND		D.HAKBUN = E.HAKBUN
AND		D.HAKBUN = F.HAKBUN		
AND		F.HJMOD_ID	= 'C'
AND		F.YEAR		= :as_year
AND		F.HAKGI		= :as_hakgi
AND		D.GWA 		LIKE :as_gwa


" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string))  sort="gwa_sym_order_seq A juya_gubun A su_hakyun A hakbun A " )
