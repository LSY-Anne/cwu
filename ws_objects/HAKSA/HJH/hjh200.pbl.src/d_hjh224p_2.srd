 column=(type=decimal(0) updatewhereclause=yes name=gwa_sym_order_seq dbname="janghak_iphak.order_seq" )
 retrieve="SELECT  	A.JANGHAK_ID ,
			SUBSTR(B.GWA,1,3) SGWA,
			B.SU_HAKYUN,
			B.JUYA_GUBUN,
			A.HAKBUN,
			B.HNAME,
			D.AVG_PYENGJUM,
			0 GUMYEAK,
			E.ORDER_SEQ		
FROM 		HAKSA.JANGHAK_IPHAK  A,
			HAKSA.JAEHAK_HAKJUK  B,
			HAKSA.DUNGROK_GWANRI C,
			HAKSA.SUNGJUKGYE		D,
			HAKSA.GWA_SYM E
WHERE 	A.continue_yn = 'N'
AND      B.GWA		= E.GWA
AND		A.HAKBUN = B.HAKBUN(+)
AND		A.HAKBUN = C.HAKBUN(+)
AND		A.HAKBUN = D.HAKBUN(+)
AND		A.YEAR	= C.YEAR(+)
AND		A.HAKGI	= C.HAKGI(+)
AND		DECODE(:as_hakgi, '1', TO_CHAR(TO_NUMBER(:as_year) - 1), :as_year)	= D.YEAR
AND		DECODE(:as_hakgi, '1', '2', '1') 						= D.HAKGI
AND		A.JANGHAK_ID IN ('I50','I51','I54','I55')
AND		A.YEAR 	= :as_year
AND		A.HAKGI	= :as_hakgi
AND		B.GWA		like :as_gwa
GROUP BY A.HAKBUN,
			B.GWA,
			A.JANGHAK_ID ,
			B.SU_HAKYUN,
			B.JUYA_GUBUN,
			B.HNAME,
			D.AVG_PYENGJUM,
			E.ORDER_SEQ	

" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string))  sort="gwa_sym_order_seq A jaehak_hakjuk_su_hakyun A " )
