 retrieve="SELECT 	A.GWA,
			A.DR_HAKYUN,
			A.HAKBUN,
			A.HNAME,
			C.AVG_PYENGJUM			 
FROM 		HAKSA.JAEHAK_HAKJUK 	A,
			HAKSA.HAKJUKBYENDONG B,
			HAKSA.SUNGJUKGYE		C				
WHERE		A.HAKBUN			= B.HAKBUN
AND		A.HAKBUN			= C.HAKBUN
AND		A.HJMOD_ID		= B.HJMOD_ID
AND		C.YEAR||C.HAKGI	=	(	SELECT 	MAX(D.YEAR||D.HAKGI)
											FROM 		HAKSA.SUNGJUKGYE	D
											WHERE		D.HAKBUN		=	C.HAKBUN
											AND		D.INJUNG_YN	=	'Y'
											AND		D.HAKGI IN ('1', '2'))
AND		A.HJMOD_ID		=	'C'
AND		C.AVG_PYENGJUM >= 4.0
AND		B.YEAR 			= :as_year
AND		B.HAKGI			= :as_hakgi

" arguments=(("as_year", string),("as_hakgi", string))  sort="jaehak_hakjuk_gwa A jaehak_hakjuk_dr_hakyun A jaehak_hakjuk_hakbun A " )
