 retrieve="select 	distinct a.hname,
		 	a.jumin_no,
		 	a.gwa,
		 	b.fname,
		 	a.dr_hakyun,
		 	a.addr,
		 	a.bo_name,
         substr(a.zip_id,1,3),
	      (select  c.zip_name1||'(시.도) '||c.zip_name2||'(시.군.구)' 
              from haksa.zipcodes c
              where a.zip_id =  c.zip_id 
                          and rownum = 1) as zip_addr,
			a.zip_id
  from 	haksa.jolup_hakjuk a, 
			cddb.kch003m b,
			haksa.zipcodes c
  where 	a.gwa								=	b.gwa
	 and	a.zip_id							=	c.zip_id (+)
	 and	substr(a.jolup_date,1,4) 	=	:as_year
	 and 	substr(a.jumin_no,7,1) 		=	'1'
    and  a.sangtae = '04'
	 AND	b.USE_GUBUN = 'Y'
	 and	nvl(c.zip_name1,' ')			like	:as_addr||'%'
	 and	nvl(c.zip_name2,' ')			like	:as_zip_id13||'%'
" arguments=(("as_year", string),("as_addr", string),("as_zip_id13", string))  sort="jolup_hakjuk_zip_id A jolup_hakjuk_dr_hakyun A " )
