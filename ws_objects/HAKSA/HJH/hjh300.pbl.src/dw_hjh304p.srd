			B.JUMIN_NO,
			B.ADDR,
			B.GWA,
			C.FNAME,
			A.HJMOD_ID ,
			A.HJMOD_SIJUM ,
			A.SAYU_ID ,
			SUBSTR(B.ADDR, 1, 2) JusoJI
FROM 		HAKSA.HAKJUKBYENDONG A,
			HAKSA.JAEHAK_HAKJUK	B,
			CDDB.KCH003M 			C    
WHERE 	C.GWA				=	B.GWA
