			JOLUP_SIHUM.JAKPUM_JUMSU ,
			JOLUP_SIHUM.HAPGYUK_YN,
			JOLUP_SIHUM.HAKBUN     
FROM 		HAKSA.JAEHAK_HAKJUK ,
			HAKSA.JOLUP_SIHUM,
			HAKSA.GWA_SYM    
WHERE 	JAEHAK_HAKJUK.GWA		=	GWA_SYM.GWA
