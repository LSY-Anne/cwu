 retrieve="SELECT  	A.HAKBUN ,
			A.HNAME ,
			A.JUMIN_NO ,
			C.GROUP7_CODE,
			A.IPHAK_DATE ,
			A.JAEIPHAK_DATE,
			A.GWA,
			C.FNAME AS GWA_NAME,
			A.JUNGONG_GUBUN,
			A.BUJUNGONG_ID,
			A.JOLUP_DATE,
			A.HAKWI_NAME,
			A.HAKWI_NO,
			D.GYO_PIL,
			D.GYO_SUN,
			(D.GYO_PIL + D.GYO_SUN) GYO_TOT,
			D.GONG_PIL,
			D.GONG_SUN,			
			D.JUN_PIL,
			D.JUN_SUN,
			(D.JUN_PIL + D.JUN_SUN + D.GONG_PIL + D.GONG_SUN ) JUN_TOT,
			D.SUN_ISU,
			D.BU_JUN,
			D.BOKSU_JUN,
			D.ILBAN,
			D.GYOJIK,
			NVL(A.INJUNG_HAKJUM, 0) INJUNG_HAKJUM,
			( D.GYO_PIL + D.GYO_SUN + D.JUN_PIL + D.JUN_SUN + D.GONG_PIL + D.GONG_SUN + D.SUN_ISU + D.BU_JUN + D.BOKSU_JUN + D.ILBAN + D.GYOJIK + nvl(A.INJUNG_HAKJUM, 0)) TOTAL,
			DECODE( A.HAKGI_CHK, 'Y', '학기제', '학점제') AS HAKGI_CHK
FROM 	HAKSA.JAEHAK_HAKJUK A,
			CDDB.KCH003M C,
			(	SELECT	SUGANG.HAKBUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '11', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) GYO_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '12', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) GYO_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '21', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) JUN_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '22', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) JUN_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '25', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) GONG_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '26', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) GONG_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '30', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) SUN_ISU,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '40', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) GYOJIK,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '50', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) BU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '60', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) BOKSU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '80', DECODE(HWANSAN_JUMSU, 'F', 0,
																										 'W', 0, SUGANG.HAKJUM))),0) ILBAN,	
							NVL(SUM(DECODE(HWANSAN_JUMSU, 'W', 0, SUGANG.HAKJUM)),0)	SINCHUNG ,
							NVL(SUM(DECODE(SUGANG.HWANSAN_JUMSU, 'F', 0,
																			 'W',	0, SUGANG.HAKJUM)),0)	CHIDK	,
							SUM(DECODE(SUGANG.PYENGJUM, NULL, 0, SUGANG.PYENGJUM) * SUGANG.HAKJUM)	PYENGJUM_TOT,
							TRUNC(SUM(DECODE(SUGANG.HWANSAN_JUMSU, 'F',0,
																				'W',0, SUGANG.PYENGJUM) * SUGANG.HAKJUM) / SUM(DECODE(HWANSAN_JUMSU, 'W', 0, SUGANG.HAKJUM)), 2)	PYENGJUM_AVG,
							SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU))	SILJUM_TOT,
							ROUND(SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU)) / COUNT(SUGANG.GWAMOK_ID), 1)	SILJUM_AVG
				FROM		HAKSA.SUGANG
				GROUP BY	SUGANG.HAKBUN	
			) D		
WHERE 	A.GWA			= C.GWA
AND		A.HAKBUN 	= D.HAKBUN
AND 		A.GWA			like	:as_gwa
AND		A.HAKBUN		like	:as_hakbun	
AND		D.CHIDK + NVL(TO_NUMBER(A.INJUNG_HAKJUM), 0)	>= :ai_hakjum
AND		A.SANGTAE	=	'01'
ORDER BY	DECODE( A.HAKGI_CHK, 'Y', '학기제', '학점제'),
                 A.GWA ASC				,
			  A.HAKBUN ASC" arguments=(("as_hakbun", string),("as_gwa", string),("ai_hakjum", number)) )
