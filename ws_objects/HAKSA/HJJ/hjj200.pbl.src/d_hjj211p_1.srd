 retrieve="SELECT 	SUBSTR(A.IPHAK_DATE, 1 ,4) IPHAK_DATE,
			A.HAKBUN,    
			DECODE(LENGTH(TRIM(:as_gwa)), 3, SUBSTR(A.GWA,1 ,3), SUBSTR(A.GWA,1 ,4)) gwa,   
			A.HNAME,  
			REPLACE(A.TEL, ' ', '')	TEL,
