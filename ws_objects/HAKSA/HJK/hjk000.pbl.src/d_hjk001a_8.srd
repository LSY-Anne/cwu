			ZIPCODES.ZIP_ID ,
			ZIPCODES.ZIP_NAME1 ,
			ZIPCODES.ZIP_NAME2 ,
			ZIPCODES.ZIP_NAME3 ,
			ZIPCODES.ZIP_NAME4,
             WORKER,
			IPADDR,
			WORK_DATE,
			JOB_UID,
			JOB_ADD,
			JOB_DATE
FROM 		HAKSA.ZIPCODES      
WHERE 	( ZIPCODES.ZIP_ID like :as_id1 || '%' ) 
and   	( ZIPCODES.ZIP_NAME1 like :as_name1 || '%' ) 
and   	( ZIPCODES.ZIP_NAME2 like :as_name2 || '%' ) 
and   	( ZIPCODES.ZIP_NAME3 like '%'||:as_name3||'%')   
ORDER BY ZIPCODES.SERIAL    ASC  


