			HAKSA.HAKJUKBYENDONG B, 
			CDDB.KCH003M C 
WHERE   	A.GWA					= C.GWA 
AND   	( A.HAKBUN = B.HAKBUN				)
and		( A.HJMOD_SAYU_ID = B.SAYU_ID		)
