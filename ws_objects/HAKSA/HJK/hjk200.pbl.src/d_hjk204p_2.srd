and		( B.HAKGI		= :as_hakgi		)
and		( A.HAKBUN 		like :as_hakbun)
and      ( A.HNAME 		like :as_hname )  
and      ( B.SAYU_ID	= 'D12')
			)  
ORDER BY C.ORDER_SEQ,
			A.SU_HAKYUN ,
			A.HAKBUN 		


