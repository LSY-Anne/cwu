 retrieve="SELECT A.APPLY_DT             AS APPLY_DT
            , A.HJMOD_GB            AS HJMOD_GB
            , A.APPLY_SEQ           AS APPLY_SEQ
            , A.SAYU_ID               AS SAYU_ID
            , A.SAYU_REMARK      AS SAYU_REMARK
            , A.HAKBUN                AS HAKBUN
            , A.HJMOD_FDT          AS HJMOD_FDT
            , A.HJMOD_TDT          AS HJMOD_TDT
            , A.BOKHAK_YEAR       AS  BOKHAK_YEAR
            , A.BOKHAK_HAKGI      AS  BOKHAK_HAKGI
            , A.ARMY_DT              AS ARMY_DT
            , A.DISCHARGE_DT      AS DISCHARGE_DT
           ,  A.EVID_YN                AS EVID_YN
            , A.SUNGJUK_INJUNG   AS SUNGJUK_INJUNG
            , A.DUNGROK_INJUNG  AS DUNGROK_INJUNG
            , A.ZIP_ID                  AS ZIP_ID
            , A.ADDR                   AS  ADDR
           ,  A.TEL                      AS TEL
           ,  A.HP                       AS HP
           ,  A.EMAIL                  AS  EMAIL
            , A.SIGN_STATUS      AS SIGN_STATUS
            , A.MEMBER_NO        AS  MEMBER_NO
            , A.SIGN_DATE          AS SIGN_DATE
            , A.RETURN_REMARK  AS RETURN_REMARK
            , A.WORKER              AS WORKER
            , A.IPADDR                AS IPADDR
            , A.WORK_DATE         AS  WORK_DATE
            , A.JOB_UID               AS JOB_UID
           ,  A.JOB_ADD             AS JOB_ADD
           ,  A.JOB_DATE           AS JOB_DATE
           , B.HNAME                 AS HNAME
           , B.GWA                     AS GWA
           , A.HAKYUN                AS HAKYUN
           , A.HAKGI                   AS HAKGI
           , A.BOKHAK_HAKYUN   AS BOKHAK_HAKYUN
           , 'N'                           AS CHK
    FROM HAKSA.HJMOD_SINCHUNG  A
           , HAKSA.JAEHAK_HAKJUK      B
  WHERE A.HAKBUN = B.HAKBUN
      AND A.APPLY_DT BETWEEN :AS_FDT AND :AS_TDT
      AND A.HJMOD_GB = :AS_HJMOD_GB
      AND A.HAKBUN     IN (SELECT A.HAKBUN 
                                       FROM HAKSA.SUM210TL C
										 , HAKSA.HAKSA_ILJUNG  D
							     WHERE C.YEAR   = D.YEAR
								     AND  C.HAKGI = D.HAKGI
								     AND  C.MEMBER_NO = :AS_MEMBER_NO
								     AND  D.SIJUM_FLAG = 'Y'
                                       UNION ALL
                                      SELECT HAKBUN  FROM HAKSA.HJMOD_SINCHUNG
                                       WHERE :AS_SIGN_NM = '2'
                                            AND APPLY_DT BETWEEN :AS_FDT AND :AS_TDT
                                            AND HJMOD_GB = :AS_HJMOD_GB 
                                     )
     AND SUBSTR(B.GWA,1,3) IN ( SELECT TRIM(GWA) FROM INDB.HIN001M
											WHERE :AS_SIGN_NM = '2'
												  AND MEMBER_NO = :AS_MEMBER_NO
											 UNION ALL
										  SELECT TRIM(GWA) FROM CDDB.KCH003M
											 WHERE :AS_SIGN_NM = '1'
										)
     AND A.SIGN_STATUS IN ( :AS_SIGN_STATUS )" update="HAKSA.HJMOD_SINCHUNG" updatewhere=1 updatekeyinplace=no arguments=(("AS_FDT", string),("AS_TDT", string),("AS_HJMOD_GB", string),("AS_MEMBER_NO", string),("AS_SIGN_NM", string),("AS_SIGN_STATUS", stringlist))  sort="apply_dt D hjmod_gb D apply_seq D " )
