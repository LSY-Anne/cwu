 retrieve="SELECT  	A.YEAR ,
			A.HAKGI ,
			B.GWAMOK_HNAME,
			A.ISU_ID ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			(A.JUMSU * A.HAKJUM) JUMSU,
			NVL(C.INJUNG_HAKJUM,0) INJUNG_HAKJUM,
			C.CHIDK
FROM 		HAKSA.SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM))CHIDK
				FROM		HAKSA.JAEHAK_HAKJUK X,
							HAKSA.SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HAKBUN LIKE :hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C
WHERE 	( A.HAKBUN = C.HAKBUN )
AND		( A.GWAMOK_ID = B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ = B.GWAMOK_SEQ)
and      (	( A.HAKBUN like :hakbun)
	and      ( A.SUNGJUK_INJUNG = 'Y' )
	AND		( C.CHIDK < 140)
			)

UNION

SELECT  	A.YEAR ,
			A.HAKGI ,
			B.GWAMOK_HNAME,
			A.ISU_ID ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			(A.JUMSU * A.HAKJUM) JUMSU,
			NVL(C.INJUNG_HAKJUM,0) INJUNG_HAKJUM,
			C.CHIDK
FROM 		HAKSA.SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM))CHIDK
				FROM		HAKSA.JAEHAK_HAKJUK X,
							HAKSA.SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HAKBUN LIKE :hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C
WHERE 	( A.HAKBUN = C.HAKBUN )
AND		( A.GWAMOK_ID = B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ = B.GWAMOK_SEQ)
and      (	( A.HAKBUN like :hakbun)
	and      ( A.SUNGJUK_INJUNG = 'Y' )
	AND		( C.CHIDK >= 140)
	AND		( A.HWANSAN_JUMSU NOT IN 'F')
			)

UNION 

SELECT  	A.YEAR ,
			A.HAKGI ,
			B.GWAMOK_HNAME ,
			A.ISU_ID ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			(A.JUMSU * A.HAKJUM) JUMSU,
			NVL(C.INJUNG_HAKJUM,0) INJUNG_HAKJUM,
			C.CHIDK      
FROM 		HAKSA.JOLUP_SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM))CHIDK
				FROM		HAKSA.JOLUP_HAKJUK X,
							HAKSA.JOLUP_SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HAKBUN LIKE :hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C      
WHERE 	( A.HAKBUN = C.HAKBUN ) 
AND		( A.GWAMOK_ID = B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ = B.GWAMOK_SEQ) 
and      (	( A.HAKBUN like :hakbun ) 
	and      ( A.SUNGJUK_INJUNG = 'Y' )
	AND		( C.CHIDK < 140) 
			) 

UNION

SELECT  	A.YEAR ,
			A.HAKGI ,
			B.GWAMOK_HNAME,
			A.ISU_ID ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			(A.JUMSU * A.HAKJUM) JUMSU,
			NVL(C.INJUNG_HAKJUM,0) INJUNG_HAKJUM,
			C.CHIDK
FROM 		HAKSA.JOLUP_SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM))CHIDK
				FROM		HAKSA.JOLUP_HAKJUK X,
							HAKSA.JOLUP_SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HAKBUN LIKE :hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C
WHERE 	( A.HAKBUN = C.HAKBUN )
AND		( A.GWAMOK_ID = B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ = B.GWAMOK_SEQ)
and      (	( A.HAKBUN like :hakbun)
	and      ( A.SUNGJUK_INJUNG = 'Y' )
	AND		( C.CHIDK >= 140)
	AND		( A.HWANSAN_JUMSU NOT IN 'F')
			)
