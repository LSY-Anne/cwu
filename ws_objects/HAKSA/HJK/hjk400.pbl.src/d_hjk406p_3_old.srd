 retrieve="SELECT  	A.HAKBUN ,
			A.HNAME ,
			A.JUMIN_NO ,
			C.GROUP7_CODE,
			A.IPHAK_DATE ,
			A.JAEIPHAK_DATE,
			C.FNAME AS GWA_NAME,
			A.JUNGONG_GUBUN,
			A.BUJUNGONG_ID,
			A.JOLUP_DATE,
			A.HAKWI_NAME,
			A.HAKWI_NO,
			B.GYO_PIL,
			B.GYO_SUN,
			(B.GYO_PIL + B.GYO_SUN) GYO_TOT,
			B.JUN_PIL,
			B.JUN_SUN,
			(B.JUN_PIL + B.JUN_SUN) JUN_TOT,
			B.SUN_ISU,
			B.BU_JUN,
			B.BOKSU_JUN,
			B.ILBAN,
			B.GYOJIK,
			nvl(A.INJUNG_HAKJUM, 0) INJUNG_HAKJUM,
			( B.GYO_PIL + B.GYO_SUN + B.JUN_PIL + B.JUN_SUN + B.SUN_ISU + B.BU_JUN + B.BOKSU_JUN + B.ILBAN + B.GYOJIK + nvl(A.INJUNG_HAKJUM, 0)) TOTAL    
FROM 		HAKSA.JAEHAK_HAKJUK A,
			( SELECT		SUGANG.HAKBUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '11', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYO_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '12', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYO_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '21', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) JUN_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '22', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) JUN_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '30', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) SUN_ISU,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '40', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYOJIK,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '50', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) BU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '60', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) BOKSU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '80', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) ILBAN,	
							NVL(SUM(SUGANG.HAKJUM),0)	SINCHUNG ,
							NVL(SUM(DECODE(SUGANG.HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM)),0)	CHIDK	,
							SUM(DECODE(SUGANG.PYENGJUM, NULL, 0, SUGANG.PYENGJUM) * SUGANG.HAKJUM)	PYENGJUM_TOT,
							TRUNC(SUM(DECODE(SUGANG.HWANSAN_JUMSU, 'F',0, SUGANG.PYENGJUM) * SUGANG.HAKJUM) / SUM(SUGANG.HAKJUM), 2)	PYENGJUM_AVG,
							SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU))	SILJUM_TOT,
							ROUND(SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU)) / COUNT(SUGANG.GWAMOK_ID), 1)	SILJUM_AVG
				FROM		HAKSA.SUGANG
				WHERE		SUNGJUK_INJUNG = 'Y'
				AND		BAL_YN			= '1'
				AND		SUGANG.HAKBUN like :as_hakbun
				GROUP BY	SUGANG.HAKBUN
			) B,
			CDDB.KCH003M C      
WHERE 	( A.HAKBUN = B.HAKBUN)
AND		( A.GWA		= C.GWA		)
AND		( A.HAKBUN 		like :as_hakbun 	) 
AND		( A.HNAME		like :as_name		)
AND		( A.JUMIN_NO 	like :as_jumin		) 

union
  
SELECT  	A.HAKBUN ,
			A.HNAME ,
			A.JUMIN_NO ,
			A.GROUP7_CODE,
			A.IPHAK_DATE ,
			A.JAEIPHAK_DATE,
			A.GWA_NAME AS GWA_NAME,
			A.JUNGONG_GUBUN,
			A.BUJUNGONG_ID,
			A.JOLUP_DATE,
			A.HAKWI_NAME,
			A.HAKWI_NO,
			B.GYO_PIL,
			B.GYO_SUN,
			(B.GYO_PIL + B.GYO_SUN) GYO_TOT,
			B.JUN_PIL,
			B.JUN_SUN,
			(B.JUN_PIL + B.JUN_SUN) JUN_TOT,
			B.SUN_ISU,
			B.BU_JUN,
			B.BOKSU_JUN,
			B.ILBAN,
			B.GYOJIK,
			nvl(A.INJUNG_HAKJUM, 0) INJUNG_HAKJUM,
			( B.GYO_PIL + B.GYO_SUN + B.JUN_PIL + B.JUN_SUN + B.SUN_ISU + B.BU_JUN + B.BOKSU_JUN + B.ILBAN + B.GYOJIK + nvl(A.INJUNG_HAKJUM, 0)) TOTAL     
FROM 		HAKSA.JOLUP_HAKJUK A,
			( SELECT		SUGANG.HAKBUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '11', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYO_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '12', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYO_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '21', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) JUN_PIL,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '22', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) JUN_SUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '30', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) SUN_ISU,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '40', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) GYOJIK,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '50', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) BU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '60', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) BOKSU_JUN,
							NVL(SUM(DECODE(SUGANG.ISU_ID, '80', DECODE(HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM))),0) ILBAN,	
							NVL(SUM(SUGANG.HAKJUM),0)	SINCHUNG ,
							NVL(SUM(DECODE(SUGANG.HWANSAN_JUMSU, 'F', 0, SUGANG.HAKJUM)),0)	CHIDK	,
							SUM(DECODE(SUGANG.PYENGJUM, NULL, 0, SUGANG.PYENGJUM) * SUGANG.HAKJUM)	PYENGJUM_TOT,
							TRUNC(SUM(DECODE(SUGANG.PYENGJUM, NULL, 0, SUGANG.PYENGJUM) * SUGANG.HAKJUM) / SUM(SUGANG.HAKJUM), 2)	PYENGJUM_AVG,
							SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU))	SILJUM_TOT,
							ROUND(SUM(DECODE(SUGANG.JUMSU, NULL, 0, SUGANG.JUMSU)) / COUNT(SUGANG.GWAMOK_ID), 1)	SILJUM_AVG
				FROM		HAKSA.JOLUP_SUGANG SUGANG
				WHERE		SUNGJUK_INJUNG = 'Y'
				AND		SUGANG.HAKBUN like :as_hakbun
				GROUP BY	SUGANG.HAKBUN
			) B
WHERE 	( A.HAKBUN 	= B.HAKBUN	)
AND		( A.HAKBUN 		like :as_hakbun 	) 
AND		( A.HNAME		like :as_name		)
AND		( A.JUMIN_NO 	like :as_jumin		) 
" arguments=(("as_hakbun", string),("as_name", string),("as_jumin", string)) )
