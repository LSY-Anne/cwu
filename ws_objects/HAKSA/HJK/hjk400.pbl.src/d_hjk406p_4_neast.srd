 column=(type=char(3) updatewhereclause=yes name=jaehak_hakjuk_injung_hakjum dbname="sugang.injung_hakjum" )
 retrieve="SELECT  	A.YEAR ,
			A.HAKGI ,
			A.GWAMOK_ID,
			B.GWAMOK_ENAME ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			B.PASS_GUBUN,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			A.JUMSU ,
			C.INJUNG_HAKJUM     
FROM 		HAKSA.SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM)) + NVL(TO_NUMBER(X.INJUNG_HAKJUM), 0) CHIDK
				FROM		HAKSA.JAEHAK_HAKJUK X,
							HAKSA.SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		Y.BAL_YN	= '1'
				AND		Y.SUNGJUK_INJUNG	= 'Y'
				AND		X.HAKBUN LIKE :as_hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C     
WHERE 	( A.HAKBUN		=	C.HAKBUN ) 
AND		( A.GWAMOK_ID	=	B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ	=	B.GWAMOK_SEQ)  
AND      ( A.HAKBUN	like	:as_hakbun ) 
AND      ( A.SUNGJUK_INJUNG = 'Y' )
AND		( A.BAL_YN	= '1' )
AND		( C.CHIDK < 114 ) 

UNION 

SELECT  	A.YEAR ,
			A.HAKGI ,
			A.GWAMOK_ID,
			B.GWAMOK_ENAME ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			B.PASS_GUBUN,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			A.JUMSU ,
			C.INJUNG_HAKJUM     
FROM 		HAKSA.SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM)) + NVL(TO_NUMBER(X.INJUNG_HAKJUM), 0)	CHIDK
				FROM		HAKSA.JAEHAK_HAKJUK X,
							HAKSA.SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		Y.BAL_YN	= '1'
				AND		Y.SUNGJUK_INJUNG	= 'Y'
				AND		X.HAKBUN LIKE :as_hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C     
WHERE 	( A.HAKBUN		=	C.HAKBUN ) 
AND		( A.GWAMOK_ID	=	B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ	=	B.GWAMOK_SEQ)  
AND      ( A.HAKBUN	like	:as_hakbun ) 
AND      ( A.SUNGJUK_INJUNG = 'Y' )
AND      ( A.BAL_YN = '1' )
AND		( C.CHIDK >= 114 ) 
AND		( A.HWANSAN_JUMSU <> 'F')


UNION

SELECT  	A.YEAR ,
			A.HAKGI ,
			A.GWAMOK_ID,
			B.GWAMOK_ENAME ,
			A.HAKJUM ,
			A.HWANSAN_JUMSU ,
			B.PASS_GUBUN,
			(A.PYENGJUM * A.HAKJUM) HWAN_JUMSU,
			A.JUMSU ,
			C.INJUNG_HAKJUM      
FROM 		HAKSA.JOLUP_SUGANG A,
			HAKSA.GWAMOK_CODE B,
			( 	SELECT 	X.HAKBUN,
							X.INJUNG_HAKJUM,
							SUM(DECODE(Y.HWANSAN_JUMSU, 'F',0,Y.HAKJUM)) + NVL(TO_NUMBER(X.INJUNG_HAKJUM), 0)	CHIDK
				FROM		HAKSA.JOLUP_HAKJUK X,
							HAKSA.JOLUP_SUGANG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		Y.SUNGJUK_INJUNG	= 'Y'
				AND		X.HAKBUN LIKE :as_hakbun
				GROUP BY X.HAKBUN,
							X.INJUNG_HAKJUM
			) C      
WHERE 	( A.HAKBUN		=	C.HAKBUN ) 
AND		( A.GWAMOK_ID	=	B.GWAMOK_ID)
AND		( A.GWAMOK_SEQ	=	B.GWAMOK_SEQ) 
AND      ( A.HAKBUN	like	:as_hakbun ) 
AND      ( A.SUNGJUK_INJUNG = 'Y' ) 
AND		( A.HWANSAN_JUMSU <> 'F')

