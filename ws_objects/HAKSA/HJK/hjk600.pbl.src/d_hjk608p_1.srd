 retrieve="SELECT 	A.HAKBUN,
			A.GWA,
			A.DR_HAKYUN,
			A.HNAME,
			B.SAYU_ID,
			B.HJMOD_SIJUM,
			A.HJMOD_SAYU_ID,
			A.HJMOD_DATE  
FROM 		HAKSA.JAEHAK_HAKJUK A,
			(	SELECT	X.HAKBUN,
							Y.HJMOD_SIJUM,
							Y.HJMOD_ID,
							Y.SAYU_ID
				FROM		HAKSA.JAEHAK_HAKJUK X,
							HAKSA.HAKJUKBYENDONG Y
				WHERE		X.HAKBUN = Y.HAKBUN
				AND		X.HUHAK_DATE = Y.HJMOD_SIJUM  
				AND		Y.SAYU_ID <> 'B13'  
			) B,
			CDDB.KCH003M C
WHERE		A.GWA		=	C.GWA
AND		A.HAKBUN = B.HAKBUN
AND		A.HJMOD_SAYU_ID = 'B13'
AND		A.HJMOD_DATE BETWEEN :as_from and :as_to
ORDER BY C.ORDER_SEQ,
			A.HAKBUN ,
			A.HJMOD_DATE 	
 
" arguments=(("as_from", string),("as_to", string)) )
