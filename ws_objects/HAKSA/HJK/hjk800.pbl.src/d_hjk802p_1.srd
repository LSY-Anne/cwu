 retrieve="SELECT 	B.JUMIN_NO,   
			B.HNAME,   
			B.HAKBUN, 
			B.GWA,
			B.ZIP_ID, 
			B.ADDR, 
			C.FNAME,   
			B.DR_HAKYUN,   
			A.HJMOD_SIJUM  
FROM 		HAKSA.HAKJUKBYENDONG A,   
			HAKSA.JAEHAK_HAKJUK B,   
			HAKSA.GWA_SYM C  
WHERE 	( B.HAKBUN 	= A.HAKBUN ) 
AND  		( B.GWA 		= C.GWA) 
AND		( B.SEX		= '1')
AND		( B.SANGTAE = '03')
AND  		( A.YEAR = :as_year ) 
AND 		( A.HAKGI like :as_hakgi ) 
AND		( A.HJMOD_ID = 'D')
AND		C.USE_GUBUN = 'Y'    
ORDER	BY	C.ORDER_SEQ,
			B.JUMIN_NO
" arguments=(("as_year", string),("as_hakgi", string))  sort="jaehak_hakjuk_addr A jaehak_hakjuk_gwa A jaehak_hakjuk_dr_hakyun A jaehak_hakjuk_hakbun A " )
