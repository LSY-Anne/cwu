 retrieve="SELECT 	Z.YEAR,
			Z.HAKGI,
			Z.GWA,
			Z.FNAME,
			Z.HAKBUN,
			Z.HNAME 
FROM		(	SELECT 	A.YEAR,
							A.HAKGI,
							B.GWA,
							C.FNAME,
							B.HAKBUN,
							B.HNAME 
				FROM 		HAKSA.HAKJUKBYENDONG A,
							HAKSA.JAEHAK_HAKJUK B,
							CDDB.KCH003M C
				WHERE 	A.HAKBUN 	=	B.HAKBUN
				AND		B.GWA			=	C.GWA
				AND		A.HJMOD_ID 	='F'
	
				UNION 				

				SELECT 	A.YEAR,
							A.HAKGI,
							B.GWA,
							C.FNAME,
							B.HAKBUN,
							B.HNAME 
				FROM 		HAKSA.JOLUP_HAKBYEN A,
							HAKSA.JOLUP_HAKJUK B,
							CDDB.KCH003M C
				WHERE 	A.HAKBUN 	=	B.HAKBUN
				AND		B.GWA			=	C.GWA
				AND		A.HJMOD_ID 	='F'

			) Z
ORDER BY Z.YEAR,
			Z.HAKGI,
			Z.GWA,
			Z.HAKBUN"  sort="year A hakgi A jaehak_hakjuk_gwa A jaehak_hakjuk_hakbun A " )
