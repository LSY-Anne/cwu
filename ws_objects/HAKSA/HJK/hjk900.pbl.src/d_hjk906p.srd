 column=(type=number updatewhereclause=yes name=c_pyengjum_avg dbname="jaehak_hakjuk.pyengjum_avg" )
 retrieve="SELECT 	B.HAKBUN,   
			B.JOLUP_JUNG_NO,   
			B.GWA,   
			A.HNAME,   
			A.JUMIN_NO,   
			B.JOLUP_DATE,   
			B.HAKWI_NO,   
			B.YEAR,   
			B.JUNHUGI,
			C.CHIDK,
			C.PYENGJUM_AVG
FROM 		HAKSA.JAEHAK_HAKJUK	A,   
			HAKSA.JOLUP_SAJUNG	B,
			(	SELECT 	Y.HAKBUN		HAKBUN,
							NVL(SUM(DECODE(Y.SUNGJUK_INJUNG, 'N', 0, DECODE(Y.HWANSAN_JUMSU, 'F', 0,'W',	0, Y.HAKJUM))),0)	CHIDK	,
							ROUND(SUM(DECODE(Z.PASS_GUBUN, 'Y', 0, DECODE(Y.SUNGJUK_INJUNG, 'N', 0, DECODE(Y.HWANSAN_JUMSU, 'W', 0, Y.PYENGJUM))) * Y.HAKJUM) / SUM(DECODE(Z.PASS_GUBUN, 'Y', 0, DECODE(Y.HWANSAN_JUMSU, 'W', 0, Y.HAKJUM))), 2)	PYENGJUM_AVG
				FROM 		HAKSA.SUGANG Y,
							HAKSA.GWAMOK_CODE	Z
				WHERE		Y.GWAMOK_ID		=	Z.GWAMOK_ID
				AND		Y.GWAMOK_SEQ	=	Z.GWAMOK_SEQ
				
				GROUP BY	Y.HAKBUN
			)	C,
			HAKSA.GWA_SYM D
WHERE 	A.GWA					=	B.GWA
AND		A.GWA					=	D.GWA
AND		A.HAKBUN	 			= B.HAKBUN
AND		A.HAKBUN	 			= C.HAKBUN
AND		B.HAPGYUK_GUBUN 	= '1'
AND  		B.YEAR 				= :as_year
AND  		B.JUNHUGI 			= :as_junhugi
AND  		B.GWA 				like :as_gwa   
ORDER	BY	D.ORDER_SEQ,
			B.HAKWI_NO" arguments=(("as_year", string),("as_junhugi", string),("as_gwa", string)) )
