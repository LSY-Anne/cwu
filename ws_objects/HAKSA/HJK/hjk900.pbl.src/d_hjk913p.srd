			HAKSA.JAEHAK_HAKJUK,
			CDDB.KCH003M     
WHERE 	CDDB.KCH003M.GWA	=	HAKSA.JAEHAK_HAKJUK.GWA
AND		HAKSA.HAKJUKBYENDONG.HAKBUN = HAKSA.JAEHAK_HAKJUK.HAKBUN  
