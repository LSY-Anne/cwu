AND 		HAKSA.HAKJUKBYENDONG.HJMOD_ID IN	('B', 'D')
ORDER BY CDDB.KCH003M.ORDER_SEQ,
			HAKSA.JAEHAK_HAKJUK.DR_HAKYUN ,
