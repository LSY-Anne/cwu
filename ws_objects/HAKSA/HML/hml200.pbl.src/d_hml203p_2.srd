         A.ACCOUNT_NO,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
    FROM HAKSA.POINT_BENEFIT  A
   WHERE ( A.HAKBUN = :as_hakbun ) AND  
         ( A.BENEFIT_YEAR = :as_year )   
            
" arguments=(("as_hakbun", string),("as_year", string)) )
