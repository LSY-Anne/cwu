         A.INCLUDE_FILE_PATH2,   
         A.INCLUDE_FILE_PATH3,   
         A.ACT_TP,   
         A.PURPOSE  
    FROM SUM140TL A  
   WHERE ( A.YEAR = :as_year ) AND  
         ( A.HAKGI = :as_hakgi ) AND  
         ( A.HAKBUN = :as_hakbun ) AND  
         ( A.COUNSEL_TP IN ('2','4') )   
