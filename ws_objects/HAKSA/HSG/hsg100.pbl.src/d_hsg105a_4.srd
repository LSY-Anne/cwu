 retrieve="  SELECT A.CASE_NO,
A.CASE_DATE,
  A.HNAME, 
   A.SEX,    
   A.GWA_NM, 
    A.SU_HAKYUN, 
   A.HAKBUN, 
   B.JUYA_GUBUN , 
   C.BIRTH_DT,
  TRUNC((SYSDATE - C.BIRTH_DT)/ 365) AS AGE,
   C.HOL_TP,
   C.HOL_REASON,
   C.MIL_TP,
   C.RETRY_TP,
   C.HOUSE_ZIP_ADDR || ' ' || C.HOUSE_ADDRESS AS ADDRESS,
   C.TEL,
   C.HP,
   C.EMAIL,
   (SELECT SCHOOL_NM FROM HAKSA.SUM230TL
        WHERE HAKBUN = A.HAKBUN 
        AND SEQ = '1') AS SCHOOL,
   (SELECT MAX(COUNSEL_DT) FROM HAKSA.SUM140TL
        WHERE HAKBUN = A.HAKBUN) AS COUNSEL_DT,
   (SELECT FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'SUM06' 
     AND  CODE = A.MONTHOD ) AS MONTHOD  , 
      A.PURPOSE, 
        (SELECT FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'SUM25' 
     AND  CODE = C.REGLION )  as REGLION,
 (SELECT FNAME FROM CDDB.KCH102D WHERE CODE_GB = 'house_gbn_code' AND  CODE = c.HOUSE_FORM  )       as HOUSE_FORM,
      A.MONTHTL_PAY,
      D.FNAME1,
      D.FNAME2,
      D.FNAME3,
      D.FNAME4,
      D.FNAME5,
      D.FNAME6,
      D.FNAME7,
      D.FNAME8,
      D.FNAME9,
      D.FNAME10,
       E.FNAME11,
      E.FNAME12,
      E.FNAME13,
      E.FNAME14,
      E.FNAME15,
      E.FNAME16,
      E.FNAME17,
      E.FNAME18,
      E.FNAME19,
      E.FNAME20
    FROM HAKSA.SUM120TL  A, HAKSA.JAEHAK_HAKJUK B, HAKSA.SUM220TL C,   
(select   max(decode(RN,1,FNAME))as FNAME1,
max(decode(RN,2,FNAME))as FNAME2,
 max(decode(RN,3,FNAME))as FNAME3,
  max(decode(RN,4,FNAME))as FNAME4,
   max(decode(RN,5,FNAME))as FNAME5,
    max(decode(RN,6,FNAME))as FNAME6,
     max(decode(RN,7,FNAME))as FNAME7,
      max(decode(RN,8,FNAME))as FNAME8,
       max(decode(RN,9,FNAME))as FNAME9,
        max(decode(RN,10,FNAME))as FNAME10         
from (SELECT ROWNUM AS RN, B.FNAME
FROM HAKSA.SUM130TL A, CDDB.KCH102D B
WHERE A.COUNSEL_CD = B.CODE
AND   B.CODE_GB = 'SUM03'  
AND A.YEAR = :as_year
AND A.HAKGI = :as_hakgi
AND A.CASE_NO = :as_case_no
AND A.HAKBUN = :as_hakbun)) D,
(select   max(decode(RN,1,FNAME))as FNAME11,
max(decode(RN,2,FNAME))as FNAME12,
 max(decode(RN,3,FNAME))as FNAME13,
  max(decode(RN,4,FNAME))as FNAME14,
   max(decode(RN,5,FNAME))as FNAME15,
    max(decode(RN,6,FNAME))as FNAME16,
     max(decode(RN,7,FNAME))as FNAME17,
      max(decode(RN,8,FNAME))as FNAME18,
       max(decode(RN,9,FNAME))as FNAME19,
        max(decode(RN,10,FNAME))as FNAME20         
from (SELECT ROWNUM AS RN, B.FNAME
FROM HAKSA.SUM130TL A, CDDB.KCH102D B
WHERE A.COUNSEL_CD = B.CODE
AND   B.CODE_GB = 'SUM04'  
AND A.YEAR = :as_year
AND A.HAKGI = :as_hakgi
AND A.CASE_NO = :as_case_no
AND A.HAKBUN = :as_hakbun)) E
WHERE  A.HAKBUN = B.HAKBUN
AND A.HAKBUN = C.HAKBUN(+)  
AND A.YEAR = :as_year
AND A.HAKGI = :as_hakgi
AND A.CASE_NO = :as_case_no
AND A.HAKBUN = :as_hakbun" arguments=(("as_year", string),("as_hakgi", string),("as_case_no", string),("as_hakbun", string)) )
