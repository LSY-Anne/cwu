 column=(type=number updatewhereclause=yes name=cnt dbname="cnt" )
 retrieve="SELECT A.SU_HAKYUN, 
C.FNAME AS MM, 
COUNT(*) AS CNT,
1 AS sortno
FROM HAKSA.SUM140TL A,
     HAKSA.SUM130TL B,
     CDDB.KCH102D C,
     CDDB.KCH102D D
WHERE A.YEAR = :as_year 
    AND A.COUNSEL_DT BETWEEN :as_fr_dt AND :as_to_dt
    AND a.COUNSEL_TP = '1'
	and a.year = b.year
	and a.hakgi = b.hakgi
	and a.case_no = b.case_no
    AND A.HAKBUN = B.HAKBUN
    AND B.COUNSEL_CD = C.CODE
    AND D.CODE_GB = 'SUM02' 
    AND D.CODE = A.PURPOSE
    AND C.CODE_GB = D.ETC_CD1
    AND  (:as_case_tp = '%' or A.PURPOSE = :as_case_tp)
GROUP BY C.FNAME,A.SU_HAKYUN 
union all
SELECT A.SU_HAKYUN, 
'합계' AS MM, 
COUNT(*) AS CNT,
0 AS sortno
FROM HAKSA.SUM140TL A,
     HAKSA.SUM130TL B,
     CDDB.KCH102D C,
     CDDB.KCH102D D
WHERE A.YEAR = :as_year 
    AND A.COUNSEL_DT BETWEEN :as_fr_dt AND :as_to_dt
    AND a.COUNSEL_TP = '1'
	and a.year = b.year
	and a.hakgi = b.hakgi
	and a.case_no = b.case_no
    AND A.HAKBUN = B.HAKBUN
    AND B.COUNSEL_CD = C.CODE
    AND D.CODE_GB = 'SUM02' 
    AND D.CODE = A.PURPOSE
    AND C.CODE_GB = D.ETC_CD1
    AND  (:as_case_tp = '%' or A.PURPOSE = :as_case_tp)
GROUP BY C.FNAME,A.SU_HAKYUN " arguments=(("as_year", string),("as_fr_dt", string),("as_to_dt", string),("as_case_tp", string))  sort="sum140tl_su_hakyun A " )
