 retrieve="  SELECT MT_MST.SERIAL,   
         MT_MST.YEAR,   
         MT_MST.HAKGI,   
         MT_MST.GUBUN,   
         MT_MST.HAKGWA,   
         MT_MST.JUYA,   
         MT_MST.HAKYUN,   
         MT_MST.SIJUM,   
         MT_MST.JONGJUM,   
         MT_MST.JANGSO,   
         MT_MST.INWON_PROF,   
         MT_MST.INWON_STU,   
         MT_MST.BISANG,   
         MT_MST.BIGO  
    FROM haksa.MT_MST 
   WHERE ( HAKSA.MT_MST.YEAR = :year ) AND  
         ( HAKSA.MT_MST.HAKGI = :hakgi ) AND  
         ( HAKSA.MT_MST.HAKGWA like :hakgwa ) AND  
         ( HAKSA.MT_MST.GUBUN = :gubun )   
ORDER BY MT_MST.SERIAL ASC
" update="haksa.MT_MST" updatewhere=1 updatekeyinplace=no arguments=(("year", string),("hakgi", string),("hakgwa", string),("gubun", string)) )
