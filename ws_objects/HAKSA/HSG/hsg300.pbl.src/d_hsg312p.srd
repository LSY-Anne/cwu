 retrieve="SELECT	B.PROF_ID	PROF,
			A.YEAR		YEAR,
			A.HAKGI		HAKGI,
			COUNT(B.PROF_ID)	CNT
FROM		HAKSA.MT_MST	A,
			HAKSA.MT_PROF	B
WHERE		A.SERIAL = B.SERIAL
AND		B.PROF_ID	LIKE :prof
AND		A.YEAR		BETWEEN :sijum	AND	:jongjum
GROUP BY	B.PROF_ID,
			A.YEAR,
			A.HAKGI
" arguments=(("prof", string),("sijum", string),("jongjum", string)) )
