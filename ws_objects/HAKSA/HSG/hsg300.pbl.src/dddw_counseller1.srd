 retrieve="SELECT DISTINCT
       A.MEMBER_NO,
       A.NAME
  FROM (
  SELECT A.JUMIN_NO MEMBER_NO,
         A.NAME
    FROM SUM110TL A
   WHERE year   = :as_year
     AND hakgi  = :as_hakgi
  UNION ALL
  SELECT A.MEMBER_NO,
         B.NAME
    FROM SUM210TL A,
         INDB.HIN001M B
   WHERE A.MEMBER_NO = B.MEMBER_NO
     AND A.YEAR      = :as_year
     AND A.HAKGI     = :as_hakgi) A
" arguments=(("as_year", string),("as_hakgi", string))  sort="name A " )
