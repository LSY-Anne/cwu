 retrieve="SELECT 	HAKSA.SUGANG_TRANS.HAKBUN,   
			HAKSA.SUGANG_TRANS.JUMSU_1,   
			HAKSA.SUGANG_TRANS.JUMSU_2,   
			HAKSA.SUGANG_TRANS.JUMSU_3,   
			HAKSA.SUGANG_TRANS.JUMSU_4,   
			HAKSA.SUGANG_TRANS.JUMSU_5,   
			HAKSA.SUGANG_TRANS.JUMSU,   
			HAKSA.SUGANG_TRANS.HWANSAN_JUMSU,   
			HAKSA.SUGANG_TRANS.PYENGJUM,   
			HAKSA.SUGANG_TRANS.SUNGJUK_INJUNG,   
			HAKSA.SUGANG_TRANS.YEAR,   
			HAKSA.SUGANG_TRANS.HAKGI,   
			HAKSA.SUGANG_TRANS.GWAMOK_ID,   
			HAKSA.SUGANG_TRANS.HAKJUM,   
			HAKSA.SUGANG_TRANS.GWA,   
			HAKSA.SUGANG_TRANS.GWAMOK_SEQ,   
			HAKSA.JAEHAK_HAKJUK.HNAME,   
			HAKSA.JAEHAK_HAKJUK.SANGTAE,   
			HAKSA.SUGANG_TRANS.JOGI_YN,   
			HAKSA.SUGANG_TRANS.HAKYUN,   
			HAKSA.SUGANG_TRANS.BAN,   
			HAKSA.SUGANG_TRANS.BUNBAN,   
			HAKSA.JAEHAK_HAKJUK.GWA,   
			HAKSA.JAEHAK_HAKJUK.DR_HAKYUN,
			HAKSA.GWA_SYM.ORDER_SEQ         
FROM 		HAKSA.SUGANG_TRANS,   
			HAKSA.JAEHAK_HAKJUK,
			HAKSA.GWA_SYM 
WHERE 	HAKSA.JAEHAK_HAKJUK.GWA	=	HAKSA.GWA_SYM.GWA
AND		HAKSA.SUGANG_TRANS.HAKBUN = HAKSA.JAEHAK_HAKJUK.HAKBUN 
and  		HAKSA.SUGANG_TRANS.YEAR = :as_year
AND  		HAKSA.SUGANG_TRANS.HAKGI = :as_hakgi
AND  		HAKSA.SUGANG_TRANS.GWA = :as_gwa
AND  		HAKSA.SUGANG_TRANS.HAKYUN = :as_hakyun
AND  		HAKSA.SUGANG_TRANS.BAN = :as_ban
AND  		HAKSA.SUGANG_TRANS.GWAMOK_ID = :as_gwamok 
AND  		HAKSA.SUGANG_TRANS.GWAMOK_SEQ = :al_gwamok_seq
AND  		HAKSA.SUGANG_TRANS.BUNBAN = :as_bunban
AND  		(HAKSA.SUGANG_TRANS.SUNGJUK_INJUNG = 'Y' OR  
			HAKSA.SUGANG_TRANS.HWANSAN_JUMSU = 'W') 
" update="HAKSA.SUGANG_TRANS" updatewhere=0 updatekeyinplace=yes arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_hakyun", string),("as_ban", string),("as_gwamok", string),("al_gwamok_seq", number),("as_bunban", string))  sort="gwa_sym_order_seq A jaehak_hakjuk_dr_hakyun D hakbun A " )
