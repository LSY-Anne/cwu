			SUNGJUKGYE.GYUNGGO_CNT,
             SUNGJUKGYE.WORKER,
             SUNGJUKGYE.IPADDR,  
             SUNGJUKGYE.WORK_DATE,
             SUNGJUKGYE.JOB_UID,
             SUNGJUKGYE.JOB_ADD, 
             SUNGJUKGYE.JOB_DATE
    FROM HAKSA.SUNGJUKGYE,   
         HAKSA.JAEHAK_HAKJUK  
   WHERE ( SUNGJUKGYE.HAKBUN = JAEHAK_HAKJUK.HAKBUN ) and  
