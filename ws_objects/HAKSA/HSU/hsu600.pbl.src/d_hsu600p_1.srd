 retrieve="SELECT 	HAKSA.GAESUL_GWAMOK.GWA,   
			HAKSA.GAESUL_GWAMOK.HAKYUN,   
			HAKSA.GAESUL_GWAMOK.BAN,   
			HAKSA.GAESUL_GWAMOK.GWAMOK_ID,   
			HAKSA.GAESUL_GWAMOK.GWAMOK_SEQ,   
			HAKSA.GAESUL_GWAMOK.BUNBAN,   
			HAKSA.GAESUL_GWAMOK.MEMBER_NO,   
			HAKSA.GAESUL_GWAMOK.PYUNGGA_GUBUN,   
			HAKSA.JAEHAK_HAKJUK.GWA,   
			HAKSA.SUGANG_TRANS.HAKJUM,   
			HAKSA.SUGANG_TRANS.JUMSU_1,   
			HAKSA.SUGANG_TRANS.JUMSU_2,   
			HAKSA.SUGANG_TRANS.JUMSU_3,   
			HAKSA.SUGANG_TRANS.JUMSU_4,   
			HAKSA.SUGANG_TRANS.JUMSU_5,   
			HAKSA.GAESUL_GWAMOK.GWAMOK_ID||HAKSA.GAESUL_GWAMOK.GWAMOK_SEQ as GWAMOK,   
			HAKSA.SUGANG_TRANS.JUMSU,   
			HAKSA.SUGANG_TRANS.HWANSAN_JUMSU,   
			HAKSA.SUGANG_TRANS.PYENGJUM,   
			HAKSA.JAEHAK_HAKJUK.HAKBUN,   
			HAKSA.JAEHAK_HAKJUK.DR_HAKYUN,   
			HAKSA.JAEHAK_HAKJUK.HNAME,   
			HAKSA.JAEHAK_HAKJUK.BAN ,
			HAKSA.GWA_SYM.ORDER_SEQ,
         'N' tmt_each_yn
FROM 		HAKSA.GAESUL_GWAMOK,   
			HAKSA.SUGANG_TRANS,   
			HAKSA.JAEHAK_HAKJUK,
			HAKSA.GWA_SYM     
WHERE 	HAKSA.GAESUL_GWAMOK.GWA	=	TRIM(HAKSA.GWA_SYM.GWA)
AND		HAKSA.GAESUL_GWAMOK.YEAR = HAKSA.SUGANG_TRANS.YEAR 
and  		HAKSA.GAESUL_GWAMOK.HAKGI = HAKSA.SUGANG_TRANS.HAKGI 
and  		HAKSA.GAESUL_GWAMOK.GWA = HAKSA.SUGANG_TRANS.GWA 
and  		HAKSA.GAESUL_GWAMOK.HAKYUN = HAKSA.SUGANG_TRANS.HAKYUN 
and  		HAKSA.GAESUL_GWAMOK.BAN = HAKSA.SUGANG_TRANS.BAN 
and  		HAKSA.GAESUL_GWAMOK.GWAMOK_ID = HAKSA.SUGANG_TRANS.GWAMOK_ID 
and  		HAKSA.GAESUL_GWAMOK.GWAMOK_SEQ = HAKSA.SUGANG_TRANS.GWAMOK_SEQ 
and  		HAKSA.GAESUL_GWAMOK.BUNBAN = HAKSA.SUGANG_TRANS.BUNBAN
and  		HAKSA.SUGANG_TRANS.HAKBUN = HAKSA.JAEHAK_HAKJUK.HAKBUN 
and  		HAKSA.SUGANG_TRANS.YEAR = :as_year 
AND  		HAKSA.SUGANG_TRANS.HAKGI = :as_hakgi
AND  		HAKSA.SUGANG_TRANS.GWA like :as_gwa 
AND  		HAKSA.SUGANG_TRANS.HAKYUN like :as_hakyun 
AND  		HAKSA.GAESUL_GWAMOK.MEMBER_NO like :as_prof 
AND  		(HAKSA.SUGANG_TRANS.SUNGJUK_INJUNG = 'Y' OR  
			HAKSA.SUGANG_TRANS.HWANSAN_JUMSU = 'W') 
AND  		HAKSA.GAESUL_GWAMOK.HAKJUM <> 0 
AND  		HAKSA.GAESUL_GWAMOK.BAN like :as_ban    
" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_hakyun", string),("as_prof", string),("as_ban", string))  sort="gwa_sym_order_seq A gaesul_gwamok_hakyun A gaesul_gwamok_ban A gaesul_gwamok_gwamok_id A gaesul_gwamok_gwamok_seq A gaesul_gwamok_bunban A jaehak_hakjuk_gwa A jaehak_hakjuk_dr_hakyun D hakbun A " )
