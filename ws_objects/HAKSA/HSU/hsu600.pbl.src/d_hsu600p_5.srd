				B.GWA,
				B.HAKBUN,
         	A.HNAME,
         	B.SINCHUNG_HAKJUM,
         	B.CHIDK_HAKJUM,
         	B.TOTAL_PYENGJUM,
         	B.AVG_PYENGJUM,
				(	SELECT	SUM(Z.HAKJUM * Z.JUMSU) 
					FROM 		HAKSA.SUGANG Z
					WHERE		Z.YEAR		= B.YEAR
					AND		Z.HAKGI 		= B.HAKGI
					AND		Z.HAKBUN		= B.HAKBUN ) TOTAL_SILJUM,
				B.SUKCHA,
				DECODE(B.GYUNGGO_YN, '0', '', '1', '학고', '2', '유예'),
				C.ORDER_SEQ
FROM 			HAKSA.JAEHAK_HAKJUK	A,
         	HAKSA.SUNGJUKGYE	   B,
				HAKSA.GWA_SYM			C
WHERE			B.GWA			=	C.GWA
AND			B.HAKBUN		=	A.HAKBUN
AND			B.YEAR 		= 	:as_year
AND  			B.HAKGI 		= 	:as_hakgi
AND			B.HAKYUN	like	:as_hakyun
