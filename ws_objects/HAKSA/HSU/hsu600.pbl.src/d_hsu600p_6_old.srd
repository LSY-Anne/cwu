 retrieve="SELECT	DISTINCT
			B.MEMBER_NO	AS	MEMBER_NO,
			B.GWA			AS	GWA,
			B.HAKYUN		AS	HAKYUN,
			B.BAN			AS	BAN,
			B.GWAMOK_ID||B.GWAMOK_SEQ	AS	GWAMOK,
			B.GWAMOK_ID||B.BUNBAN		AS	HAKSU_NO
FROM	HAKSA.SUGANG_TRANS	A,
		HAKSA.GAESUL_GWAMOK	B
WHERE 	A.YEAR			= B.YEAR
AND		A.HAKGI			= B.HAKGI
AND		A.GWA				= B.GWA
AND		A.HAKYUN			= B.HAKYUN
AND		A.BAN				= B.BAN
