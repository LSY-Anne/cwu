 retrieve="SELECT	B.YEAR				AS	YEAR	,
			B.HAKGI				AS HAKGI	,
			A.EVALUATE_ID		AS	E_ID	,
			A.EVALUATE_NAME	AS	E_NAME,
			ROUND(NVL(SUM(DECODE(C.SU_HAKYUN, '1', B.JUMSU, 0)) * 20 / SUM(DECODE(C.SU_HAKYUN, '1', 1, NULL)), 0), 2)	AS HAKYUN1	,
			ROUND(NVL(SUM(DECODE(C.SU_HAKYUN, '2', B.JUMSU, 0)) * 20 / SUM(DECODE(C.SU_HAKYUN, '2', 1, NULL)), 0), 2)	AS HAKYUN2	,
			ROUND(NVL(SUM(DECODE(C.SU_HAKYUN, '3', B.JUMSU, 0)) * 20 / SUM(DECODE(C.SU_HAKYUN, '3', 1, NULL)), 0), 2)	AS HAKYUN3	,
			ROUND(NVL(SUM(DECODE(C.SU_HAKYUN, '4', B.JUMSU, 0)) * 20 / SUM(DECODE(C.SU_HAKYUN, '4', 1, NULL)), 0), 2)	AS HAKYUN4
FROM		HAKSA.EVALUATE_CODE	A,
			HAKSA.EVALUATE			B,
			HAKSA.JAEHAK_HAKJUK	C
WHERE		A.YEAR			= B.YEAR
AND		A.HAKGI			= B.HAKGI
AND		A.EVALUATE_ID	= B.EVALUATE_ID
AND		B.HAKBUN			= C.HAKBUN
AND		B.YEAR			= :year
AND		B.HAKGI			= :hakgi
GROUP BY	B.YEAR			,
			B.HAKGI			,
			A.EVALUATE_ID	,
			A.EVALUATE_NAME" arguments=(("year", string),("hakgi", string)) )
