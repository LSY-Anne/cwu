 retrieve="SELECT	RTRIM(A.NAME)				KNAME,
			A.MEMBER_NO,
			A.JUMIN_NO,
			A.GWA,
			SUBSTR(A.DUTY_CODE,1,1)	JIKJONG_CODE,
			A.JIKWI_CODE,
			A.DUTY_CODE,
			A.JIKMU_CODE,
			A.FIRSTHIRE_DATE,
			A.HAKWONHIRE_DATE,
			A.SAL_CLASS,
			FU_DEPT_NM(A.GWA,'K')														COM_DEPT_NM,
			FU_CODE_NM('HAENG','JIKJONG_CODE',SUBSTR(A.DUTY_CODE,1,1),'K')	COM_JIKJONG_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')					COM_JIKWI_NM,
			FU_DUTY_NM(A.DUTY_CODE,'K')												COM_DUTY_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKMU_CODE,'K')					COM_JIKMU_NM
FROM		INDB.HIN001M A
WHERE		A.NAME      LIKE :as_KName||'%'
AND		A.MEMBER_NO LIKE :as_MemberNo||'%'
AND		A.DUTY_CODE  BETWEEN DECODE(:as_JikjongCode,'1','000','2','200','000') 
							 AND     DECODE(:as_JikjongCode,'1','199','2','999','999')
ORDER	BY	A.MEMBER_NO
" update="INDB.HIN001M" updatewhere=1 updatekeyinplace=no arguments=(("as_KName", string),("as_MemberNo", string),("as_JikjongCode", string)) )
