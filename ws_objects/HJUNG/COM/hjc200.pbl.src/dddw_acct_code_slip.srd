 retrieve="
select	acct_code, acct, bdgt, 
			rtrim(acct_name) acct_name,
			slip_class,
			decode(level_class, 4, 	decode(	(	select	rtrim(acct_name)
															from		acdb.hac001m
															where		slip_class	=	a.slip_class
															and		level_class	<>	a.level_class	
															and		substr(acct_code, 1, 5)	=	substr(a.acct_code, 1, 5)	),	rtrim(acct_name), 
														rtrim(acct_name), 
														(	select	rtrim(acct_name)
															from		acdb.hac001m
															where		slip_class	=	a.slip_class
															and		level_class	<>	a.level_class	
															and		substr(acct_code, 1, 5)	=	substr(a.acct_code, 1, 5)	) || ' ' || rtrim(acct_name)),
											rtrim(acct_name)) as full_name
from		acdb.hac001m a
where		slip_class		like	:as_slip_class	 || '%'
and		level_class		like	:as_level_class || '%'
order by acct_code		
" update="acdb.hac001m" updatewhere=0 updatekeyinplace=no arguments=(("as_slip_class", string),("as_level_class", string)) )
