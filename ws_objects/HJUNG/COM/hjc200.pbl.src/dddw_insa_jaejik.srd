 column=(type=char(8) updatewhereclause=yes name=firsthire_date dbname="hin001m.firsthire_date" )
 column=(type=char(8) updatewhereclause=yes name=retire_date dbname="hin001m.retire_date" )
 column=(type=decimal(0) updatewhereclause=yes name=ann_opt dbname="hin001m.ann_opt" values="호봉제	1/연봉제	2/" )
 retrieve="
	select	a.member_no, a.name, a.sal_class, a.gwa, b.jikjong_code, b.duty_code, 
				c.fname as dept_name, b.jikjong_fname, b.duty_name,
				nvl(jikwi_code, '') jikwi_code, 
				nvl(jikmu_code, '') jikmu_code, 
				nvl(bojik_code1, '') bojik_code1,
				jumin_no,
				a.member_no || '   ' || rtrim(a.name) as member_name,
				decode(a.jaejik_opt, 1, 1, 2) jaejik_opt,
				a.firsthire_date, a.retire_date,
				a.ann_opt
	from		indb.hin001m a, indb.hin003m_02v b, cddb.kch003m c
	where		b.jikjong_code 		>=		:ai_str_jikjong
	and		b.jikjong_code			<=		:ai_end_jikjong
	and		a.gwa			like	:as_gwa || '%'
	and		a.duty_code	=		b.duty_code	(+)
	and		a.gwa			=		c.gwa(+)
	and		(jaejik_opt	in (1, 2, 4)	or (jaejik_opt = 3 and substr(retire_date, 1, 6) >= :as_yearmonth))
	order by a.gwa, a.member_no
" arguments=(("ai_str_jikjong", number),("ai_end_jikjong", number),("as_gwa", string),("as_yearmonth", string)) )
