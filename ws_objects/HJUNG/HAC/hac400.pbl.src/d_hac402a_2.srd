			A.JOB_ADD,
			A.JOB_DATE
FROM		ACDB.HAC012H A, ACDB.HAC011H B
WHERE		A.BDGT_YEAR = B.BDGT_YEAR
AND		A.GWA = B.GWA
AND		A.ACCT_CODE = B.ACCT_CODE
AND		A.ACCT_CLASS = B.ACCT_CLASS
