 column=(type=char(4) updatewhereclause=yes name=gwan_code dbname="hac011h.gwan_code" )
 column=(type=char(36) updatewhereclause=yes name=hac101m_acctm_name dbname="acctm_name" )
 column=(type=number updatewhereclause=yes name=amt dbname="amt" )
 column=(type=number updatewhereclause=yes name=bef_amt dbname="bef_amt" )
 retrieve="SELECT	CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	OR	SUBSTR(A.ACCT_CODE,1,1) = '5'
					THEN	1
					WHEN	SUBSTR(A.ACCT_CODE,1,1) > '5'
					THEN	9
					ELSE	2
			END	GUBUN,
			B.LARGE_ACCT	GWAN_CODE,
			CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) < '6'
					THEN	DECODE(A.IO_GUBUN,'1',C.ACCT_INAME,'2',C.ACCT_ONAME,C.ACCT_NAME)||'('||B.LARGE_ACCT||')'
					ELSE	DECODE(A.IO_GUBUN,'1',C.ACCT_INAME,'2',C.ACCT_ONAME,C.ACCT_NAME)
			END	ACCTM_NAME,
			DECODE(:AS_AMT_GUBUN,'1',NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.REQ_AMT,-A.REQ_AMT),A.REQ_AMT),0)),0),
										'2',NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.ADJUST_AMT,-A.ADJUST_AMT),A.ADJUST_AMT),0)),0),
											 NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.CONFIRM_AMT,-A.CONFIRM_AMT),A.CONFIRM_AMT),0)),0))	AMT,
			DECODE(:AS_AMT_GUBUN,'1',NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BEF_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.REQ_AMT,-A.REQ_AMT),A.REQ_AMT),0)),0),
										'2',NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BEF_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.ADJUST_AMT,-A.ADJUST_AMT),A.ADJUST_AMT),0)),0),
											 NVL(SUM(DECODE(A.BDGT_YEAR,:AS_BEF_BDGT_YEAR,DECODE(SUBSTR(A.ACCT_CODE,1,1),'9',DECODE(SUBSTR(A.ACCT_CODE,1,3),'911',A.CONFIRM_AMT,-A.CONFIRM_AMT),A.CONFIRM_AMT),0)),0))	BEF_AMT
FROM		ACDB.HAC011H A, ACDB.HAC001M B, ACDB.HAC001M C
WHERE		A.ACCT_CODE = B.ACCT_CODE (+)
AND		B.LARGE_ACCT||'00' = C.ACCT_CODE (+)
AND		A.BDGT_YEAR IN (:AS_BDGT_YEAR, :AS_BEF_BDGT_YEAR)
AND		A.ACCT_CLASS = :AI_ACCT_CLASS
AND		A.IO_GUBUN = :AS_SLIP_CLASS
AND		A.BDGT_CLASS = :AI_BDGT_CLASS
AND		DECODE(:AS_AMT_GUBUN,'1',A.REQ_AMT,'2',A.ADJUST_AMT,A.CONFIRM_AMT) <> 0
GROUP BY	SUBSTR(A.ACCT_CODE,1,1), B.LARGE_ACCT, DECODE(A.IO_GUBUN,'1',C.ACCT_INAME,'2',C.ACCT_ONAME,C.ACCT_NAME)
ORDER BY	CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	OR	SUBSTR(A.ACCT_CODE,1,1) = '5'
					THEN	1
					WHEN	SUBSTR(A.ACCT_CODE,1,1) > '5'
					THEN	9
					ELSE	2
			END, B.LARGE_ACCT
" arguments=(("as_bdgt_year", string),("as_bef_bdgt_year", string),("ai_acct_class", number),("as_slip_class", string),("ai_bdgt_class", number),("as_amt_gubun", string)) )
