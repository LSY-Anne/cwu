AND		A.APPLY_DATE BETWEEN :AS_FROM_DATE AND :AS_TO_DATE
AND		A.ORD_CLASS IN (1, 3, 999)
ORDER BY A.APPLY_GWA, 
			A.APPLY_DATE, 
			A.GIAN_NUM

