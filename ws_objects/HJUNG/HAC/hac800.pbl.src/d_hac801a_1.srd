			A.APPLY_DATE,
			A.GIAN_NUM,
			A.ITEM_NO,
			A.ITEM_NAME,
			A.ACCT_CODE,
			B.ACCT_ONAME,
			A.ASSN_USED_AMT,
			A.ASSN_TEMP_AMT,
			A.ASSN_REAL_AMT,
			A.ITEM_STAND_SIZE,
			A.APPLY_QTY,
			A.APPLY_PRICE,
			A.APPLY_AMT,
			A.APPLY_NOTE,
			A.ORD_CLASS,
			A.WORKER	JOB_UID
FROM		STDB.HST105H A, ACDB.HAC001M B
WHERE		A.ACCT_CODE = B.ACCT_CODE (+)
