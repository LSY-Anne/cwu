 retrieve="SELECT	A.ACCT_CLASS,
			A.RESOL_DATE,
			A.RESOL_NO,
			A.RESOL_SEQ,
			A.ACCT_CODE,
			A.DRCR_CLASS,
			A.PROOF_GUBUN,
			A.PROOF_DATE,
			A.RESOL_AMT,
			A.RESOL_AMT	COM_RESOL_AMT,
			A.MANA_CODE1,
			A.MANA_DATA1,
			A.MANA_CODE2,
			A.MANA_DATA2,
			A.MANA_CODE3,
			A.MANA_DATA3,
			A.MANA_CODE4,
			A.MANA_DATA4,
			A.REMARK,
			A.ASSN_USED_AMT,
			A.ASSN_TEMP_AMT,
			A.ASSN_REAL_AMT,
			A.SLIP_DATE,
			A.SLIP_NO,
			A.SLIP_SEQ,
			A.BDGT_YEAR,
			A.WORKER,
			A.IPADDR,
			A.WORK_DATE,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
			DECODE(B.SLIP_CLASS,'1',C.ACCT_INAME,'2',C.ACCT_ONAME,C.ACCT_NAME)	COM_ACCT_NAME,
			C.BDGT_CNTL_YN,
			B.RESOL_GWA,
			B.SLIP_CLASS,
			''	COM_MANA1_NAME,
			''	COM_MANA2_NAME,
			''	COM_MANA3_NAME,
			''	COM_MANA4_NAME,
			C.DRCR_CLASS	COM_DRCR_CLASS,
			C.MANA_CODE		COM_MANA_CODE,
			C.MI_ACCT_YN	COM_MI_GUBUN
FROM		FNDB.HFN102M A,
			FNDB.HFN101M B,
			ACDB.HAC001M C
WHERE		A.ACCT_CLASS = B.ACCT_CLASS
AND		A.RESOL_DATE = B.RESOL_DATE
AND		A.RESOL_NO   = B.RESOL_NO
AND		A.ACCT_CODE  = C.ACCT_CODE (+)
AND		A.ACCT_CLASS = :AI_ACCT_CLASS
AND		A.RESOL_DATE = :AS_RESOL_DATE
AND		A.RESOL_NO   = :AI_RESOL_NO
ORDER BY A.RESOL_SEQ" update="FNDB.HFN102M" updatewhere=0 updatekeyinplace=no arguments=(("ai_acct_class", number),("as_resol_date", string),("ai_resol_no", number)) )
