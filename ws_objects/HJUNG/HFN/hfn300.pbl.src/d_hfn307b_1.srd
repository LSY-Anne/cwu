 retrieve="SELECT	A.ACCT_CLASS,
			A.SLIP_DATE,
			A.SLIP_NO,
			A.SLIP_CLASS,
			A.REMARK,
			A.STEP_OPT,
			A.SLIP_GWA,
			A.BDGT_YEAR,
			A.ACCT_DATE,
			A.ACCT_DATE	OLD_ACCT_DATE,
			A.ACCT_NO,
			A.ACCT_NO	OLD_ACCT_NO,
			(	SELECT	NVL(SUM(B.SLIP_AMT),0)
				FROM		FNDB.HFN202M B
				WHERE		A.ACCT_CLASS = B.ACCT_CLASS
				AND		A.SLIP_DATE  = B.SLIP_DATE
				AND		A.SLIP_NO    = B.SLIP_NO
				AND		B.DRCR_CLASS = 'D'	)	TOT_AMT,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
			0	STAT_GBN,
			A.RESOL_DATE,
			A.RESOL_NO
FROM		FNDB.HFN201M A
WHERE		A.ACCT_CLASS = :AI_ACCT_CLASS
AND		A.SLIP_DATE BETWEEN :AS_FROM_DATE AND :AS_TO_DATE
AND		A.SLIP_CLASS = :AI_SLIP_CLASS
AND		A.STEP_OPT   = :AI_STEP_OPT
ORDER BY	A.SLIP_CLASS, A.SLIP_DATE, A.SLIP_NO" update="FNDB.HFN201M" updatewhere=1 updatekeyinplace=yes arguments=(("ai_acct_class", number),("ai_slip_class", number),("as_from_date", string),("as_to_date", string),("ai_step_opt", number)) )
