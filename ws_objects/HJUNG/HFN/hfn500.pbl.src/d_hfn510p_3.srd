table(column=(type=char(1) updatewhereclause=yes name=slip_class dbname="slip_class" values="수입	1/지출	2/" )
 column=(type=char(1) updatewhereclause=yes name=sort dbname="sort" )
 column=(type=char(7) updatewhereclause=yes name=large_code dbname="large_code" )
 column=(type=char(30) updatewhereclause=yes name=large_name dbname="large_name" )
 column=(type=char(8) updatewhereclause=yes name=middle_code dbname="middle_code" )
 column=(type=char(30) updatewhereclause=yes name=middle_name dbname="middle_name" )
 column=(type=char(9) updatewhereclause=yes name=acct_code dbname="acct_code" )
 column=(type=char(30) updatewhereclause=yes name=acct_name dbname="acct_name" )
 column=(type=number updatewhereclause=yes name=bdgt_acct_amt dbname="bdgt_acct_amt" )
 column=(type=number updatewhereclause=yes name=yebibi_amt dbname="yebibi_amt" )
 column=(type=number updatewhereclause=yes name=jen_amt dbname="jen_amt" )
 column=(type=number updatewhereclause=yes name=acct_amt dbname="acct_amt" )
 column=(type=char(1) updatewhereclause=yes name=gubun dbname="gubun" )
 column=(type=number updatewhereclause=yes name=acct_month_amt dbname="acct_month_amt" )
 column=(type=char(35) updatewhereclause=yes name=title_dt dbname="title_dt" )
 retrieve="SELECT	A.SLIP_CLASS						SLIP_CLASS,
			A.SORT								SORT,
			A.LARGE_CODE						LARGE_CODE,
			A.LARGE_NAME						LARGE_NAME,
			A.MIDDLE_CODE						MIDDLE_CODE,
			A.MIDDLE_NAME						MIDDLE_NAME,
			A.ACCT_CODE							ACCT_CODE,
			A.ACCT_NAME							ACCT_NAME,
			NVL(SUM(A.BDGT_ACCT_AMT),0)	BDGT_ACCT_AMT,
			NVL(SUM(A.YEBIBI_AMT),0)		YEBIBI_AMT,
			NVL(SUM(A.JEN_AMT),0)			JEN_AMT,
			NVL(SUM(A.ACCT_AMT),0)			ACCT_AMT,
			A.GUBUN								GUBUN,
			NVL(SUM(A.ACCT_MONTH_AMT),0)	ACCT_MONTH_AMT,
'( ' || to_char(to_date(:as_strdate), 'yyyy/mm/dd')  || ' 부터 ' ||  to_char(to_date(:as_enddate), 'yyyy/mm/dd') || ' 까지 )' as title_dt
FROM		(
			 
			SELECT	DISTINCT
						'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '5'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						C.ACCT_ONAME								LARGE_NAME,
						''												MIDDLE_CODE,
						''												MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'1'											GUBUN
			FROM		ACDB.HAC001M A, ACDB.HAC001M C
			WHERE		SUBSTR(A.ACCT_CODE,1,2)	IN	(	SELECT	DISTINCT	SUBSTR(B.FORM_CODE,1,2)
																FROM		ACDB.HAC006M B
																WHERE		SUBSTR(B.USED_GBN,4,1) = '9'	)
			AND		A.LEVEL_CLASS = '1'
			AND		A.JICHUL_GUBUN = 'Y'
			AND		SUBSTR(A.ACCT_CODE,1,2)||'0000' = C.ACCT_CODE
			UNION ALL
			 
			SELECT	DISTINCT
						'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '5'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						''												LARGE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,3)||'0'	MIDDLE_CODE,
						C.ACCT_ONAME								MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'2'											GUBUN
			FROM		ACDB.HAC001M A, ACDB.HAC001M C
			WHERE		SUBSTR(A.ACCT_CODE,1,3)	IN	(	SELECT	DISTINCT	SUBSTR(B.FORM_CODE,1,3)
																FROM		ACDB.HAC006M B
																WHERE		SUBSTR(B.USED_GBN,4,1) = '9'	)
			AND		A.LEVEL_CLASS = '2'
			AND		A.JICHUL_GUBUN = 'Y'
			AND		SUBSTR(A.ACCT_CODE,1,3)||'000' = C.ACCT_CODE
			UNION ALL
			 
			SELECT	DISTINCT
						'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '5'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						''												LARGE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,3)||'0'	MIDDLE_CODE,
						''												MIDDLE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,4)			ACCT_CODE,
						C.ACCT_ONAME								ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'3'											GUBUN
			FROM		ACDB.HAC001M A, ACDB.HAC001M C
			WHERE		SUBSTR(A.ACCT_CODE,1,4)	IN	(	SELECT	DISTINCT	SUBSTR(B.FORM_CODE,1,4)
																FROM		ACDB.HAC006M B
																WHERE		SUBSTR(B.USED_GBN,4,1) = '9'	)
			AND		A.LEVEL_CLASS = '3'
			AND		A.JICHUL_GUBUN = 'Y'
			AND		SUBSTR(A.ACCT_CODE,1,4)||'00' = C.ACCT_CODE
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						B.ACCT_ONAME								LARGE_NAME,
						''												MIDDLE_CODE,
						''												MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						NVL(SUM(A.ASSN_USED_AMT),0)			BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'1'											GUBUN
			FROM		ACDB.HAC012M A, ACDB.HAC001M B
			WHERE		BDGT_YEAR = :AS_BDGT_YEAR
			AND		(	SUBSTR(A.ACCT_CODE,1,1) = '4' OR
							SUBSTR(A.ACCT_CODE,1,3) IN ('122','123','124','131','132','211','221','222')	)
			AND		SUBSTR(A.ACCT_CODE,1,2)||'0000' = B.ACCT_CODE
			AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
			AND		A.IO_GUBUN = '2'
			GROUP	BY	SUBSTR(A.ACCT_CODE,1,1), SUBSTR(A.ACCT_CODE,1,2), B.ACCT_ONAME
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						''												LARGE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,3)||'0'	MIDDLE_CODE,
						B.ACCT_ONAME								MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						NVL(SUM(A.ASSN_USED_AMT),0)			BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'2'											GUBUN
			FROM		ACDB.HAC012M A, ACDB.HAC001M B
			WHERE		BDGT_YEAR = :AS_BDGT_YEAR
			AND		(	SUBSTR(A.ACCT_CODE,1,1) = '4' OR
							SUBSTR(A.ACCT_CODE,1,3) IN ('122','123','124','131','132','211','221','222')	)
			AND		SUBSTR(A.ACCT_CODE,1,3)||'000' = B.ACCT_CODE
			AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
			AND		A.IO_GUBUN = '2'
			GROUP	BY	SUBSTR(A.ACCT_CODE,1,1), SUBSTR(A.ACCT_CODE,1,2), SUBSTR(A.ACCT_CODE,1,3), B.ACCT_ONAME
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						''												LARGE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,3)||'0'	MIDDLE_CODE,
						''												MIDDLE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,4)			ACCT_CODE,
						B.ACCT_ONAME								ACCT_NAME,
						NVL(SUM(A.ASSN_USED_AMT),0)			BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'3'											GUBUN
			FROM		ACDB.HAC012M A, ACDB.HAC001M B
			WHERE		BDGT_YEAR = :AS_BDGT_YEAR
			AND		(	SUBSTR(A.ACCT_CODE,1,1) = '4' OR
							SUBSTR(A.ACCT_CODE,1,3) IN ('122','123','124','131','132','211','221','222')	)
			AND		SUBSTR(A.ACCT_CODE,1,4)||'00' = B.ACCT_CODE
			AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
			AND		A.IO_GUBUN = '2'
			GROUP	BY	SUBSTR(A.ACCT_CODE,1,1), SUBSTR(A.ACCT_CODE,1,2), SUBSTR(A.ACCT_CODE,1,3), SUBSTR(A.ACCT_CODE,1,4), B.ACCT_ONAME
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						B.ACCT_ONAME								LARGE_NAME,
						''												MIDDLE_CODE,
						''												MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0)) -
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0))
										ELSE
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0)) -
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0))
										END
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '2'	THEN	
								CASE	WHEN	B.DRCR_CLASS = 'C'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								ELSE
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								END	ACCT_AMT,
						'1'											GUBUN
			FROM		(SELECT AA.ACCT_DATE 	ACCT_DATE, 
								  AA.ACCT_CLASS 	ACCT_CLASS, 
								  AA.BDGT_YEAR 	BDGT_YEAR, 
								  AA.ACCT_CODE 	ACCT_CODE, 
								  AA.DR_ALT_AMT 	DR_ALT_AMT,
					  			  AA.DR_CASH_AMT 	DR_CASH_AMT,	
								  AA.CR_ALT_AMT 	CR_ALT_AMT, 
								  AA.CR_CASH_AMT 	CR_CASH_AMT
			 			 FROM   FNDB.HFN502H AA, ACDB.HAC001M BB 
						 WHERE  AA.ACCT_CODE = BB.ACCT_CODE AND BB.JG_GUBUN = 'Y') A, ACDB.HAC001M B
			WHERE		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
			AND		A.BDGT_YEAR = :AS_BDGT_YEAR
			AND		SUBSTR(A.ACCT_DATE,5,4) <> '0000'
			AND		A.ACCT_DATE <= :AS_ENDDATE
			AND		(	SUBSTR(A.ACCT_CODE,1,1) = '4' OR
							SUBSTR(A.ACCT_CODE,1,3) IN ('122','123','124','131','132','211','221','222')	)
			AND		SUBSTR(A.ACCT_CODE,1,2)||'0000' = B.ACCT_CODE
			GROUP BY SUBSTR(A.ACCT_CODE,1,1), SUBSTR(A.ACCT_CODE,1,2), B.ACCT_ONAME, B.DRCR_CLASS
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						B.ACCT_ONAME								LARGE_NAME,
						''												MIDDLE_CODE,
						''												MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0)) -
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0))
										ELSE
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0)) -
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0))
										END
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '2'	THEN	
								CASE	WHEN	B.DRCR_CLASS = 'C'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								ELSE
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								END	ACCT_MONTH_AMT,
						0												ACCT_AMT,
						'1'											GUBUN
			FROM		(SELECT AA.ACCT_DATE 	ACCT_DATE, 
								  AA.ACCT_CLASS 	ACCT_CLASS, 
								  AA.BDGT_YEAR 	BDGT_YEAR, 
								  AA.ACCT_CODE 	ACCT_CODE, 
								  AA.DR_ALT_AMT 	DR_ALT_AMT,
					  			  AA.DR_CASH_AMT 	DR_CASH_AMT,	
								  AA.CR_ALT_AMT 	CR_ALT_AMT, 
								  AA.CR_CASH_AMT 	CR_CASH_AMT
			 			 FROM   FNDB.HFN502H AA, ACDB.HAC001M BB 
						 WHERE  AA.ACCT_CODE = BB.ACCT_CODE AND BB.JG_GUBUN = 'Y') A, ACDB.HAC001M B
			WHERE		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
			AND		A.BDGT_YEAR = :AS_BDGT_YEAR
			AND		SUBSTR(A.ACCT_DATE,1,6) = SUBSTR(:AS_ENDDATE,1,6)
			AND		(	SUBSTR(A.ACCT_CODE,1,1) = '4' OR
							SUBSTR(A.ACCT_CODE,1,3) IN ('122','123','124','131','132','211','221','222')	)
			AND		SUBSTR(A.ACCT_CODE,1,2)||'0000' = B.ACCT_CODE
			GROUP BY SUBSTR(A.ACCT_CODE,1,1), SUBSTR(A.ACCT_CODE,1,2), B.ACCT_ONAME, B.DRCR_CLASS
			UNION	ALL
			 
			SELECT	'2'	SLIP_CLASS,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN	'1'
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '1'	THEN	'2'
								ELSE	'3'	END	SORT,
						'2'||SUBSTR(A.ACCT_CODE,1,2)||'00'	LARGE_CODE,
						''												LARGE_NAME,
						'2'||SUBSTR(A.ACCT_CODE,1,3)||'0'	MIDDLE_CODE,
						B.ACCT_ONAME								MIDDLE_NAME,
						''												ACCT_CODE,
						''												ACCT_NAME,
						0												BDGT_ACCT_AMT,
						0												YEBIBI_AMT,
						0												JEN_AMT,
						0												ACCT_MONTH_AMT,
						CASE	WHEN	SUBSTR(A.ACCT_CODE,1,1) = '4'	THEN
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0)) -
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0))
										ELSE
										(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0)) -
										(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0))
										END
								WHEN	SUBSTR(A.ACCT_CODE,1,1) = '2'	THEN	
								CASE	WHEN	B.DRCR_CLASS = 'C'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								ELSE
								CASE	WHEN	B.DRCR_CLASS = 'D'	THEN
										NVL(SUM(DR_ALT_AMT),0) + NVL(SUM(DR_CASH_AMT),0)
										ELSE
										NVL(SUM(CR_ALT_AMT),0) + NVL(SUM(CR_CASH_AMT),0)
										END
								END	ACCT_AMT,
						'2'											GUBUN
			FROM		(SELECT AA.ACCT_DATE 	ACCT_DATE, 
								  AA.ACCT_CLASS 	ACCT_CLASS, 
								  AA.BDGT_YEAR 	BDGT_YEAR, 
								  AA.ACCT_CODE 	ACCT_CODE, 
								  AA.DR_ALT_AMT 	DR_ALT_AMT,
					  			  AA.DR_CASH_AMT 	DR_CASH_AMT,	
								  AA.CR_ALT_AMT 	CR_ALT_AMT, 
								  AA.CR_CASH_AMT 	CR_CASH_AMT
			 			 FROM   FNDB.HFN502H AA, ACDB.HAC001M BB 
						 WHERE  AA.ACCT_CODE = BB.ACCT_CODE AND BB.JG_GUBUN = 'Y') A, ACDB.HAC001M B
			WHERE		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
