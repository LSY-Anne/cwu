 retrieve="SELECT	A.NOTES_NO			GU_NOTES_NO,
			A.BAL_BANK_CODE	GU_BAL_BANK_CODE,
			A.SU_DATE			GU_SU_DATE,
			CASE	WHEN	A.NOTE_ISSUE_DATE BETWEEN :AS_FRSLIPDATE AND :AS_TOSLIPDATE
					THEN	A.NOTES_NO
					ELSE	''
			END	BAL_NOTES_NO,
			CASE	WHEN	A.NOTE_ISSUE_DATE IS NULL
					THEN	A.NOTES_NO
					ELSE	''
			END	MI_NOTES_NO
FROM		FNDB.HFN004M A
WHERE		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
AND		A.NOTES_CLASS = 2
AND		(	(	A.NOTE_ISSUE_DATE BETWEEN :AS_FRSLIPDATE AND :AS_TOSLIPDATE	)
OR			(	A.NOTE_ISSUE_DATE	IS NULL	)	)
ORDER BY A.BAL_BANK_CODE, A.SU_DATE, A.NOTES_NO" arguments=(("ai_acct_class", number),("as_FrSlipDate", string),("as_ToSlipDate", string)) )
