 retrieve="SELECT	'전입금'								COM_HANG_NAME,
			SUBSTR(A.ACCT_CODE,1,4)||'00'	COM_MOK_CODE,
			B.ACCT_NAME							COM_MOK_NAME,
			A.ACCT_CODE							COM_ACCT_CODE,
			C.ACCT_NAME							COM_ACCT_NAME,
			CASE	WHEN	C.DRCR_CLASS = 'D'
					THEN	(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0)) -
							(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0))
					ELSE	(NVL(SUM(A.CR_ALT_AMT),0) + NVL(SUM(A.CR_CASH_AMT),0)) -
							(NVL(SUM(A.DR_ALT_AMT),0) + NVL(SUM(A.DR_CASH_AMT),0))
			END									COM_AMT
FROM		FNDB.HFN502H A, ACDB.HAC001M B, ACDB.HAC001M C
WHERE		SUBSTR(A.ACCT_CODE,1,4)||'00' = B.ACCT_CODE (+)
AND		A.ACCT_CODE = C.ACCT_CODE (+)
AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
AND		A.BDGT_YEAR = SUBSTR(:AS_FRSLIPDATE,1,4)
AND		((A.ACCT_DATE BETWEEN :AS_FRSLIPDATE AND :AS_TOSLIPDATE) OR A.ACCT_DATE = SUBSTR(:AS_FRSLIPDATE,1,4)||'0000')
AND		SUBSTR(A.ACCT_CODE,1,3) = '521'
GROUP BY SUBSTR(A.ACCT_CODE,1,4), B.ACCT_NAME, A.ACCT_CODE, C.ACCT_NAME, C.DRCR_CLASS
