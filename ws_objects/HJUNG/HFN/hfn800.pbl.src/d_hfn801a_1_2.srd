			A.JOB_DATE,
			A.JUMIN_GBN,
			B.CUST_NAME,
			B.JUMIN_NO
FROM		FNDB.HFN007M A, 
			FNDB.HFN603H B
WHERE		A.TAX_CUST_NO 				= B.CUST_NO (+)
