 retrieve="SELECT	A.ACCT_CLASS		ACCT_CLASS,
			A.TAX_NO		TAX_NO,
			A.TAX_TYPE		TAX_TYPE,
			A.TAX_GUBUN		TAX_GUBUN,
			A.TAX_DATE		TAX_DATE,
			A.TAX_CUST_NO		TAX_CUST_NO,
			A.TAX_AMT		TAX_AMT,
			A.TAX_VAT		TAX_VAT,
			A.REMARK		REMARK,
			A.WORKER		WORKER,
			A.IPADDR			IPADDR,
			A.WORK_DATE		WORK_DATE,
			A.JOB_UID		JOB_UID,
			A.JOB_ADD		JOB_ADD,
			A.JOB_DATE		JOB_DATE,
			DECODE(A.JUMIN_GBN,'Y',C.CUST_NAME,B.CUST_NAME)		CUST_NAME,
			DECODE(A.JUMIN_GBN,'Y',C.JUMIN_NO,B.BUSINESS_NO)	BUSINESS_NO,
			A.RESOL_NO		RESOL_NO,
'( ' || to_char(to_date(:AS_FROM_DATE), 'yyyy/mm/dd')  || ' ∼ ' ||  to_char(to_date(:AS_TO_DATE), 'yyyy/mm/dd') || ' )' as title_dt,
(case when  :as_tax_type = '1' then 
		(case when :as_tax_gubun = '1' then '매입계산서 내역' else  '매출계산서 내역' end)
	 else	(case when :as_tax_gubun = '1' then '매입세금계산서 내역' else '매출세금계산서 내역' END) END)  AS TITLE
FROM		FNDB.HFN007M A, 
			STDB.HST001M B,
			FNDB.HFN603H C
WHERE		A.TAX_CUST_NO = B.CUST_NO (+)
AND		A.TAX_CUST_NO = C.CUST_NO (+)
AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
AND		A.TAX_TYPE = :AS_TAX_TYPE
AND		A.TAX_GUBUN = :AS_TAX_GUBUN
AND		A.TAX_DATE BETWEEN :AS_FROM_DATE AND :AS_TO_DATE
ORDER	BY	A.TAX_NO

