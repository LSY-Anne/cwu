 retrieve="SELECT	B.BUSINESS_NO,
			B.CAMPUS_NAME,
			B.PRESIDENT,
			B.ADDRESS,
			B.CONDITION,
			B.CATEGORY,
			A.RPT_FDATE,
			A.RPT_TDATE,
			A.BUSINESS_NO,
			A.SUM_OF_TAX,
			A.NUM_OF_NULL,
			C.CUST_NAME,
			C.UPTAE,
			C.UPJONG,
			A.AMT,
			A.TAX_AMT
FROM		FNDB.HFN602H A, 
			CDDB.KCH000M B, 
			STDB.HST001M C
WHERE		A.BUSINESS_NO 	= C.BUSINESS_NO (+)
AND		A.CUST_NO 		= C.CUST_NO (+)
AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
AND		A.RPT_FDATE 	= :AS_FROM_DATE
AND		A.TAX_CLASS 	= :AS_TAX_CLASS
AND		A.SEQ_NO 		> 10
AND		A.BUSINESS_OPT = '1'
ORDER BY A.SEQ_NO
