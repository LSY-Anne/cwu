 retrieve="SELECT	A.ACCT_CLASS,
			A.TAX_NO,
			A.TAX_TYPE,
			A.TAX_GUBUN,
			A.TAX_DATE,
			A.TAX_CUST_NO,
			A.TAX_AMT,
			A.TAX_VAT,
			A.REMARK,
			DECODE(A.JUMIN_GBN,'Y',C.CUST_NAME,B.CUST_NAME) CUST_NAME
FROM		FNDB.HFN007M A, 
			STDB.HST001M B,
			FNDB.HFN603H C
WHERE		A.TAX_CUST_NO = B.CUST_NO (+)
AND		A.TAX_CUST_NO = C.CUST_NO (+)
AND		DECODE(:AI_ACCT_CLASS,0,0,A.ACCT_CLASS) = :AI_ACCT_CLASS
AND		A.TAX_TYPE = '2'
AND		A.TAX_DATE BETWEEN :AS_FROM_DATE AND :AS_TO_DATE
ORDER	BY	A.TAX_GUBUN, A.TAX_DATE, A.TAX_CUST_NO
" arguments=(("ai_acct_class", number),("as_from_date", string),("as_to_date", string)) )
