 retrieve="  SELECT A.APPLY_GWA,   
         A.APPLY_DATE,   
         D.ITEM_MIDDLE, 
         A.ITEM_NO,  
			A.ITEM_NAME,
			A.MODEL,
			A.GIAN_NUM,
			A.MEASURE,
         A.ITEM_STAND_SIZE,
         A.APPLY_MEMBER_NO,   
         A.REAL_USE_PERSON,   
         A.ACCEPT_GWA,   
         A.ROOM_CODE,   
         A.APPLY_CLASS,   
         A.ACCEPT_DATE,   
         A.ACCT_CODE,
         E.ACCT_NAME,   
         A.SHEET_DATE,   
         A.APPLY_CTRL_GWA,   
         A.APPLY_CTRL_NO,   
         A.USE_YN,   
         A.APPLY_QTY,   
         A.UPDATE_QTY,   
         A.PURCHASE_QTY,   
         A.APPLY_NOTE,   
         A.APPLY_AMT,   
         A.APPLY_PRICE,   
         A.APPLY_USE_DATE,   
         A.ORD_CLASS,   
			A.GOODS_KIND,
         A.WORKER,   
         A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
         A.REQ_NO,     
         B.MIDD_NAME,
         C.ROOM_NAME,
			''  DEL_CHK,
         (A.ASSN_TEMP_AMT + A.ASSN_REAL_AMT) - A.ASSN_USED_AMT    ASSN_USED_AMT   
    FROM STDB.HST105H A,   
         STDB.HST003M B,
         STDB.HST242M C,
         STDB.HST004M D,
         ACDB.HAC001M E
   WHERE  A.ITEM_NO = D.ITEM_NO  AND
          D.ITEM_MIDDLE = B.ITEM_MIDDLE  AND
          A.ROOM_CODE = C.ROOM_CODE(+)  AND
          A.ACCT_CODE = E.ACCT_CODE     AND 
          A.APPLY_DATE >= :as_apply_date_f  AND  
          A.APPLY_DATE <= :as_apply_date_t  AND  
          D.ITEM_MIDDLE like :as_item_no  AND  
          A.APPLY_GWA like :as_gwa		  AND  
          A.ITEM_NAME like :as_item_name  AND  
          A.WORKER 	= :as_uid  AND  
          A.ORD_CLASS like :ai_ord_class||'%'
" update="STDB.HST105H" updatewhere=0 updatekeyinplace=no arguments=(("as_apply_date_f", string),("as_apply_date_t", string),("as_item_no", string),("as_item_name", string),("ai_ord_class", number),("as_gwa", string),("as_uid", string))  sort="apply_date A gian_num A ord_class A " )
