 retrieve="  SELECT A.APPLY_GWA,   
         A.APPLY_DATE,   
         A.ITEM_NO,   
			A.ITEM_NAME,
         A.ITEM_STAND_SIZE,
         A.APPLY_MEMBER_NO,   
         A.REAL_USE_PERSON,   
         A.ACCEPT_GWA,   
         A.ROOM_CODE,   
         A.APPLY_CLASS,   
         A.ACCEPT_DATE,   
         A.ACCT_CODE,   
         A.SHEET_DATE,   
         A.APPLY_CTRL_GWA,   
         A.APPLY_CTRL_NO,   
         A.USE_YN,   
         A.APPLY_QTY,   
         A.UPDATE_QTY,   
         A.PURCHASE_QTY,   
         A.APPLY_NOTE,   
         A.APPLY_AMT,   
         A.APPLY_PRICE,   
         A.APPLY_USE_DATE,   
         A.ORD_CLASS,
         A.REMARK,   
         A.WORKER,   
         A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
         A.ORD_NO,     
         B.MIDD_NAME,
         B.ITEM_MIDDLE,
         D.ROOM_NAME,
         sysdate       
    FROM STDB.HST105H A,   
         STDB.HST003M B,
         STDB.HST004M C,
         STDB.HST242M D  
   WHERE  A.ITEM_NO = C.ITEM_NO 
   AND    C.ITEM_MIDDLE = B.ITEM_MIDDLE 
	AND    A.ROOM_CODE = D.ROOM_CODE(+)
   AND    A.ORD_CLASS = '3'
   AND    A.APPLY_GWA like :as_APPLY_gwa 
   AND    A.APPLY_DATE  BETWEEN  :as_fr_date  AND :as_to_date
   AND    A.ACCT_CODE = :as_acct_code
   AND    A.GIAN_NUM  LIKE :as_gian_num


" arguments=(("as_apply_gwa", string),("as_fr_date", string),("as_to_date", string),("as_acct_code", string),("as_gian_num", string)) )
