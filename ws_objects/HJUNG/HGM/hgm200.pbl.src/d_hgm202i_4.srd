 retrieve="  SELECT A.ID_NO,   
         A.ITEM_CLASS,   
         A.APPLY_DATE,   
         A.APPLY_NO,   
         A.ORD_NO,   
         A.REP_DATE,   
         A.REP_GWA,   
         A.REP_MEMBER_NO,   
         A.ACCEPT_GWA,   
         A.APPLY_AMT,   
         A.REQ_NO,   
         A.ITEM_SEQ,   
         A.REP_NEED,   
         A.ACCT_CODE,   
         A.REP_REMARK,   
         A.REP_AMT,   
         A.REP_FDATE,   
         A.REP_TDATE,   
         A.AUDIT_DATE,
         A.AUDIT_MEMBER_NO,
         A.AUDIT_POSITION,   
         A.REP_SOURCE,   
         A.SIGN_YN,   
         A.REPAIR_YN,   
         A.STAT_CLASS,    
         A.WORKER,   
         A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
         A.ORD_NO,     
         C.ITEM_MIDDLE,
         B.ROOM_CODE,
         B.ITEM_NO,
         B.ITEM_STAND_SIZE,
         B.MODEL,
         B.PURCHASE_QTY,
         D.MIDD_NAME,
         sysdate       
    FROM STDB.HST030H A, 
         STDB.HST027M B,
         STDB.HST004M C,
         STDB.HST003M D
   WHERE  A.ID_NO = B.ID_NO
   AND    A.ITEM_CLASS = B.ITEM_CLASS
   AND    B.ITEM_NO = C.ITEM_NO
   AND    C.ITEM_MIDDLE = D.ITEM_MIDDLE
   AND    A.STAT_CLASS = '3'
   AND    A.REP_GWA like :as_rep_gwa||'%'
   AND    A.APPLY_DATE  between  :as_fr_date and  :as_to_date
   AND    nvl(A.ACCT_CODE,' ') = :as_acct_code
   AND    A.APPLY_NO   like :as_apply_no||'%'
  
  
" arguments=(("as_rep_gwa", string),("as_fr_date", string),("as_to_date", string),("as_acct_code", string),("as_apply_no", string)) )
