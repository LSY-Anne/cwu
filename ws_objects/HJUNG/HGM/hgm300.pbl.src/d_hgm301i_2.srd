         A.APPLY_USE_DATE,   
         A.ORD_CLASS,   
         A.WORKER,   
         A.WORK_DATE, 
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE, 
         A.REQ_NO,
         B.MIDD_NAME,            
		   A.ASSN_USED_AMT  LIM_AMT    
    FROM STDB.HST105H A,   
         STDB.HST003M B,   
         STDB.HST004M E
   WHERE  A.ITEM_NO 		= E.ITEM_NO  AND
