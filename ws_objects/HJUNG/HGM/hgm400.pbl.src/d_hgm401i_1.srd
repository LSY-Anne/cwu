 retrieve="    SELECT A.APPLY_GWA,   
         A.APPLY_DATE,   
         A.ITEM_NO,   
         A.ITEM_NAME,   
			A.ITEM_SEQ,
         B.MIDD_NAME,   
         A.APPLY_MEMBER_NO,   
         A.REAL_USE_PERSON,   
         A.ROOM_CODE,   
         A.ACCEPT_GWA,   
         A.USE_YN,   
         A.APPLY_QTY,   
         A.UPDATE_QTY,   
         A.PURCHASE_QTY,   
         A.APPLY_NOTE,   
         A.APPLY_AMT,   
         A.APPLY_PRICE,   
         A.WORKER,   
         A.WORK_DATE,   
         '' WORK_GBN,
         A.ORD_CLASS,   
         A.MODEL,
			A.item_stand_size,   
         A.APPLY_CLASS,   
         A.ACCT_CODE,   
         A.SHEET_DATE,
         A.REQ_NO,   
         A.REQ_NO||A.ITEM_SEQ     REQ_SEQ,   
         A.APPLY_CTRL_GWA,   
         A.APPLY_CTRL_NO,   
         A.APPLY_USE_DATE,   
         A.ACCEPT_DATE,
			FU_CODE_NM('HAENG','ORD_CLASS',A.ORD_CLASS,'K')		ORD_CLASS_NM
    FROM STDB.HST105H A,  
         STDB.HST003M B,
         STDB.HST004M C
   WHERE  A.ITEM_NO = C.ITEM_NO           AND
          C.ITEM_MIDDLE = B.ITEM_MIDDLE   AND
 			 A.ACCEPT_DATE between  :as_fr_date and  :as_to_date AND
			 A.APPLY_GWA like  :as_apply_gwa||'%'		 AND
          A.ORD_CLASS in (2,6)             AND
			 A.ORD_NO	IS NULL
          
          



" arguments=(("as_fr_date", string),("as_to_date", string),("as_apply_gwa", string))  sort="req_no A item_no A apply_gwa A " )
