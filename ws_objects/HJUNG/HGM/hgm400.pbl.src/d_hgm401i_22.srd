			A.QUOT_QTY,   
			A.ORD_NO,
         A.WORKER,   
         A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,         
         B.ITEM_NO,    
			B.ITEM_NAME,
         NVL(B.UPDATE_QTY,0)  UPDATE_QTY,
         B.ITEM_NO MIDD,
			B.MODEL,
			B.ITEM_STAND_SIZE,
			B.APPLY_GWA,
			B.ACCT_CODE
			
    FROM STDB.HST107H A,   
         STDB.HST105H B
   WHERE  A.REQ_NO = B.REQ_NO       AND   
			 A.ITEM_SEQ	=	B.ITEM_SEQ	AND
