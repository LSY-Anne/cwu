 retrieve="select a.business_no,
       a.cust_name,
       a.president_name,
       (a.addr1||a.addr2)    addr,
       a.uptae,
       a.main_items,
       a.phoneno,
       a.fax,
       b.BUSINESS_NO,
       b.CAMPUS_NAME,
       b.PRESIDENT,
       b.ADDRESS,
       c.item_name,
       (c.model||c.item_stand_size)   model_size,
       c.measure,
       c.update_qty,
       d.quot_price,
       e.ord_amt,
       e.tax_amt,
       d.req_no,
       e.ord_no,
       e.remark
from   stdb.hst001m a,
       cddb.kch000m b,
       stdb.hst105h c,
       stdb.hst107h d,
       stdb.hst106h e
where  c.req_no    = d.req_no
