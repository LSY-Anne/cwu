 retrieve="  SELECT A.ORD_NO,      
         B.REQ_NO,      
         B.ITEM_SEQ,    
         A.QUOT_NUM,    
         A.CUST_NO,     
         A.DEPT_CODE,   
         B.ACCT_CODE,   
         A.ORD_AMT,     
         A.VAT_AMT,     
         A.ORD_DAY,     
         A.ORD_DATE,    
         A.PAY_DATE,    
         A.DEVILERY_DT, 
         A.SIGN_CONDITION,
         A.AUDIT_PERSON,
         A.AUDIT_POSITION,
         A.REMARK,      
         A.ORD_CLASS,
         B.ID_NO,
         B.rep_gwa,
         C.ITEM_NO
  FROM   STDB.HST032H  A,
         STDB.HST030H  B,
         STDB.HST027M  C
  WHERE  A.ORD_NO = B.ORD_NO
  AND    B.ID_NO  = C.ID_NO
  AND    B.ITEM_CLASS = C.ITEM_CLASS
  AND    A.ORD_DATE BETWEEN  :as_fr_date  AND  :as_to_date
  AND    B.REP_GWA    LIKE  :as_gwa
  AND    B.ACCT_CODE  LIKE  :as_acct_code
  AND    A.ORD_CLASS =  6" arguments=(("as_fr_date", string),("as_to_date", string),("as_gwa", string),("as_acct_code", string)) )
