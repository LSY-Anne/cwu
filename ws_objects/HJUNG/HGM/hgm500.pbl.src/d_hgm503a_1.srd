 retrieve="  SELECT A.IN_NO,   
         A.ORD_NO,
         A.GWA,   
			A.REQ_DATE,
         B.ITEM_NAME, 
         A.ITEM_NO,  
         A.IN_DATE,     
         A.IN_QTY,   
         A.IN_PRICE,   
         A.IN_AMT,   
         A.ITEM_CLASS,   
         A.REMARK,   
         A.CUST_NO,
         C.APPLY_GWA,
         C.ACCT_CODE,
			C.APPLY_AMT,
			C.APPLY_NOTE,
         A.REVENUE_OPT,
         A.PROOF_DATE,
         A.ITEM_SEQ,
         A.REQ_NO,
         A.IN_OPT,
         A.WORKER, 
         A.IPADDR,
         A.WORK_DATE,
         A.JOB_UID,
         A.JOB_ADD,
         A.JOB_DATE      
 FROM    STDB.HST109H A,
         STDB.HST004M B,
         STDB.HST105H C
 WHERE   A.ITEM_NO = B.ITEM_NO
 AND     A.ORD_NO  = C.ORD_NO(+)
 AND     A.REQ_NO  = C.REQ_NO(+)
 AND     A.IN_DATE  BETWEEN  :as_fr_date AND :as_to_date
 AND     nvl(C.APPLY_GWA,' ') LIKE :as_gwa
 AND     nvl(C.ACCT_CODE,' ') LIKE :as_acct_code" arguments=(("as_fr_date", string),("as_to_date", string),("as_gwa", string),("as_acct_code", string))  sort="hst109h_ord_no A " )
