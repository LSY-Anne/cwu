 retrieve="  SELECT A.APPLY_GWA,   
         A.APPLY_DATE,   
         D.ITEM_MIDDLE, 
         A.ITEM_NO,  
			A.ITEM_NAME,
			A.MODEL,
			A.GIAN_NUM,
			A.MEASURE,
         A.ITEM_STAND_SIZE,
         A.APPLY_MEMBER_NO,   
         A.ACCEPT_GWA,   
         A.ROOM_CODE,   
         A.APPLY_CLASS,   
         A.ACCEPT_DATE,   
         A.ACCT_CODE,
         E.ACCT_NAME,   
         A.APPLY_QTY,   
         A.UPDATE_QTY,   
         A.PURCHASE_QTY,   
         A.APPLY_NOTE,   
         A.APPLY_AMT,   
         A.APPLY_PRICE,   
         A.APPLY_USE_DATE,   
         FU_CODE_NM('HAENG','ORD_CLASS',A.ORD_CLASS,'K') ORD_CLASS,   
			A.GOODS_KIND,
         A.WORKER,   
         A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
         A.REQ_NO,
			A.ORD_NO,     
         B.MIDD_NAME,
         C.ROOM_NAME,
			''  DEL_CHK
           
    FROM STDB.HST105H A,   
         STDB.HST003M B,
         STDB.HST242M C,
         STDB.HST004M D,
         ACDB.HAC001M E
   WHERE  A.ITEM_NO = D.ITEM_NO  AND
          D.ITEM_MIDDLE = B.ITEM_MIDDLE  AND
          A.ROOM_CODE = C.ROOM_CODE(+)  AND
          A.ACCT_CODE = E.ACCT_CODE(+)  AND 
          A.APPLY_DATE >= :as_fr_date  AND  
          A.APPLY_DATE <= :as_to_date  AND  
          A.APPLY_GWA like :as_gwa||'%'	AND
			 A.ORD_CLASS	= 1
  ORDER BY A.APPLY_DATE

" arguments=(("as_fr_date", string),("as_to_date", string),("as_gwa", string)) )
