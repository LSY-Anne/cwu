			B.WORK_DATE,    
			B.IPADDR,
			B.JOB_UID, 
			B.JOB_ADD,
			B.JOB_DATE
FROM		INDB.HIN001M A,   
			INDB.HIN800M B  
WHERE		A.MEMBER_NO = B.MEMBER_NO
