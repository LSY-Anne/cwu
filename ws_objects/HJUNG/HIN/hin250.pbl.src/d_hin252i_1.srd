 retrieve="SELECT	A.MEMBER_NO,
			A.SEQ_NO,
			A.FROM_DATE,
			A.CHANGE_OPT,
			A.CHANGE_REASON,
			A.CHANGE_CON,
			A.TO_DATE,
			A.GWA,
			A.SOSOK_DATE,
			A.JIKJONG_DATE,
			A.JIKWI_CODE,
			trim(A.DUTY_CODE) DUTY_CODE,
			A.DUTY_DATE,
			A.SAL_YEAR,
			A.SAL_CLASS,
			A.SAL_DATE,
			A.SAL_LEFTMONTH,
			A.JAEIMYONG_START,
			A.JAEIMYONG_END,
			A.RETIRE_DATE,
			A.OLD_CHANGE_OPT,
			A.OLD_GWA,
			A.OLD_FROM_DATE,
			A.OLD_TO_DATE,
			A.JIKMU_CODE,
			A.SIGN_OPT,
			A.WORKER,
			A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
			B.NAME		KNAME,
			B.JUNIM_DATE,
			B.JOGYOSU_DATE,
			B.BUGYOSU_DATE,
			B.GYOSU_DATE,
			FU_CODE_NM('HAENG','CHANGE_OPT',A.CHANGE_OPT,'K')		COM_CHANGE_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')		COM_JIKWI_NM,
			FU_DEPT_NM(A.GWA,'K')											COM_DEPT_NM,
			FU_DUTY_NM(A.DUTY_CODE,'K')									COM_DUTY_NM,
			FU_CODE_NM('HAENG','CHANGE_OPT',A.OLD_CHANGE_OPT,'K')	COM_OLDCHANGE_NM,
			FU_DEPT_NM(A.OLD_GWA,'K')										COM_OLDDEPT_NM,
			FU_CODE_NM('HAENG','JIKMU_CODE',A.JIKMU_CODE,'K')		COM_JIKMU_NM,
			FU_DEPT_NM(A.GWA,'E')											COM_DEPT_ENM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'E')		COM_JIKWI_ENM
FROM		INDB.HIN007H A,
			INDB.HIN001M B,
			INDB.HIN003M C
WHERE		A.MEMBER_NO  = B.MEMBER_NO
AND		A.DUTY_CODE  = C.DUTY_CODE(+)
AND		B.NAME       LIKE :as_KName||'%'
AND		A.SIGN_OPT   LIKE :as_SignOpt||'%'
AND		A.CHANGE_OPT LIKE :as_ChangeOpt||'%'
AND		B.DUTY_CODE  BETWEEN DECODE(:as_JikjongCode,'1','000','2','400','000') 
							 AND     DECODE(:as_JikjongCode,'1','399','2','999','999')
ORDER	BY	A.MEMBER_NO,
			A.FROM_DATE,
			A.TO_DATE,
			A.SEQ_NO" update="INDB.HIN007H" updatewhere=0 updatekeyinplace=no arguments=(("as_KName", string),("as_SignOpt", string),("as_ChangeOpt", string),("as_JikJongCode", string)) )
