 retrieve="SELECT	A.MEMBER_NO,
			A.SEQ_NO,
			A.FROM_DATE,
			A.CHANGE_OPT,
			A.CHANGE_REASON,
			A.CHANGE_CON,
			A.TO_DATE,
			A.GWA,
			A.SOSOK_DATE,
			A.JIKJONG_DATE,
			A.JIKWI_CODE,
			A.DUTY_CODE,
			A.DUTY_DATE,
			A.SAL_CLASS,
			A.SAL_DATE,
			A.SAL_LEFTMONTH,
			A.JAEIMYONG_START,
			A.JAEIMYONG_END,
			A.RETIRE_DATE,
			A.OLD_CHANGE_OPT,
			A.OLD_GWA,
			A.JIKMU_CODE,
			A.SIGN_OPT,
			A.WORKER,
			A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE,
			RTRIM(B.NAME)														COM_KNAME,
			FU_CODE_NM('HAENG','CHANGE_OPT',A.CHANGE_OPT,'K')		COM_CHANGE_OPT_NM,
			FU_DUTY_NM(A.DUTY_CODE,'K')									COM_DUTY_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')		COM_JIKWI_NM,
			FU_CODE_NM('HAENG','JIKMU_CODE',A.JIKMU_CODE,'K')		COM_JIKMU_NM,
			FU_DEPT_NM(A.GWA,'K')											COM_DEPT_NM,
			FU_DEPT_NM(A.OLD_GWA,'K')										COM_OLDDEPT_NM,
			FU_CODE_NM('COMM','SIGN_OPT',A.SIGN_OPT,'K')				COM_SIGN_NM
FROM		INDB.HIN007H A,
			INDB.HIN001M B
WHERE		A.MEMBER_NO  = B.MEMBER_NO
AND		A.SIGN_OPT   > 1
AND		B.NAME       LIKE :as_KName||'%'
AND		A.SIGN_OPT   LIKE :as_SignOpt||'%'
AND		A.CHANGE_OPT LIKE :as_ChangeOpt||'%'
AND		A.FROM_DATE  BETWEEN :as_FrDate AND :as_ToDate
ORDER	BY	A.CHANGE_OPT,
			A.TO_DATE,
			A.MEMBER_NO,
			A.SEQ_NO
" arguments=(("as_KName", string),("as_SignOpt", string),("as_ChangeOpt", string),("as_FrDate", string),("as_ToDate", string)) )
