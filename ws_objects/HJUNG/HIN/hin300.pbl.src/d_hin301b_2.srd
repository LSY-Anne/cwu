 retrieve="SELECT	
 			A.MEMBER_NO,
			0	COM_SEQ_NO,
			A.GWA						DEPT_CODE,
			A.SOSOK_DATE,
			A.JIKJONG_DATE,
			A.JIKWI_CODE,
			B.UP_DUTY_CODE,
			B.SPEND_YEAR,
			A.DUTY_CODE,
			A.DUTY_DATE,
			A.SAL_CLASS,
			A.SAL_DATE,
			A.SAL_LEFTMONTH,
			A.JAEIMYONG_START,
			A.JAEIMYONG_END,
			A.RETIRE_DATE,
			A.OLD_GWA				OLD_DEPT,
			A.JIKMU_CODE,
			A.FIRSTHIRE_DATE,
			A.JOGYOSU_DATE,
			A.BUGYOSU_DATE,
			A.GYOSU_DATE,
			A.NAME					KNAME,
			FU_CODE_NM('HAENG','CHANGE_OPT',21,'K')						COM_CHANGE_OPT_NM,
			FU_DEPT_NM(A.GWA,'K')												COM_DEPT_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')			COM_JIKWI_NM,
			FU_DUTY_NM(LTRIM(TO_CHAR(A.DUTY_CODE - 1,'000')),'K')		COM_DUTY_NM,
			FU_CODE_NM('HAENG','JIKMU_CODE',A.JIKMU_CODE,'K')			COM_JIKMU_NM,
			FU_DEPT_NM(A.OLD_GWA,'K')											COM_OLDDEPT_NM,
			FU_CODE_NM('COMM','SIGN_OPT',1,'K')								COM_SIGN_NM,
			A.SAL_YEAR
FROM		INDB.HIN001M A,
			INDB.HIN024M B,
			
			(SELECT	AA.MEMBER_NO,
					SUM(AA.TOTAL_POINT)/COUNT(AA.TOTAL_POINT) TOTAL_AVG
			FROM	(
					SELECT MEMBER_NO,
							(NVL(EDU_POINT,0)+NVL(RESCH_POINT,0)+NVL(SERVE_POINT,0)+NVL(ETC_POINT,0))/4 TOTAL_POINT
					FROM   INDB.HIN800M
					WHERE  FROM_DATE = SUBSTR(:as_date,1,6)
					)AA
			GROUP BY  AA.MEMBER_NO) C 	
WHERE		A.DUTY_CODE    = B.DUTY_CODE
AND		A.MEMBER_NO 	= C.MEMBER_NO(+)
AND 		C.TOTAL_AVG(+)	>= 70	
AND		A.DUTY_CODE    = '102'
AND		B.SPEND_YEAR   > 0
AND		B.UPGRADE_OPT  = 1
AND		FU_RTN_HWAN_YEAR(NVL(FU_RTN_YEAR_MONTH(A.BUGYOSU_DATE,:as_Date),0)
				+ (NVL(A.SAL_LEFTMONTH,0) * 0.01),100) >= B.SPEND_YEAR
AND		A.NAME         LIKE :as_KName||'%'
AND		NVL(LENGTH(RTRIM(A.GYOSU_DATE)),0) = 0
AND		NOT (NVL(LENGTH(RTRIM(B.UP_DUTY_CODE)),0) = 0)

AND		A.JAEJIK_OPT   = 1
UNION	ALL
SELECT
 			A.MEMBER_NO,
			0	COM_SEQ_NO,
			A.GWA,
			A.SOSOK_DATE,
			A.JIKJONG_DATE,
			A.JIKWI_CODE,
			B.UP_DUTY_CODE,
			B.SPEND_YEAR,
			A.DUTY_CODE,
			A.DUTY_DATE,
			A.SAL_CLASS,
			A.SAL_DATE,
			A.SAL_LEFTMONTH,
			A.JAEIMYONG_START,
			A.JAEIMYONG_END,
			A.RETIRE_DATE,
			A.OLD_GWA,
			A.JIKMU_CODE,
			A.FIRSTHIRE_DATE,
			A.JOGYOSU_DATE,
			A.BUGYOSU_DATE,
			A.GYOSU_DATE,
			A.NAME,
			FU_CODE_NM('HAENG','CHANGE_OPT',21,'K')						COM_CHANGE_OPT_NM,
			FU_DEPT_NM(A.GWA,'K')												COM_DEPT_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')			COM_JIKWI_NM,
			FU_DUTY_NM(LTRIM(TO_CHAR(A.DUTY_CODE - 1,'000')),'K')		COM_DUTY_NM,
			FU_CODE_NM('HAENG','JIKMU_CODE',A.JIKMU_CODE,'K')			COM_JIKMU_NM,
			FU_DEPT_NM(A.OLD_GWA,'K')											COM_OLDDEPT_NM,
			FU_CODE_NM('COMM','SIGN_OPT',1,'K')								COM_SIGN_NM,
			A.SAL_YEAR
FROM		INDB.HIN001M A,
			INDB.HIN024M B,
			
			(SELECT	AA.MEMBER_NO,
					SUM(AA.TOTAL_POINT)/COUNT(AA.TOTAL_POINT) TOTAL_AVG
			FROM	(
					SELECT MEMBER_NO,
							(NVL(EDU_POINT,0)+NVL(RESCH_POINT,0)+NVL(SERVE_POINT,0)+NVL(ETC_POINT,0))/4 TOTAL_POINT
					FROM   INDB.HIN800M
					WHERE  FROM_DATE = SUBSTR(:as_date,1,6)
					)AA
			GROUP BY  AA.MEMBER_NO) C 	
WHERE		A.DUTY_CODE    = B.DUTY_CODE
AND		A.MEMBER_NO 	= C.MEMBER_NO(+)
AND 		C.TOTAL_AVG(+)	>= 70	
AND		A.DUTY_CODE    = '103'
AND		B.SPEND_YEAR   > 0
AND		B.UPGRADE_OPT  = 1
AND		FU_RTN_HWAN_YEAR(NVL(FU_RTN_YEAR_MONTH(A.JOGYOSU_DATE,:as_Date),0)
				+ (NVL(A.SAL_LEFTMONTH,0) * 0.01),100) >= B.SPEND_YEAR
AND		A.NAME         LIKE :as_KName||'%'
AND		NVL(LENGTH(RTRIM(A.BUGYOSU_DATE)),0) = 0
AND		NVL(LENGTH(RTRIM(A.GYOSU_DATE)),0) = 0
AND		NOT (NVL(LENGTH(RTRIM(B.UP_DUTY_CODE)),0) = 0)

AND		A.JAEJIK_OPT   = 1
UNION	ALL
SELECT	
 			A.MEMBER_NO,
			0	COM_SEQ_NO,
			A.GWA,
			A.SOSOK_DATE,
			A.JIKJONG_DATE,
			A.JIKWI_CODE,
			B.UP_DUTY_CODE,
			B.SPEND_YEAR,
			A.DUTY_CODE,
			A.DUTY_DATE,
			A.SAL_CLASS,
			A.SAL_DATE,
			A.SAL_LEFTMONTH,
			A.JAEIMYONG_START,
			A.JAEIMYONG_END,
			A.RETIRE_DATE,
			A.OLD_GWA,
			A.JIKMU_CODE,
			A.FIRSTHIRE_DATE,
			A.JOGYOSU_DATE,
			A.BUGYOSU_DATE,
			A.GYOSU_DATE,
			A.NAME,
			FU_CODE_NM('HAENG','CHANGE_OPT',21,'K')						COM_CHANGE_OPT_NM,
			FU_DEPT_NM(A.GWA,'K')												COM_DEPT_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')			COM_JIKWI_NM,
			FU_DUTY_NM(LTRIM(TO_CHAR(A.DUTY_CODE - 1,'000')),'K')		COM_DUTY_NM,
			FU_CODE_NM('HAENG','JIKMU_CODE',A.JIKMU_CODE,'K')			COM_JIKMU_NM,
			FU_DEPT_NM(A.OLD_GWA,'K')											COM_OLDDEPT_NM,
			FU_CODE_NM('COMM','SIGN_OPT',1,'K')								COM_SIGN_NM,
			A.SAL_YEAR
FROM		INDB.HIN001M A,
			INDB.HIN024M B,
			
			(	SELECT	AA.MEMBER_NO,
							SUM(AA.TOTAL_POINT)/COUNT(AA.TOTAL_POINT) TOTAL_AVG
				FROM		(	SELECT	MEMBER_NO,
											(	NVL(EDU_POINT,0) + NVL(RESCH_POINT,0) + 
												NVL(SERVE_POINT,0) + NVL(ETC_POINT,0) ) / 4 TOTAL_POINT
								FROM		INDB.HIN800M
								WHERE		FROM_DATE = SUBSTR(:as_date,1,6)	)	AA
				GROUP	BY	AA.MEMBER_NO	) C 	
WHERE		A.DUTY_CODE    = B.DUTY_CODE
AND		A.MEMBER_NO 	= C.MEMBER_NO(+)
AND 		C.TOTAL_AVG(+) >= 70	
AND		A.DUTY_CODE    = '104'
AND		B.SPEND_YEAR   > 0
AND		B.UPGRADE_OPT  = 1
AND		FU_RTN_HWAN_YEAR(NVL(FU_RTN_YEAR_MONTH(A.FIRSTHIRE_DATE,:as_Date),0)
				+ (NVL(A.SAL_LEFTMONTH,0) * 0.01),100) >= B.SPEND_YEAR
AND		A.NAME         LIKE :as_KName||'%'
AND		NVL(LENGTH(RTRIM(A.JOGYOSU_DATE)),0) = 0
AND		NVL(LENGTH(RTRIM(A.BUGYOSU_DATE)),0) = 0
AND		NVL(LENGTH(RTRIM(A.GYOSU_DATE)),0) = 0
AND		NOT (NVL(LENGTH(RTRIM(B.UP_DUTY_CODE)),0) = 0)

AND		A.JAEJIK_OPT   = 1
" arguments=(("as_Date", string),("as_KName", string)) )
