 retrieve="SELECT	A.GWA									DEPT_CODE,
			A.NAME								KNAME,
			B.COMMITEE_CODE,
			B.SYYMMDD,
			B.EYYMMDD,
			C.COMMITEE_NAME,
			FU_DUTY_NM(A.DUTY_CODE,'K')	COM_DUTY_NM,
			FU_DEPT_NM(A.GWA,'K')			COM_DEPT_NM,
			FU_CODE_NM('HAENG','COMMITEE_OPT',B.COMMITEE_OPT,'K')	COM_BIGO_NM
FROM		INDB.HIN001M A,
			INDB.HIN030H B,
			INDB.HIN029M C
WHERE		A.MEMBER_NO     = B.MEMBER_NO
AND		B.COMMITEE_CODE = C.COMMITEE_CODE
AND		B.COMMITEE_CODE LIKE :as_CommiteeCode||'%'
AND		SUBSTR(B.SYYMMDD,1,4) = :AS_YEAR
ORDER	BY	B.COMMITEE_CODE,
			B.COMMITEE_OPT,
			A.DUTY_CODE,
			B.MEMBER_NO" arguments=(("as_CommiteeCode", string),("AS_YEAR", string)) )
