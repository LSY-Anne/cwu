 retrieve="SELECT	A.GWA		DEPT_CODE,
			A.DUTY_CODE,
			A.MEMBER_NO,
			FU_DUTY_NM(A.DUTY_CODE,'K')														COM_DUTY_NM,
			FU_DEPT_NM(A.GWA,'K')																COM_DEPT_NM,
			FU_CODE_NM('HAENG','JIKJONG_CODE',SUBSTR(A.DUTY_CODE,1,1),'K')			COM_JIKJONG_NM,
			FU_CODE_NM('HAENG','JIKWI_CODE',A.JIKWI_CODE,'K')							COM_JIKWI_NM,
			FU_BOJIK_NM(A.BOJIK_CODE1,'K')													COM_BOJIK_NM,
			A.NAME			KNAME,
			FU_CODE_NM('HAENG','SEX_CODE',A.SEX_CODE,'K')								COM_SEX_NM,
			A.JUMIN_NO,
			A.FIRSTHIRE_DATE,
			A.JAEIMYONG_END,
			E.HOME_POSTNO,
			RTRIM(E.HOME_ADDR1||' '||E.HOME_ADDR2)	COM_HOME_ADDR,
			B.ENT_ZIP,
			B.ENT_ADDRESS,
			B.TEL_PHONE,
			B.ENT_NAME,
			B.ENT_BUSINESS,
			B.ENT_PRESIDENT,
			C.USE_OPT,
			C.PRINT_NUM,
			NVL(D.COM_AMT011,0)	COM_AMT011,
			NVL(D.COM_AMT012,0)	COM_AMT012,
			NVL(D.COM_AMT021,0)	COM_AMT021,
			NVL(D.COM_AMT022,0)	COM_AMT022,
			NVL(D.COM_AMT031,0)	COM_AMT031,
			NVL(D.COM_AMT032,0)	COM_AMT032,
			NVL(D.COM_AMT041,0)	COM_AMT041,
			NVL(D.COM_AMT042,0)	COM_AMT042,
			NVL(D.COM_AMT051,0)	COM_AMT051,
			NVL(D.COM_AMT052,0)	COM_AMT052,
			NVL(D.COM_AMT061,0)	COM_AMT061,
			NVL(D.COM_AMT062,0)	COM_AMT062,
			NVL(D.COM_AMT071,0)	COM_AMT071,
			NVL(D.COM_AMT072,0)	COM_AMT072,
			NVL(D.COM_AMT081,0)	COM_AMT081,
			NVL(D.COM_AMT082,0)	COM_AMT082,
			NVL(D.COM_AMT091,0)	COM_AMT091,
			NVL(D.COM_AMT092,0)	COM_AMT092,
			NVL(D.COM_AMT101,0)	COM_AMT101,
			NVL(D.COM_AMT102,0)	COM_AMT102,
			NVL(D.COM_AMT111,0)	COM_AMT111,
			NVL(D.COM_AMT112,0)	COM_AMT112,
			NVL(D.COM_AMT121,0)	COM_AMT121,
			NVL(D.COM_AMT122,0)	COM_AMT122
FROM		INDB.HIN001M A,
			CDDB.KCH007M B,
			INDB.HIN013H C,
			INDB.HIN011M E,
			(	SELECT	A.MEMBER_NO,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'01',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT011,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'01',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT012,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'02',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT021,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'02',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT022,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'03',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT031,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'03',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT032,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'04',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT041,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'04',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT042,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'05',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT051,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'05',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT052,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'06',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT061,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'06',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT062,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'07',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT071,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'07',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT072,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'08',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT081,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'08',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT082,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'09',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT091,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'09',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT092,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'10',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT101,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'10',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT102,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'11',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT111,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'11',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT112,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'12',DECODE(SIGN(A.CODE - 51),-1,A.PAY_AMT,0),0))	COM_AMT121,
							SUM(DECODE(SUBSTR(A.YEAR_MONTH,5,2),'12',DECODE(SIGN(A.CODE - 50), 1,A.PAY_AMT,0),0))	COM_AMT122
				FROM		PADB.HPA005D A
				WHERE		A.MEMBER_NO = :as_MemberNo
				AND		A.YEAR_MONTH LIKE SUBSTR(:as_ApplyDate,1,4)||'%'
				AND		A.CODE       < '53'
				GROUP	BY	A.MEMBER_NO	) D
WHERE		A.MEMBER_NO  = :as_MemberNo
AND		B.ENT_CLASS  = '1'
AND		A.MEMBER_NO  = C.MEMBER_NO
AND		C.APPLY_DATE = :as_ApplyDate
AND		C.MEMBER_OPT = :ai_MemberOpt
AND		C.APPLY_NO   = :ai_ApplyNo
AND		C.PRINT_OPT  = :ai_PrintOpt
AND		A.MEMBER_NO  = D.MEMBER_NO(+)
AND		A.MEMBER_NO  = E.MEMBER_NO(+)
ORDER	BY	A.GWA,
			A.DUTY_CODE,
			A.MEMBER_NO
" arguments=(("as_MemberNo", string),("as_ApplyDate", string),("ai_MemberOpt", number),("ai_ApplyNo", number),("ai_PrintOpt", number)) )
