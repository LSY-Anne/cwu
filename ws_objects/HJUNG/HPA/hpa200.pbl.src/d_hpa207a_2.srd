 retrieve="  SELECT PADB.HPA001M.MEMBER_NO,   
         PADB.HPA001M.YEAR_MONTH,   
         rtrim(PADB.HPA001M.NAME) NAME,   
         PADB.HPA001M.JUMIN_NO,   
         PADB.HPA001M.GWA,   
         PADB.HPA001M.JIKJONG_CODE,   
         PADB.HPA001M.JIKWI_CODE,   
         PADB.HPA001M.DUTY_CODE,   
         PADB.HPA001M.SAL_CLASS,   
         PADB.HPA001M.JIKMU_CODE,   
         PADB.HPA001M.BOJIK_CODE,   
         PADB.HPA001M.SALARY,   
         PADB.HPA001M.WORK_YEAR,   
         PADB.HPA001M.JAEJIK_OPT,   
         PADB.HPA001M.FIRST_DATE,   
         nvl(PADB.HPA001M.RETIRE_DATE, ' ') retire_date,   
         PADB.HPA001M.ANN_OPT,   
         PADB.HPA001M.NUM_OF_PAYWORK,   
         PADB.HPA001M.NUM_OF_BONWORK,   
         PADB.HPA001M.NUM_OF_HEALTHWORK,   
         PADB.HPA001M.NUM_OF_ABS,   
         PADB.HPA001M.NUM_OF_LONG,   
         PADB.HPA001M.WIFE_NUM,  
         PADB.HPA001M.FAMILY_NUM,  
         PADB.HPA001M.SUPPORT_20,   
         PADB.HPA001M.SUPPORT_60,   
         PADB.HPA001M.HANDYCAP,   
         PADB.HPA001M.OLD_NUM,   
         PADB.HPA001M.HAKBI1_YN,   
         PADB.HPA001M.HAKBI2_YN,   
         PADB.HPA001M.HAKBI3_YN,   
         PADB.HPA001M.IPHAK_AMT,   
         PADB.HPA001M.HAKGY_AMT,   
         PADB.HPA001M.GISUNG_AMT,   
         PADB.HPA001M.OVERTIME_NUM,   
         PADB.HPA001M.WOMAN,   
         PADB.HPA001M.ETC_SAVING,   
         PADB.HPA001M.ETC_GONGJE,   
         PADB.HPA001M.CONTRIBUTION,   
         PADB.HPA001M.SANGJO_OPT,   
         PADB.HPA001M.UNION_OPT,   
         PADB.HPA001M.PAY_OPT,   
         PADB.HPA001M.OLD_GWA,   
			PADB.HPA001M.LIVE_CNT,
			PADB.HPA001M.LICENSE_OPT,
			PADB.HPA001M.CHANGE_OPT,
         PADB.HPA001M.WORKER,   
         PADB.HPA001M.WORK_DATE,   
         PADB.HPA001M.IPADDR,   
         nvl(CDDB.KCH003M.FNAME, ' ') fname,
         nvl(INDB.V_HIN003M_1.FNAME, ' ') jikjong_fname,
         nvl(INDB.V_HIN003M_1.DUTY_NAME, ' ') duty_name,
         PADB.HPA001M.JOB_UID,   
         PADB.HPA001M.JOB_ADD,
         PADB.HPA001M.JOB_DATE,
			SUBSTR(PADB.HPA003M.PAY_MONTH, TO_NUMBER(SUBSTR(:as_yearmonth, 5, 2)), 1) as sang_gbn,
			INDB.HIN001M.CAREER_YM,
			PADB.HPA001M.FOOD_NONTAX_AMT,
			PADB.HPA001M.YOUNGU_NONTAX_AMT,
			PADB.HPA001M.YOUNGUBOJO_NONTAX_AMT,
			PADB.HPA001M.JIKGUBBOJO_NONTAX_AMT,
         PADB.HPA001M.CONFIRM_GBN
    FROM PADB.HPA001M,   
         CDDB.KCH003M,   
         INDB.V_HIN003M_1,
			PADB.HPA003M,
			INDB.HIN001M  
   WHERE ( rtrim(padb.hpa001m.duty_code) = rtrim(indb.V_HIN003M_1.duty_code (+))) and  
         ( rtrim(padb.hpa001m.gwa) = rtrim(cddb.kch003m.gwa (+))) and  
			( padb.hpa001m.member_no = indb.hin001m.member_no ) and
         ( ( PADB.HPA001M.YEAR_MONTH = :as_yearmonth ) AND  
         ( PADB.HPA001M.GWA like :as_gwa || '%' ) ) AND
			( PADB.HPA003M.CODE	=	'03' )
ORDER BY PADB.HPA001M.YEAR_MONTH ASC,   
         PADB.HPA001M.GWA ASC,   
         PADB.HPA001M.DUTY_CODE ASC,   
         PADB.HPA001M.MEMBER_NO ASC   
" arguments=(("as_yearmonth", string),("as_gwa", string)) )
