 retrieve="  SELECT A.MEMBER_NO,   
         A.YEAR_MONTH,
         A.CHASU,   
         A.PAY_AMT,   
         A.NONTAX_AMT,   
         A.SORT,   
         A.CONTENTS,   
         A.REMARK,   
         A.WORKER,   
         A.WORK_DATE,   
         A.IPADDR,   
         B.GWA,   
         B.DUTY_CODE,   
         C.ITEM_NAME,   
         A.CODE,   
         B.NAME,   
         A.PAY_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE,   
         D.DUTY_NAME,   
         B.CONFIRM_GBN,   
         0 isrowmodified,   
         A.EXCEPTE_GBN  
    FROM PADB.HPA005D A,   
         PADB.HPA003M C,   
         PADB.HPA001M B,   
         INDB.HIN003M_02V  D
   WHERE ( A.member_no 		 = 	B.member_no (+)) and  
         ( A.year_month 	 = 	B.year_month (+)) and  
         ( B.duty_code 		 = 	D.duty_code (+)) and  
         ( A.CODE 			 = 	C.CODE ) and  
         ( ( A.YEAR_MONTH 	 = 	:as_yearmonth ) AND  
         ( A.CODE 			 = 	:as_code  ) AND  
         ( B.JIKJONG_CODE >= 	:ai_str_jikjong ) AND  
         ( B.JIKJONG_CODE <= 	:ai_end_jikjong ) )   
ORDER BY A.YEAR_MONTH ASC,   
         B.DUTY_CODE ASC,   
         A.MEMBER_NO ASC,   
         A.CODE ASC   
" update="PADB.HPA005D" updatewhere=0 updatekeyinplace=yes arguments=(("as_yearmonth", string),("as_code", string),("ai_str_jikjong", number),("ai_end_jikjong", number)) )
