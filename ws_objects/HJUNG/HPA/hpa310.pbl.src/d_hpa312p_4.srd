 retrieve="select  	b.member_no,  
			b.year_month,
        	b.name, 
			FU_DUTY_NM(b.duty_code, 'K') DUTY_NAME,
			FU_DEPT_NM(B.GWA,'K') GWA_NAME,
			A.PAY_AMT,
			C.PAY_AMT,
			A.PAY_AMT - C.PAY_AMT CHAIN,
			FU_CODE_NM('comm','bank_code',D.BANK_CODE,'K') BANK_NM,
			B.SAL_CLASS,
			FU_CODE_NM('comm','JIKWI_CODE',B.JIKWI_CODE,'K') JIKJONG,
			B.JIKJONG_CODE,
			D.ACCT_NO,
			G.GONJI
from		padb.hpa001m b, PADB.HPA005SUM_VIEW A, PADB.HPA005SUM_VIEW C, PADB.HPA020M D, PADB.HPA202M G, PADB.HPA021M H
where		b.year_month	= :as_yearmonth
and		b.member_no		=	:as_member
AND		A.MEMBER_NO		=	B.MEMBER_NO
AND		A.YEAR_MONTH	=	B.YEAR_MONTH
AND		A.CODE			=	'AA'
AND		A.CHASU			=	'5'
AND		C.MEMBER_NO		=	B.MEMBER_NO
AND		C.YEAR_MONTH	=	B.YEAR_MONTH
AND		C.CODE			=	'BB'
AND		C.CHASU			=	'5'
AND		B.MEMBER_NO		=	D.MEMBER_NO (+)
AND		D.PAY_CLASS		IN (0,1)
AND		A.YEAR_MONTH		=	G.YEAR_MONTH (+)
AND		A.CHASU				=	G.CHASU (+)
AND		A.MEMBER_NO = H.MEMBER_NO
AND		A.YEAR_MONTH = H.YEAR_MONTH
AND		A.CHASU = H.CHASU
AND		H.CONFIRM_GBN = 9
" arguments=(("as_yearmonth", string),("as_member", string)) )
