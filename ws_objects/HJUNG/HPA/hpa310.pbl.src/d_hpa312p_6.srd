         sum(decode(a.code,'23',pay_amt,0)) yeungubojo, 
         sum(decode(a.code,'25',pay_amt,0)) hengjeongyeungu
from		padb.hpa005d a, padb.hpa001m b, indb.v_hin003m_1 c, cddb.kch003m d, padb.hpa020m e, cddb.kch001m f
where		a.year_month	= :as_yearmonth
