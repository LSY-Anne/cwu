and		b.jikjong_code	>=	:ai_str_jikjong
and		b.jikjong_code	<=	:ai_end_jikjong
and		a.member_no		>=	:as_str_member
and		a.member_no		<=	:as_end_member
and		b.duty_code	=	c.duty_code
and      a.member_no     =       e.member_no
and      e.pay_class    in ('0','1')
and		a.member_no				=	b.member_no
and		a.year_month			=	b.year_month
and      b.gwa                =  d.gwa
and      f.type               = 'bank_code'
and      f.code               = e.bank_code
and      a.chasu              = '3'
group by a.member_no, b.name, a.pay_date, f.fname,d.fname, e.acct_no, a.year_month, b.duty_code, c.duty_name
