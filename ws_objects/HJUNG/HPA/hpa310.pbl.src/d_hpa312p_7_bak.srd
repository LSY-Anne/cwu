 retrieve="select  a.member_no, a.pay_date, a.year_month, d.fname, 
        b.name, b.duty_code, c.duty_name, f.fname, e.acct_no,
        sum(decode(a.code,'00',pay_amt,0)) base, 
        sum(decode(a.code,'15',pay_amt,0)) bojik,
        sum(decode(a.code,'27',pay_amt,0)) drive,
        sum(decode(a.code,'02',pay_amt,0)) family,
        sum(decode(a.code,'03',pay_amt,0)) sang,
        sum(decode(a.code,'26',pay_amt,0)) skill,
        sum(decode(a.code,'19',pay_amt,0)) jikgub,
        sum(decode(a.code,'01',pay_amt,0)) gunsok, 
        sum(decode(a.code,'18',pay_amt,0)) culnab,
        sum(decode(a.code,'13',pay_amt,0)) hakbi,
        sum(decode(a.code,'21',pay_amt,0)) traffic,
        sum(decode(a.code,'08',pay_amt,0)) hangjeng,
        sum(decode(a.code,'09',pay_amt,0)) haksa,
        sum(decode(a.code,'17',pay_amt,0)) food,
        sum(decode(a.code,'29',pay_amt,0)) timeover,
        sum(decode(a.code,'29',pay_amt,0)) study,
        sum(decode(a.code,'51',pay_amt,0)) gabgun,
        sum(decode(a.code,'52',pay_amt,0)) jumin,
        sum(decode(a.code,'54',pay_amt,0)) medical_insure,
        sum(decode(a.code,'73',pay_amt,0)) chungsa,
        sum(decode(a.code,'56',pay_amt,0)) sanghwan,
        sum(decode(a.code,'77',pay_amt,0)) sujae,
        sum(decode(a.code,'71',pay_amt,0)) gookmin,
        sum(decode(a.code,'55',pay_amt,0)) gyowon,
        sum(decode(a.code,'74',pay_amt,0)) tennis,
        sum(decode(a.code,'53',pay_amt,0)) sahak,
        sum(decode(a.code,'72',pay_amt,0)) telephone,
        sum(decode(a.code,'59',pay_amt,0)) sangjo,
        sum(decode(a.code,'29',pay_amt,0)) jeongi
from		padb.hpa005d a, padb.hpa001m b, indb.v_hin003m_1 c, cddb.kch003m d, padb.hpa020m e, cddb.kch001m f
where		a.year_month	= :as_yearmonth
and		b.gwa		like	:as_gwa || '%'
and		b.jikjong_code	>=	:ai_str_jikjong
and		b.jikjong_code	<=	:ai_end_jikjong
and		a.member_no		>=	:as_str_member
and		a.member_no		<=	:as_end_member
and		b.duty_code	=	c.duty_code
and      a.member_no     =       e.member_no
and      e.pay_class    in ('0','1')
and		a.member_no				=	b.member_no
and		a.year_month			=	b.year_month
and      b.gwa                =  d.gwa
and      f.type               = 'bank_code'
and      f.code               = e.bank_code
and      a.chasu              = '5'
group by a.member_no, b.name, a.pay_date, f.fname,d.fname, e.acct_no, a.year_month, b.duty_code, c.duty_name
order by a.member_no" arguments=(("as_yearmonth", string),("as_gwa", string),("ai_str_jikjong", number),("ai_end_jikjong", number),("as_str_member", string),("as_end_member", string)) )
