 retrieve="
SELECT SUBSTR(b.YEAR_MONTH,1,4) YEAR,
		 SUBSTR(b.YEAR_MONTH,1,4) + 1 YEAR1,
		 A.NAME, 
		 A.JUMIN_NO,
		 C.HOME_ADDR1||' '||C.HOME_ADDR2 ADDR,
		 D.PRESIDENT,
		 D.CAMPUS_NAME,
		 D.BUSINESS_NO,
		 D.ADDRESS,
		 D.CORP_NO,
		 D.TEL_PHONE,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'01',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'01',B.PAY_AMT))) A01,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'01',B.PAY_AMT))) B01,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'02',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'02',B.PAY_AMT))) A02,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'02',B.PAY_AMT))) B02,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'03',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'03',B.PAY_AMT))) A03,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'03',B.PAY_AMT))) B03,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'04',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'04',B.PAY_AMT))) A04,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'04',B.PAY_AMT))) B04,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'05',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'05',B.PAY_AMT))) A05,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'05',B.PAY_AMT))) B05,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'06',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'06',B.PAY_AMT))) A06,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'06',B.PAY_AMT))) B06,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'07',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'07',B.PAY_AMT))) A07,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'07',B.PAY_AMT))) B07,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'08',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'08',B.PAY_AMT))) A08,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'08',B.PAY_AMT))) B08,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'09',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'09',B.PAY_AMT))) A09,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'09',B.PAY_AMT))) B09,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'10',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'10',B.PAY_AMT))) A10,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'10',B.PAY_AMT))) B10,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'11',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'11',B.PAY_AMT))) A11,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'11',B.PAY_AMT))) B11,
		 SUM(DECODE(B.CODE,'00',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'12',B.PAY_AMT),'99',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'12',B.PAY_AMT))) A12,
		 SUM(DECODE(B.CODE,'51',DECODE(SUBSTR(B.YEAR_MONTH,5,2),'12',B.PAY_AMT))) B12,
		 SUM(DECODE(B.CODE,'00',B.PAY_AMT,'99',B.PAY_AMT))	TOT_AMT,
		 SUM(DECODE(B.CODE,'51',B.PAY_AMT))	TAX_AMT,
		'' as purpose, '' as customer, 0 as qty,
		'' as tdate, '' as apply,
		 SYSDATE
 FROM  PADB.HPA005V B, INDB.HIN001M A, INDB.HIN011M C, CDDB.KCH000M D
 WHERE B.YEAR_MONTH LIKE :as_yearmonth||'%'
   AND A.MEMBER_NO = B.MEMBER_NO
   AND A.MEMBER_NO = C.MEMBER_NO (+)
   AND A.NAME LIKE :as_name||'%'
 GROUP BY SUBSTR(b.YEAR_MONTH,1,4), B.MEMBER_NO, A.NAME, A.JUMIN_NO, C.HOME_ADDR1, C.HOME_ADDR2,
		 D.PRESIDENT,
		 D.CAMPUS_NAME,
		 D.BUSINESS_NO,
		 D.ADDRESS,
		 D.CORP_NO,
		 D.TEL_PHONE
" arguments=(("as_yearmonth", string),("as_name", string)) )
