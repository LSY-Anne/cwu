    C.NAME,   
	A.YEAR,
	A.SEQ_NO,   
	A.APPLY_DATE,   
	A.BUSINESS_NO,   
	A.BUSINESS_NAME,   
	A.PAY_TOTAL,   
	A.SUPPLY_COUNT,   
	A.GWANGAE_CODE,   
	A.JUMIN_NO,   
	A.HANDICAP_OLD_GUN,
	A.FOREIGNER_GBN,
	A.SELF_OLD_GBN,
	A.CARD_CNT,
	A.CARD_AMT,
	A.CASH_CNT,
	A.CASH_AMT,
	A.WORKER,   
	A.IPADDR,   
	A.WORK_DATE,   
	A.JOB_UID,   
	A.JOB_ADD,   
	A.JOB_DATE
	 
FROM PADB.HPA023M  A,
	  PADB.HPA019H  B,
	  INDB.HIN001M	 C
WHERE  A.MEMBER_NO	= B.MEMBER_NO
AND    A.MEMBER_NO	= C.MEMBER_NO(+)
AND 	A.YEAR  =   B.YEAR
AND	A.YEAR	=	:AS_YEAR
AND	A.MEMBER_NO 	LIKE  :AS_MEMBER_NO||'%' 
order by A.MEMBER_NO
" arguments=(("as_year", string),("as_member_no", string)) )
