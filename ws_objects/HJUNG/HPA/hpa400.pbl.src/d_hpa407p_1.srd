 column=(type=char(4) updatewhereclause=yes name=hin001m_gwa dbname="hin001m.gwa" )
 column=(type=decimal(0) updatewhereclause=yes name=hin001m_jikwi_code dbname="hin001m.jikwi_code" )
 column=(type=decimal(0) updatewhereclause=yes name=hin001m_ann_opt dbname="hin001m.ann_opt" )
 column=(type=char(3) updatewhereclause=yes name=hin001m_duty_code dbname="hin001m.duty_code" )
 column=(type=char(3) updatewhereclause=yes name=hin001m_sal_class dbname="hin001m.sal_class" )
 column=(type=decimal(0) updatewhereclause=yes name=hin001m_jikmu_code dbname="hin001m.jikmu_code" )
 column=(type=char(4) updatewhereclause=yes name=hin001m_bojik_code1 dbname="hin001m.bojik_code1" )
 column=(type=char(8) updatewhereclause=yes name=hin001m_firsthire_date dbname="hin001m.firsthire_date" )
 column=(type=char(8) updatewhereclause=yes name=hin001m_retire_date dbname="hin001m.retire_date" )
 column=(type=char(100) updatewhereclause=yes name=home_addr1 dbname="home_addr1" )
 column=(type=char(80) updatewhereclause=yes name=home_addr2 dbname="home_addr2" )
 column=(type=char(40) updatewhereclause=yes name=campus_campus_name dbname="campus_name" )
 column=(type=char(10) updatewhereclause=yes name=campus_business_no dbname="hin001m.business_no" )
 column=(type=char(120) updatewhereclause=yes name=campus_address dbname="hin001m.address" )
 column=(type=char(16) updatewhereclause=yes name=campus_tel_phone dbname="hin001m.tel_phone" )
 retrieve="	select	b.member_no, rtrim(b.name) as name, b.jumin_no, b.gwa, b.jikwi_code, b.ann_opt,
				b.duty_code, b.sal_class, b.jikmu_code, b.bojik_code1, b.firsthire_date, b.retire_date,
				nvl(f.home_addr1, ' ') as home_addr1, nvl(f.home_addr2, ' ') as home_addr2,
				trim(e.campus_name) as campus_name, e.business_no, e.address, e.tel_phone
	from		indb.hin001m b, indb.hin003m d, CDDB.KCH000M e, indb.hin011m f
	where		b.gwa		like	:as_gwa || '%'
	and		b.member_no		>=		:as_str_member
	and		b.member_no		<=		:as_end_member
	and		b.duty_code		=		d.duty_code
	and		b.duty_code		like	'1%'
	and		b.member_no		=		f.member_no
	and		e.campus_code	=		(	select	min(campus_code)
												from		CDDB.KCH000M	)
	and		(substr(b.firsthire_date, 1, 4) <=	:as_year
	and		(b.retire_date is null or substr(b.retire_date, 1, 4) >= :as_year))
	and		b.member_no		in		
				(	select	member_no
					from	(	select	member_no, jumin_no
								from		indb.hin001m a
								where		(substr(firsthire_date, 1, 4) <=	:as_year
								and		(retire_date is null or substr(retire_date, 1, 4) >= :as_year))
								order by	jaejik_opt, 
											decode(substr(duty_code, 1, 1), '3', 'z' || duty_code, '0' || duty_code),
											firsthire_date	DESC,
											decode(substr(member_no, 1, 1), '0', 'z' || member_no, '0' || member_no) )
					where		jumin_no	=	b.jumin_no
					and		rownum	=	1	)
	order by b.member_no" arguments=(("as_year", string),("as_gwa", string),("as_str_member", string),("as_end_member", string)) )
