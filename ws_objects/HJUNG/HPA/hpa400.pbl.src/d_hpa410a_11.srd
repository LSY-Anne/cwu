		C.NAME,   
		A.YEAR,
		A.BUSINESS_NO,   
		A.BUSINESS_NAME,   
		A.GIBO_CODE,
		A.SEQ_NO,
		A.APPLY_MONTH,
		A.GIBO_COUNT,
		A.GIBO_AMT,
		A.GIBO_NO,
		A.NAME,
		A.JUMIN_NO,
		A.GWANGAE_CODE,
		A.FOREIGNER_GBN, 
		A.WORKER,   
		A.IPADDR,   
		A.WORK_DATE,   
		A.JOB_UID,   
		A.JOB_ADD,   
		A.JOB_DATE
FROM 	PADB.HPA025M  A,
	  	PADB.HPA019H  B,
	  	INDB.HIN001M  C
WHERE A.MEMBER_NO	=  B.MEMBER_NO
AND	A.MEMBER_NO	=  C.MEMBER_NO
AND	A.YEAR		=	B.YEAR
AND	A.YEAR		=	:AS_YEAR
AND	A.MEMBER_NO	like :AS_MEMBER_NO||'%'
ORDER BY A.SEQ_NO" update="PADB.HPA025M" updatewhere=0 updatekeyinplace=no arguments=(("AS_YEAR", string),("AS_MEMBER_NO", string)) )
