	C.NAME,   
	A.YEAR,
	A.BUSINESS_NO,   
	A.BUSINESS_NAME,   
	A.GIBO_CODE,
	A.SEQ_NO,
	A.APPLY_MONTH,
	A.GIBO_COUNT,
	A.GIBO_AMT,   
	A.WORKER,   
	A.IPADDR,   
	A.WORK_DATE,   
	A.JOB_UID,   
	A.JOB_ADD,   
	A.JOB_DATE
	 
FROM PADB.HPA025M  A,
	  PADB.HPA019H  B,
	  INDB.HIN001M	 C
WHERE  A.MEMBER_NO	= B.MEMBER_NO
AND    A.MEMBER_NO	= C.MEMBER_NO(+)
AND 	A.YEAR  =   B.YEAR
AND	A.YEAR	=	:AS_YEAR
AND	A.MEMBER_NO 	LIKE  :AS_MEMBER_NO||'%' 
ORDER BY A.MEMBER_NO

