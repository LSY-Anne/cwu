(SELECT FNAME FROM CDDB.KCH102D
 WHERE UPPER(CODE_GB) = 'GIBO_OPT' AND CODE = A.P45COD) 
 AS P45CODNM,
A.P45COD,
A.P45DTL,
A.P45YRM,
A.P45BNM,
A.P45BNO,
A.P45PTL,
A.P45RLS,
A.P45CHM, 
A.P45RNO
FROM 
(
SELECT A.*, ROW_NUMBER() OVER (ORDER BY P45RLS ASC, P45RNO ASC , P45COD ASC) AS ROWNUMBER  
FROM PADB.HPAP45T A
WHERE A.P45YAR = :AS_YEAR
