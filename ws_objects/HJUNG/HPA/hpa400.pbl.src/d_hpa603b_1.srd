 retrieve="select	a.member_no, a.child_num, a.support_20_num, a.support_60_num, a.wife_num,
			a.insure_medical_amt, a.old_num, a.handicap_num, a.woman_gbn, a.insure_amt,
			a.medical_gen_amt, a.medical_old_amt, a.medical_handicap_amt, a.medical_amt,
			a.insure_pension_amt, a.insure_employee_amt, a.insure_handicap_amt,
			a.insure_life_amt1, a.insure_life_amt2, a.education_amt, a.house_amt, 
			a.card_used_amt, a.constribution_amt, a.pension_indi_amt, a.pension_amt,
			a.investment_20_amt, a.investment_30_amt,
			a.constribution_legal_amt, a.constribution_gen_amt,
			a.foreign_income, a.house_income_amt, a.fund_amt1, a.fund_amt2,
			a.house_in_amt, a.house_repay_amt,
			rtrim(b.name) name, b.gwa, b.duty_code, b.sal_class,
			b.firsthire_date, b.ann_opt,
			c.jikjong_code, c.jikjong_fname, c.duty_name as duty_name, d.fname as dept_name,
			b.jaejik_opt, e.fname, b.jumin_no, b.retire_date
from		padb.hpa019h a, indb.hin001m b, indb.hin003m_02v c, cddb.kch003m d, cddb.kch001m e
where		a.year			=		:as_year
and		b.gwa		like	:as_gwa || '%'
and		a.member_no		=		b.member_no (+)
and		trim(b.duty_code)		=		trim(c.duty_code(+))
and		b.gwa				=		d.gwa (+)
and		a.member_no		>=		:as_str_member
and		a.member_no		<=		:as_end_member
and		c.jikjong_code >=	   :ai_fjikjong
and	   c.jikjong_code <=	   :ai_tjikjong
and		b.jaejik_opt	=		:as_jaejik_opt
and		e.type			=		'jaejik_opt'
and		b.jaejik_opt	=		e.code
and		a.member_no		in
			(	select	member_no
				from	(	select	member_no, jumin_no
							from		indb.hin001m a
							where		a.jaejik_opt	=	:as_jaejik_opt
							order by	jaejik_opt,
										decode(substr(duty_code, 1, 1), '3', 'z' || duty_code, '0' || duty_code),
										firsthire_date	DESC,
										decode(substr(member_no, 1, 1), '0', 'z' || member_no, '0' || member_no) )
				where		jumin_no	=	b.jumin_no
				and		rownum	=	1	)
order by a.member_no" arguments=(("as_year", string),("as_gwa", string),("ai_fjikjong", number),("ai_tjikjong", number),("as_str_member", string),("as_end_member", string),("as_jaejik_opt", string)) )
