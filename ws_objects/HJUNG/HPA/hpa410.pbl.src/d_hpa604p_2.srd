 retrieve="  SELECT A.MEMBER_NO,
         A.YEAR,
         A.LIVE_GBN,   
         A.WIFE_NUM,
         A.SUPPORT_20_NUM,   
         A.SUPPORT_60_NUM,
         A.OLD_NUM,
         A.HANDICAP_NUM,   
         A.WOMAN_GBN,
         A.CHILD_NUM,         
         B.NAME,
         B.JUMIN_NO,
         B.DUTY_CODE,   
        	E.CAMPUS_NAME,
   		E.BUSINESS_NO 
	FROM  PADB.HPA019H A,   
         INDB.HIN001M B,   
       	CDDB.KCH000M E
   WHERE A.MEMBER_NO = B.MEMBER_NO  and  
         A.YEAR    = :as_year  AND  
         B.GWA 	   like :as_gwa||'%'  AND  
         A.MEMBER_NO >= :as_str_member  AND  
         A.MEMBER_NO <= :as_end_member  
AND		TO_CHAR(B.JAEJIK_OPT) LIKE :AS_JAEJIK || '%'
AND		DECODE(:AS_JAEJIK,'3',B.RETIRE_DATE,:AS_YEAR) LIKE :AS_YEAR || '%'
AND		B.DUTY_CODE	  IN  ('101','102','103','104','111','112','113','114','115','301')
ORDER BY B.DUTY_CODE ASC,   
         B.MEMBER_NO ASC   
" arguments=(("as_year", string),("as_gwa", string),("ai_str_jikjong", number),("ai_end_jikjong", number),("as_str_member", string),("as_end_member", string),("as_jaejik", string)) )
