         A.SUPPORT_60_NUM,          A.OLD_NUM,         	A.HANDICAP_NUM,   
         A.WOMAN_GBN,               A.CHILD_NUM,         A.INSURE_PENSION_AMT,   
         PADB.FU_INSURE_AMT(A.Year, A.Member_No, '3') INSURE_MEDICAL_AMT, A.INSURE_EMPLOYEE_AMT,  
			padb.fu_insure_amt(A.YEAR, A.member_no,'4') insure_fee,
		   padb.fu_insure_amt(A.YEAR, A.member_no,'5') handicap_fee,  
		   padb.fu_medical_amt(A.YEAR,A.member_no,d.pay_total_amt) medical_fee,
		   padb.fu_education_amt(A.Year,A.member_no)  education_fee,
         A.MEDICAL_OLD_AMT,         A.MEDICAL_HANDICAP_AMT, A.MEDICAL_AMT,   
         A.EDUCATION_SELF_AMT,      A.EDUCATION_CHILDREN_AMT,A.EDUCATION_AMT1,   
         A.EDUCATION_AMT2,          A.EDUCATION_HANDICAP_AMT,A.EDUCATION_AMT,   
         A.CONSTRIBUTION_LEGAL_AMT, A.CONSTRIBUTION_GEN_AMT, A.CONSTRIBUTION_ASSGN_AMT, 
		   A.INSURE_LIFE_AMT1,		   A.INSURE_LIFE_AMT2,		 A.INSURE_HANDICAP_AMT , 
         A.CONSTRIBUTION_AMT,       A.PENSION_INDI_AMT,     A.PENSION_AMT,   
         A.INVESTMENT_20_AMT,       A.INVESTMENT_30_AMT,    A.CARD_USED_AMT,   
         A.CARD_GIRO,               A.CARD_PASS,            A.CARD_DETUCT_AMT,   
         A.FOREIGN_INCOME,          A.HOUSE_INCOME_AMT,     A.FUND_AMT1,   
         A.FUND_AMT2,               A.FOREIGN_AMT,          A.EXCEPTION_LAW_AMT,   
         B.NAME,                    B.JUMIN_NO,             B.DUTY_CODE,   
         B.SAL_CLASS,               C.HOME_ADDR1,           C.HOME_ADDR2,   
         D.NON_FOREIGN_AMT,         D.NON_NIGHT_AMT,        D.NON_ETC_AMT,   
         D.NON_TAX_TOT,             D.PAY_TOTAL_AMT,        D.LABOR_INCOME_AMT,   
         D.INCOME_DETUCT_AMT,       D.INCOME_TAX_AMT,       D.BASIC_SELF_AMT,   
         D.BASIC_WIFE_AMT,          D.BASIC_FAMILY_AMT,     D.BASIC_AMT,   
         D.ADD_OLD_AMT,             D.ADD_HANDICAP_AMT,     D.ADD_WOMAN_AMT,   
         D.ADD_CHILD_AMT,           D.ADD_AMT,              D.ADD_DETUCT_AMT,   
         D.INSURE_PENSION_AMT,      D.SPECIAL_INSURE_AMT,   D.SPECIAL_MEDICAL_AMT,   
         D.SPECIAL_EDUCATION_AMT,   D.SPECIAL_HOUSE_AMT,    D.SPECIAL_CONTRIBUTE_AMT,   
         D.SPECIAL_AMT,             D.STANDARD_AMT,         D.MINUS_INCOME_AMT,   
         D.PENSION_INDI_AMT,        D.PENSION_SAVE_AMT,     D.INVEST_INCOME_AMT,   
         D.FUND_AMT,                D.EDUCATION_AMT,        D.CARD_AMT,   
         D.TOTAL_INCOME_STD,        D.TAX_RATE,             D.PRODUCT_TAX,   
         D.EXCEPTION_LAW_AMT,       D.INCOME_LAW_AMT,       D.LAW_AMT,   
         D.LABOR_INCOME_TAX,        D.UNION_TAX,            D.HOUSE_LOAN_TAX,   
         D.SAVING_TAX,              D.FOREIGN_TAX,          D.TAX_TAX,   
         D.DECISION_INCOME_TAX,     D.DECISION_JUMIN_TAX,   D.DECISION_FARM_TAX,   
         D.DECISION_TAX,            D.PREPAY_INCOME_TAX,    D.PREPAY_JUMIN_TAX,   
         D.PREPAY_FARM_TAX,         D.PREPAY_TAX,           D.TAX_AMT,   
         D.JUMIN_AMT,               D.FARM_AMT,             D.TAX_TOT,   
         D.BALANCE_TAX_AMT,         D.BALANCE_JUMIN_AMT,    D.BALANCE_FARM_AMT,   
         D.BALANCE_INCOME_AMT,		E.CAMPUS_NAME,   			E.BUSINESS_NO 
         
	FROM  PADB.HPA019H A,   
         INDB.HIN001M B,   
         INDB.HIN011M C,   
         PADB.HPA018H D,
			CDDB.KCH000M E
   WHERE A.MEMBER_NO = B.MEMBER_NO  and  
         B.MEMBER_NO = C.MEMBER_NO  and  
		   A.YEAR      = D.YEAR 	 AND
		   A.MEMBER_NO = D.MEMBER_NO   AND
         A.YEAR    = :as_year  AND  
         B.GWA 	   like :as_gwa||'%'  AND  
         A.MEMBER_NO >= :as_str_member  AND  
         A.MEMBER_NO <= :as_end_member  
AND		TO_CHAR(B.JAEJIK_OPT) LIKE :AS_JAEJIK || '%'
AND		DECODE(:AS_JAEJIK,'3',B.RETIRE_DATE,:AS_YEAR) LIKE :AS_YEAR || '%'
AND		B.DUTY_CODE	  NOT IN  ('101','102','103','104','107','108','109','111','112','113','114','115','301')
ORDER BY B.DUTY_CODE ASC,   
         B.MEMBER_NO ASC   
