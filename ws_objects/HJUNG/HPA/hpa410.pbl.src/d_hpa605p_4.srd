 retrieve="SELECT	A.MEMBER_NO,
			A.YEAR,
			A.JUMIN_NO,
			A.GWANGAE_CODE,
			A.FOREIGNER_GBN,
			A.NAME,
			A.BASIC_OPT,
			A.HANDICAP_OPT,
			A.CHILD_OPT,
			A.INSURE_OPT,
			A.MEDICAL_OPT,
			A.EDUCATION_OPT,
			A.CARD_OPT,
			A.add_child_opt,
			A.old_opt,
			A.woman_opt,
			A.BIRTH_OPT,
			A.INSURE_AMT,
			A.MEDICAL_AMT,
			A.EDUCATION_AMT,
			A.CARD_AMT,
			A.CASH_AMT,
			A.GIBO_AMT,
			A.A_INSURE_AMT,
			A.A_MEDICAL_AMT,
			A.A_EDUCATION_AMT,
			A.A_CARD_AMT,
			A.A_CASH_AMT,
			A.A_GIBO_AMT
FROM		PADB.HPA024M A,
			PADB.HPA018H B,
			INDB.HIN001M C
WHERE		A.YEAR			=  B.YEAR
AND		A.MEMBER_NO 	=  B.MEMBER_NO
AND		A.MEMBER_NO 	=  C.MEMBER_NO
AND		A.YEAR			=  :AS_YEAR
AND		B.GWA 			LIKE :AS_GWA||'%'
AND		B.JIKJONG_CODE >= :ai_str_jikjong
AND		B.JIKJONG_CODE <= :ai_END_jikjong
AND		A.MEMBER_NO 	>=  :AS_STR_MEMBER
AND		A.MEMBER_NO 	<=  :AS_END_MEMBER
AND		C.JAEJIK_OPT LIKE :AS_JAEJIK||'%'
ORDER	BY	A.MEMBER_NO, A.GWANGAE_CODE, A.JUMIN_NO" arguments=(("AS_YEAR", string),("AS_GWA", string),("ai_str_jikjong", number),("ai_end_jikjong", number),("AS_STR_MEMBER", string),("AS_end_MEMBER", string),("as_jaejik", string)) )
