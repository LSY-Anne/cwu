 retrieve="
select	tax_office, business_no, campus_name, president, address, tel_phone,
			(	select	count(distinct member_no)
				from		padb.hpa018h
				where		year	=	:as_year	)	year_count,
			(	select	count(distinct member_no)
				from		padb.hpa017m
				where		year	=	:as_year	)	bef_count,
			(	select	nvl(sum(labor_income_amt), 0)
				from		padb.hpa018h
				where		year	=	:as_year )	income_import_amt,
			(	select	nvl(sum(non_tax_tot), 0)
				from		padb.hpa018h
				where		year	=	:as_year	)	year_nontax_amt,
			(	select	nvl(sum(balance_income_amt), 0)
				from		padb.hpa018h
				where		year	=	:as_year	)	balance_tax_amt
from		cddb.kch000m
where		campus_code	=	(	select	min(campus_code)
									from		cddb.kch000m	)
" arguments=(("as_year", string),("as_uname", string),("as_date", string)) )
