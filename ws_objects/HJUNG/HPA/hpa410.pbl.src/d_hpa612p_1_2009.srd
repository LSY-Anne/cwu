 retrieve="SELECT	 E.CAMPUS_NAME, E.BUSINESS_NO, E.ADDRESS, E.TEL_PHONE, E.PRESIDENT, A.p46yar as YEAR, E.CORP_NO,
	 		COUNT(A.p46nno) 		 						   			TCNT,
	 		SUM(A.P46ICW)	+ SUM(A.P46TFM)	   			TOT_PAY,
	 		SUM(A.P46TXP)							   DECISION_INCOME,
	 		SUM(A.P46RTR)							  	DECISION_JUMIN,
	 		SUM(A.P46FTR)								DECISION_FARM
FROM    	PADB.hpap46t A,
			CDDB.KCH000M E,
            indb.hin003m_02v c
WHERE   	A.p46yar		   =		:as_Year
AND		A.p46dcd		LIKE	:as_gwa || '%'
and        trim(a.p46grd)        =        trim(c.duty_code(+))
AND		c.JIKJONG_CODE	>=		:ai_str_jikjong
AND		c.JIKJONG_CODE	<=		:ai_end_jikjong	
AND		E.CAMPUS_CODE	=	'1'
GROUP 	BY E.CAMPUS_NAME, E.BUSINESS_NO, E.ADDRESS, E.TEL_PHONE, E.PRESIDENT, A.p46yar, E.CORP_NO" arguments=(("as_year", string),("as_gwa", string),("ai_str_jikjong", number),("ai_end_jikjong", number)) )
