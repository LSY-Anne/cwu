table(column=(type=char(40) updatewhereclause=yes name=campus_name dbname="hpa018h.campus_name" )
 column=(type=char(10) updatewhereclause=yes name=business_no dbname="business_no" )
 column=(type=char(120) updatewhereclause=yes name=address dbname="address" )
 column=(type=char(16) updatewhereclause=yes name=tel_phone dbname="tel_phone" )
 column=(type=char(20) updatewhereclause=yes name=president dbname="president" )
 column=(type=char(14) updatewhereclause=yes name=corp_no dbname="corp_no" )
 column=(type=char(4) updatewhereclause=yes name=year dbname="year" )
 column=(type=number updatewhereclause=yes name=total_cnt dbname="total_cnt" )
 column=(type=number updatewhereclause=yes name=total_amt dbname="total_amt" )
 column=(type=number updatewhereclause=yes name=decision_income_tax dbname="decision_income_tax" )
 column=(type=number updatewhereclause=yes name=decision_jumin_tax dbname="decision_jumin_tax" )
 column=(type=number updatewhereclause=yes name=decision_farm_tax dbname="decision_farm_tax" )
 retrieve="
	select    distinct e.campus_name, e.business_no, e.address, e.tel_phone, e.president, e.CORP_NO, a.P46YAR as year,
                (    select    count(*)
                    from        padb.hpap46t b, indb.hin003m_02v d
                    where        b.P46YAR            =        :as_year
                    and        b.P46DCD        like    :as_gwa || '%'
                     and        trim(b.p46grd)        =        trim(d.duty_code(+))
                    and        d.jikjong_code    >=        :ai_str_jikjong
                    and        d.jikjong_code    <=        :ai_end_jikjong    )    total_cnt,
                (    select    nvl(sum(P46ICS), 0) + nvl(sum(P46TFM), 0)
                    from        padb.hpap46t b, indb.hin003m_02v d
                    where        b.P46YAR            =        :as_year
                    and        b.P46DCD        like    :as_gwa || '%'
                     and        trim(b.p46grd)        =        trim(d.duty_code(+))
                    and        d.jikjong_code    >=        :ai_str_jikjong
                    and        d.jikjong_code    <=        :ai_end_jikjong    )    total_amt,                
                (    select    nvl(sum(P46TXP), 0)
                    from        padb.hpap46t b, indb.hin003m_02v d
                    where        b.P46YAR            =        :as_year
                    and        b.P46DCD        like    :as_gwa || '%'
                     and        trim(b.p46grd)        =        trim(d.duty_code(+))
                    and        d.jikjong_code    >=        :ai_str_jikjong
                    and        d.jikjong_code    <=        :ai_end_jikjong   )    decision_income_tax,
                (    select    nvl(sum(P46RTR), 0)
                    from        padb.hpap46t b, indb.hin003m_02v d
                    where        b.P46YAR            =        :as_year
                    and        b.P46DCD        like    :as_gwa || '%'
                     and        trim(b.p46grd)        =        trim(d.duty_code(+))
                    and        d.jikjong_code    >=        :ai_str_jikjong
                    and        d.jikjong_code    <=        :ai_end_jikjong    )    decision_jumin_tax,
                (    select    nvl(sum(P46FTR), 0)
                    from        padb.hpap46t b, indb.hin003m_02v d
                    where        b.P46YAR            =        :as_year
                    and        b.P46DCD        like    :as_gwa || '%'
                     and        trim(b.p46grd)        =        trim(d.duty_code(+))
                    and        d.jikjong_code    >=        :ai_str_jikjong
                    and        d.jikjong_code    <=        :ai_end_jikjong     )    decision_farm_tax                
    from        padb.hpap46t a,
                cddb.kch000m e,
                indb.hin001m k,
                indb.hin003m_02v c
    where        a.P46YAR            =        :as_year
    and        a.p46nno        =        k.member_no
    and        k.jaejik_opt    =        1
    and        a.P46DCD        like    :as_gwa || '%'
   and        trim(a.p46grd)        =        trim(c.duty_code(+))
    and        c.jikjong_code    >=        :ai_str_jikjong
    and        c.jikjong_code    <=        :ai_end_jikjong   	" arguments=(("as_year", string),("as_gwa", string),("ai_str_jikjong", number),("ai_end_jikjong", number)) )
