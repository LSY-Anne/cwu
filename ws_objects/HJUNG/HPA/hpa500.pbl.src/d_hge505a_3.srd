         A.PAY_ITEM_CODE, 
         A.PAY_TIME,   
         A.PAY_AMT,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.HOLIDAY_OPT,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE,   
         B.PRICE_PER_TIME,   
         A.RETRO_AMT,   
         A.MONTH_AMT,
			FU_CODE_NM('COMM','SEC_CODE',B.SEC_CODE,'K') PAY_ITEM_NAME
    FROM GEDB.HPA116T A, HAKSA.D_HPA102M B
   WHERE ( A.PAY_ITEM_CODE = B.PAY_ITEM_CODE) AND
			( ( A.YEAR = :as_year ) AND  
         ( A.HAKGI  = :as_hakgi ) AND  
         ( A.MONTH  = :ai_month ) AND  
         ( A.MEMBER_NO = :as_member_no ) )   
