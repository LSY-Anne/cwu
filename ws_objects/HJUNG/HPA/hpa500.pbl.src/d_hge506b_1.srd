 retrieve="
  SELECT A.MEMBER_NO,
         A.NAME,
         A.YEAR,
         A.HAKGI,
         A.GWA,
         A.JIKJONG_CODE,
         A.DUTY_CODE,
         A.SEC_CODE,
         A.SAL_CLASS,
         A.BANK_CODE,
         A.ACCT_NO,
         A.NUM_OF_TIME,
         A.NUM_OF_GENERAL,
         A.NUM_OF_MIDDLE,
         A.NUM_OF_LARGE,
         A.NUM_OF_ETC1,
         A.NUM_OF_NIGENERAL,
         A.NUM_OF_NIMIDDLE,
         A.NUM_OF_NILARGE,
         A.LIMIT_TIME,
         A.HOLIDAY_OPT,
         A.WORKER,
         A.IPADDR,
         A.WORK_DATE,
         C.FNAME AS DEPT_NAME,
         B.DUTY_NAME,   
         B.FNAME  
    FROM GEDB.HPA111M A,   
         INDB.V_HIN003M_1 B,   
         CDDB.KCH003M  C
   WHERE A.gwa 		= C.gwa (+) and  
         A.duty_code = B.duty_code (+) and  
         A.YEAR 		= :as_year  AND  
         A.HAKGI 		= :as_hakgi  AND  
         A.GWA 		like :as_gwa || '%'  AND
			A.MEMBER_NO	like	:as_member_no || '%'	 
ORDER BY A.GWA ASC,
		   A.MEMBER_NO ASC" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_member_no", string)) )
