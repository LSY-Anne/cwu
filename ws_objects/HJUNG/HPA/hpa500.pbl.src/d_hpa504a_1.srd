 retrieve="  SELECT A.MEMBER_NO,
         A.NAME,
         A.YEAR,
         A.HAKGI,
         A.GWA,
         A.JIKJONG_CODE,
         A.DUTY_CODE,
         A.BOJIK_CODE,
         A.SEC_CODE,
         A.SAL_CLASS,
         A.BANK_CODE,
         A.ACCT_NO,
         A.TRANS_MEMBER_NO,
         A.TRANS_NAME,
         A.NUM_OF_TIME,
         A.NUM_OF_GENERAL,
         A.NUM_OF_MIDDLE,
         A.NUM_OF_LARGE,
         A.NUM_OF_ETC1,
         A.NUM_OF_NIGENERAL,
         A.NUM_OF_NIMIDDLE,
         A.NUM_OF_NILARGE,
         A.NUM_OF_NIETC1,
         A.LIMIT_TIME,
         A.HOLIDAY_OPT,
         A.WORKER,
         A.WORK_DATE,
         nvl(B.DUTY_NAME, ' ') duty_name,
         nvl(B.FNAME, ' ') jikjong_fname,
         A.IPADDR,
         A.JOB_UID,
         A.JOB_ADD,
         A.JOB_DATE,
         nvl(C.FNAME, ' ') fname,
         nvl(D.BOJIK_NAME, ' ') bojik_name,
         A.TRANS_REMARK,
			E.OLD_DUTY_YN
    FROM PADB.HPA111M A,   
         INDB.V_HIN003M_1 B,   
         CDDB.KCH003M C,
         INDB.HIN005M D,
         INDB.HIN001M E
   WHERE  A.duty_code 	= B.duty_code (+) and  
          A.gwa 			= C.gwa (+) and  
          A.bojik_code 	= D.bojik_code (+) and  
          A.member_no 	= E.member_no and  
          A.YEAR 			= :as_year  AND  
          A.HAKGI 		= :as_hakgi  AND  
          A.DUTY_CODE 	like :as_duty || '%'  AND  
          A.GWA 			like :as_gwa || '%'     
ORDER BY A.GWA 			ASC,   
         A.MEMBER_NO 	ASC   
" update="PADB.HPA111M" updatewhere=0 updatekeyinplace=no arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_duty", string)) )
