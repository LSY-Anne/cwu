 retrieve="  SELECT PADB.HPA111M.MEMBER_NO,   
         PADB.HPA111M.NAME,   
         PADB.HPA111M.YEAR,   
         PADB.HPA111M.HAKGI,   
         PADB.HPA111M.GWA,   
         PADB.HPA111M.JIKJONG_CODE,   
         PADB.HPA111M.DUTY_CODE,   
         PADB.HPA111M.SEC_CODE,   
         PADB.HPA111M.SAL_CLASS,   
         PADB.HPA111M.BANK_CODE,   
         PADB.HPA111M.ACCT_NO,   
         PADB.HPA111M.NUM_OF_TIME,   
         PADB.HPA111M.NUM_OF_GENERAL,   
         PADB.HPA111M.NUM_OF_MIDDLE,   
         PADB.HPA111M.NUM_OF_LARGE,   
         PADB.HPA111M.NUM_OF_ETC1,   
         PADB.HPA111M.NUM_OF_NIGENERAL,   
         PADB.HPA111M.NUM_OF_NIMIDDLE,   
         PADB.HPA111M.NUM_OF_NILARGE,   
         PADB.HPA111M.NUM_OF_NIETC1,   
         PADB.HPA111M.LIMIT_TIME,   
         PADB.HPA111M.HOLIDAY_OPT,   
         PADB.HPA111M.WORKER,   
         PADB.HPA111M.WORK_DATE,   
         INDB.V_HIN003M_1.DUTY_NAME,   
         INDB.V_HIN003M_1.FNAME,   
         PADB.HPA111M.IPADDR,   
         CDDB.KCH003M.FNAME  
    FROM PADB.HPA111M,   
         INDB.V_HIN003M_1,   
         CDDB.KCH003M  
   WHERE ( padb.hpa111m.duty_code = indb.v_hin003m_1.duty_code (+) and  
         ( padb.hpa111m.gwa) = cddb.kch003m.gwa (+)) and  
         ( ( PADB.HPA111M.YEAR = :as_year ) AND  
         ( PADB.HPA111M.HAKGI = :as_hakgi ) AND
			( PADB.HPA111M.DUTY_CODE	LIKE	:as_duty || '%' ) AND
         ( PADB.HPA111M.GWA like :as_gwa || '%' ) )   
ORDER BY PADB.HPA111M.DUTY_CODE ASC,   
         PADB.HPA111M.MEMBER_NO ASC   
" arguments=(("as_year", string),("as_hakgi", string),("as_gwa", string),("as_duty", string)) )
