         INDB.V_HIN003M_1.DUTY_NAME ,   
         INDB.V_HIN003M_1.FNAME,   
         CDDB.KCH003M.FNAME  
    FROM GEDB.HPA111M,   
         INDB.V_HIN003M_1,   
         CDDB.KCH003M  
   WHERE ( GEDB.hpa111m.duty_code = indb.v_hin003m_1.duty_code (+)) and  
         ( GEDB.hpa111m.gwa = cddb.kch003m.gwa (+)) and  
