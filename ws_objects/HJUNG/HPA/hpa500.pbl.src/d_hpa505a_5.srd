 retrieve="  SELECT PADB.HPA112T.YEAR,   
         PADB.HPA112T.HAKGI,   
         PADB.HPA112T.MONTH,   
         PADB.HPA112T.MEMBER_NO,   
         PADB.HPA112T.WEEK_WEEKEND,   
         PADB.HPA112T.NUM_OF_TIME,   
         PADB.HPA112T.NUM_OF_GENERAL,   
         PADB.HPA112T.NUM_OF_MIDDLE,   
         PADB.HPA112T.NUM_OF_LARGE,   
         PADB.HPA112T.NUM_OF_ETC1,   
         PADB.HPA112T.NUM_OF_NIGENERAL,   
         PADB.HPA112T.NUM_OF_NIMIDDLE,   
         PADB.HPA112T.NUM_OF_NILARGE,   
         PADB.HPA112T.NUM_OF_NIETC1,   
         PADB.HPA112T.NUM_OF_OVERTIME1,   
         PADB.HPA112T.NUM_OF_OVERTIME2,   
         PADB.HPA112T.NUM_OF_OVERTIME3,   
         PADB.HPA112T.NUM_OF_OVERTIME4,   
         PADB.HPA111M.NAME,   
         INDB.V_HIN003M_1.DUTY_NAME,   
         INDB.V_HIN003M_1.FNAME,   
         PADB.HPA111M.JIKJONG_CODE,   
         PADB.HPA111M.DUTY_CODE,   
         PADB.HPA112T.LIMIT_TIME,   
         PADB.HPA112T.TIME_OF_PAUSE1,   
         PADB.HPA112T.TIME_OF_PAUSE2,   
         PADB.HPA112T.TIME_OF_PAUSE3,   
         PADB.HPA112T.TIME_OF_PAUSE4,   
         PADB.HPA112T.BOGANG_SISU_1,   
         PADB.HPA112T.BOGANG_SISU_2,   
         PADB.HPA112T.BOGANG_SISU_3,   
         PADB.HPA112T.BOGANG_SISU_4,   
         PADB.HPA112T.MISAN_SISU_1,   
         PADB.HPA112T.MISAN_SISU_2,   
         PADB.HPA112T.MISAN_SISU_3,   
         PADB.HPA112T.MISAN_SISU_4,   
         PADB.HPA112T.WORKER,   
         PADB.HPA112T.IPADDR,   
         PADB.HPA112T.WORK_DATE,   
         PADB.HPA112T.WORK_GBN,   
         CDDB.KCH003M.FNAME,   
         PADB.HPA111M.GWA  
    FROM PADB.HPA111M,   
         INDB.V_HIN003M_1,   
         PADB.HPA112T,   
         CDDB.KCH003M  
   WHERE ( PADB.hpa111m.duty_code = indb.v_hin003m_1.duty_code (+)) and  
         ( PADB.hpa111m.gwa = cddb.kch003m.gwa (+)) and  
         ( PADB.HPA111M.YEAR = PADB.HPA112T.YEAR ) and  
         ( PADB.HPA111M.HAKGI = PADB.HPA112T.HAKGI ) and  
         ( PADB.HPA111M.MEMBER_NO = PADB.HPA112T.MEMBER_NO ) and  
         ( ( PADB.HPA111M.YEAR = :as_year ) AND  
         ( PADB.HPA111M.HAKGI = :as_hakgi ) AND  
         ( PADB.HPA111M.JIKJONG_CODE >= :ai_str_jikjong ) AND  
         ( PADB.HPA111M.JIKJONG_CODE <= :ai_end_jikjong ) AND  
         ( PADB.HPA112T.WEEK_WEEKEND >= :ai_str_week ) AND  
         ( PADB.HPA112T.WEEK_WEEKEND <= :ai_end_week ) AND  
         ( PADB.HPA111M.GWA like :as_gwa ) ) AND
         (  PADB.HPA111M.DUTY_CODE like :as_duty_code||'%')   
ORDER BY PADB.HPA111M.GWA ASC,   
         PADB.HPA111M.DUTY_CODE ASC,   
         PADB.HPA112T.MEMBER_NO ASC,   
         PADB.HPA112T.MONTH	ASC,   
         PADB.HPA112T.WEEK_WEEKEND ASC   
" arguments=(("as_year", string),("as_hakgi", string),("ai_str_week", number),("ai_end_week", number),("ai_str_jikjong", number),("ai_end_jikjong", number),("as_gwa", string),("as_duty_code", string))  sort="hpa111m_gwa A hpa111m_duty_code A hpa112t_member_no A hpa112t_week_weekend A " )
