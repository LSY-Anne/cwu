 retrieve="	
	select	rtrim(a.member_no) 	member_no,
				rtrim(a.name)			name,
				a.jaejik_opt,
				a.duty_code,
				b.jikjong_code,
				rtrim(b.duty_name)	duty_name,
				c.bank_code,
				c.acct_no,
				c.depositor,
				c.pay_class,
				c.use_yn,
				a.gwa
	from		indb.hin001m a, indb.v_hin003m_1 b, padb.hpa020m c
	where		b.jikjong_code	>=	:ai_str_jikjong
	and		b.jikjong_code <=	:ai_end_jikjong
	and 		a.gwa		like	:as_gwa||'%'
	and		substr(b.duty_code, 1, 1) in ('1', '3')
	and		a.duty_code		=	b.duty_code (+)
	and		a.member_no		=	c.member_no	(+)
	order by a.name,
				a.gwa,
				a.duty_code,
				c.pay_class,
				c.bank_code" arguments=(("ai_str_jikjong", number),("ai_end_jikjong", number),("as_gwa", string))  sort="hin001m_duty_code A member_no A " )
