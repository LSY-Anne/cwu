       b.name,
       case when nvl(a.home_phoneno2, ' ') <> ' ' then a.home_phoneno1 || ')' || a.home_phoneno2 || '-' || a.home_phoneno3 end phone_no,
       case when nvl(a.cell_phoneno2, ' ') <> ' ' then a.cell_phoneno1 || ')' || a.cell_phoneno2 || '-' || a.cell_phoneno3 end cell_no,
       case when nvl(a.home_addr1, ' ')    <> ' ' then substr(a.home_postno, 1, 3) || '-' || substr(a.home_postno, 4, 6) || ') ' || home_addr1 || ' ' || home_addr2 end home_addr
