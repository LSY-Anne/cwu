 retrieve="   SELECT A.BUIL_NO, 
          B.BUIL_NAME,  
         A.OPER_DATE,   
         A.TON_CNT,   
         A.FROM_TIME1,   
         A.TO_TIME1,   
         A.FROM_TIME2,   
         A.TO_TIME2,   
         A.FROM_TIME3,   
         A.TO_TIME3,   
         A.FROM_TIME4,   
         A.TO_TIME4,
         A.FROM_TIME5,   
         A.TO_TIME5,   
         A.FROM_TIME6,   
         A.TO_TIME6,   
         A.FROM_TIME7,   
         A.TO_TIME7,   
         A.FROM_TIME8,   
         A.TO_TIME8,      
         A.TANK_AMOUNT,   
         A.IBGO_AMOUNT,   
         A.AMOUNT,   
         A.JAN_AMOUNT,   
         A.REMARK,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
  FROM STDB.HST305H A,
  		 STDB.HST240M B
  WHERE 	A.BUIL_NO = B.BUIL_NO
  AND    A.BUIL_NO like :as_buil_no||'%'
  AND		A.OPER_DATE  BETWEEN :as_fr_date and :as_to_date 
" update="STDB.HST305H" updatewhere=1 updatekeyinplace=no arguments=(("as_buil_no", string),("as_fr_date", string),("as_to_date", string))  sort="buil_no A oper_date A " )
