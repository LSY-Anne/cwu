 retrieve="  SELECT a.ITEM_ENAME,   
         a.ITEM_NAME,   
         b.ID_NO,   
         b.PURCHASE_DATE,   
         b.ITEM_STAND_SIZE,   
         b.MODEL,   
         b.SERIAL_NO,   
         b.MAKER,   
         b.NATION_CODE,   
         b.CUST_NO,   
         b.PURCHASE_QTY,   
         b.REVENUE_OPT,   
         b.PURCHASE_PRICE,   
         b.PURCHASE_AMT,   
         b.GWA,   
         b.ROOM_CODE,   
         b.LIMIT_TERM,   
         c.buil_no,
         d.useful_opt,
         e.apply_member_no,
         f.inve_status,
         b.item_no
   FROM  STDB.HST004M  a,   
         STDB.HST027M  b,
         stdb.hst242m  c,
         stdb.hst109h  d,
         stdb.hst105h  e,
         stdb.hst033h  f,
         stdb.hst029h  g
   WHERE a.ITEM_NO    = b.ITEM_NO 
   and   b.room_code  = c.room_code(+)
   and   b.item_no    = d.item_no(+)
   and   b.item_class = d.item_class(+)
   and   b.item_no    = e.item_no(+)
   and   b.id_no      = f.id_no(+)
   and   b.item_class = f.item_class(+)
   and   b.id_no      = g.id_no(+)
   and   b.item_class = g.item_class(+)
   and   b.id_no      =  :as_id_no
   and   b.gwa        =  :as_gwa


" arguments=(("as_id_no", string),("as_gwa", string)) )
