           A.ITEM_SUB_DATE,
           A.ITEM_SUB_NAME,
           A.ITEM_STAND_SIZE,
           A.MODEL,
           A.ITEM_AMT,
           A.CUST_NAME,
           B.ITEM_SUB_NO,
           B.ITEM_SUB_DATE,
           B.ITEM_SUB_NAME,
           B.ITEM_STAND_SIZE,
           B.MODEL,
           B.ITEM_AMT,
           B.CUST_NAME 
  From
(select A.*
  from        
     (select  rownum  row_num,
               A.ITEM_SUB_NO,
               A.ITEM_SUB_DATE,
               A.ITEM_SUB_NAME,
               A.ITEM_STAND_SIZE,
               A.MODEL,
               A.ITEM_AMT,
               A.CUST_NAME
       from   stdb.hst028h  A
       where  A.id_no = :as_id_no
       order by A.ITEM_SUB_DATE desc) A
   where a.row_num in (1,3,5,7)) a,
(select A.row_num - 1 row_num,
               A.ITEM_SUB_NO,
               A.ITEM_SUB_DATE,
               A.ITEM_SUB_NAME,
               A.ITEM_STAND_SIZE,
               A.MODEL,
               A.ITEM_AMT,
               A.CUST_NAME
  from        
     (select  rownum  row_num,
               A.ITEM_SUB_NO,
               A.ITEM_SUB_DATE,
               A.ITEM_SUB_NAME,
               A.ITEM_STAND_SIZE,
               A.MODEL,
               A.ITEM_AMT,
               A.CUST_NAME
       from   stdb.hst028h  a
       where   A.id_no = :as_id_no
       order by A.ITEM_SUB_DATE desc) A
   where a.row_num in (2,4,6,8)) b
where a.row_num = b.row_num(+)" arguments=(("as_id_no", string)) )
