 retrieve="  SELECT A.ID_NO,   
         A.ITEM_NO, 
         B.ITEM_NAME,    
         A.ITEM_CLASS,   
         A.PURCHASE_DATE,   
         A.ITEM_STAND_SIZE,   
         A.MODEL,   
         A.SERIAL_NO,   
         A.MAKER,   
         A.CUST_NO,   
         A.REVENUE_OPT,   
         A.ITEM_SUB,
         A.PURCHASE_PRICE, 
         A.PURCHASE_QTY,  
         NVL(PURCHASE_AMT,0) 	PURCHASE_AMT,   
         NVL(NATION_AMT,0) 	NATION_AMT,   
         NVL(SCHOOL_AMT,0) 	SCHOOL_AMT,   
         NVL(REPLACE_AMT,0) 	REPLACE_AMT,   
         NVL(PREPARE_AMT,0)   PRE_AMT,   
         NVL(SELF_AMT,0)  		SELF_AMT,
         A.GWA,   
         A.ROOM_CODE,   
         A.FAC_CODE,   
         A.OPER_OPT,   
         A.PURCHASE_OPT,   
         NVL(LIMIT_TERM,0) LIMIT_TERM,   
         A.IN_NO,   
         A.MGR_GWA,   
         A.ACCT_CODE,   
         A.EDU_BASIS,   
         A.TOOL_CLASS,   
         A.DEPR_OPT,   
         A.DEPR_RATE,   
         A.CUSTOMS_NAME,   
         A.INCOME_NO,   
         A.USEFUL ,
         A.ID_DATE,
         A.REMARK,
         A.AUDIT_NAME,
         A.LOSS_DATE,
         A.MEASURE,
			A.MANAGER,
         A.NATION_CODE
FROM  STDB.HST027M  A, 
      STDB.HST004M  B
WHERE		A.ITEM_NO = B.ITEM_NO(+)
AND      A.ID_NO     						LIKE :as_id_no
AND		NVL(A.ITEM_NO,' ')   			LIKE :as_item_no
AND		nvl(B.ITEM_NAME,' ')     		LIKE :as_item_name
AND		A.GWA       						LIKE :as_dept_code
AND		A.ID_DATE 								>= :as_date_f
AND		A.ID_DATE 								<= :as_date_t
AND		nvl(ROOM_CODE,' ')          	LIKE :as_room_code
AND		NVL(TO_CHAR(REVENUE_OPT),0)   LIKE :as_revenue_opt
AND		NVL(TO_CHAR(OPER_OPT),0)      LIKE :as_oper_opt
AND		NVL(A.acct_code,' ') 			LIKE :as_acct_code
AND		NVL(A.manager, ' ') 				LIKE :as_manager
AND		NVL(A.CUST_NO, ' ') 				LIKE :as_cust_no
AND		NVL(A.USEFUL,' ')             LIKE :as_useful||'%'
order by  A.ID_NO" arguments=(("as_id_no", string),("as_item_no", string),("as_item_name", string),("as_dept_code", string),("as_date_f", string),("as_date_t", string),("as_room_code", string),("as_revenue_opt", string),("as_oper_opt", string),("as_acct_code", string),("as_manager", string),("as_cust_no", string),("as_useful", string))  sort="id_no A " )
