 retrieve="  SELECT A.ID_NO,   
         A.ITEM_NO, 
         B.ITEM_NAME,    
         A.ITEM_CLASS,   
         A.PURCHASE_DATE,   
         A.ITEM_STAND_SIZE,   
         A.MODEL,   
         A.SERIAL_NO,   
         A.MAKER,   
         A.CUST_NO,   
         A.REVENUE_OPT,
         A.ITEM_SUB,   
         A.PURCHASE_PRICE,
   		NVL(PURCHASE_AMT,0) PURCHASE_AMT,   
         NVL(NATION_AMT,0) NATION_AMT,   
         NVL(SCHOOL_AMT,0) SCHOOL_AMT,   
         NVL(REPLACE_AMT,0) REPLACE_AMT,   
         NVL(PREPARE_AMT,0) + NVL(SELF_AMT,0) PRE_AMT,   
         A.GWA,   
         A.ROOM_CODE,
         A.FAC_CODE,   
         A.OPER_OPT,   
         A.PURCHASE_OPT,
         NVL(LIMIT_TERM,0) LIMIT_TERM,   
         A.IN_NO,   
         A.MGR_GWA,   
         A.ACCT_CODE,   
         A.EDU_BASIS,   
         A.TOOL_CLASS,   
         A.DEPR_OPT,   
         A.DEPR_RATE,   
         A.CUSTOMS_NAME,   
         A.INCOME_NO,   
         A.USEFUL ,
         A.NATION_CODE,
         A.PURCHASE_QTY,
			A.LOSS_DATE,
         A.REMARK,
			AA.TOTAL_SUM,
         AA.TOTAL_CNT
    FROM  STDB.HST027M  A, 
          STDB.HST004M  B,			
    		(
			SELECT 	NVL(SUM(TOTAL),0) TOTAL_SUM , NVL(COUNT(D.ITEM_SUB_NO),0) TOTAL_CNT
			FROM   	(
					 SELECT  		AB.ID_NO, AB.ITEM_CLASS,SUM(AB.ITEM_AMT) TOTAL, AB.ITEM_SUB_NO
					 FROM 	 		STDB.HST028H AB
					 WHERE			AB.ITEM_SUB_DATE >= :as_date_f		AND
					 					AB.ITEM_SUB_DATE <= :as_date_t
					 GROUP BY 		AB.ID_NO, AB.ITEM_CLASS, AB.ITEM_SUB_NO
					 )D,STDB.HST027M G, STDB.HST004M C
			WHERE				G.ID_NO		 						=			D.ID_NO			   
					AND		G.ITEM_CLASS 						=			D.ITEM_CLASS	
					AND		G.ITEM_NO	 						=			C.ITEM_NO(+)
					AND		G.ID_NO     						LIKE :as_id_no
					AND		NVL(G.ITEM_NO,' ')   			LIKE :as_item_no
					AND		nvl(C.ITEM_NAME,' ') 			LIKE :as_item_name
					AND		G.GWA       						LIKE :as_dept_code
					AND		G.ID_DATE 							>= :as_date_f
					AND		G.ID_DATE 							<= :as_date_t
					AND		NVL(G.ROOM_CODE,' ')          LIKE :as_room_code
					AND		NVL(TO_CHAR(G.REVENUE_OPT),0) LIKE :as_revenue_opt
					AND		NVL(TO_CHAR(G.OPER_OPT),0)    LIKE :as_oper_opt
					AND		NVL(G.ACCT_CODE,' ')   			LIKE :as_acct_code
					AND		NVL(G.manager,' ')            LIKE :as_manager
					AND		NVL(G.USEFUL,' ')             LIKE :as_useful||'%'
					AND      G.Acct_code							not in '131201'
					AND		NVL(TO_CHAR(G.OPER_OPT),0) <> SUBSTR('3%',decode(:as_oper_opt,'%',1,2),1)
					AND		NVL(TO_CHAR(G.OPER_OPT),0) <> SUBSTR('5%',decode(:as_oper_opt,'%',1,2),1)
					)AA        		 
WHERE		A.ITEM_NO = B.ITEM_NO(+)
AND      ID_NO     							LIKE :as_id_no
AND		NVL(A.ITEM_NO,' ')   			LIKE :as_item_no
AND		nvl(B.ITEM_NAME,' ')     		LIKE :as_item_name
AND		GWA       							LIKE :as_dept_code
AND		ID_DATE 								>= :as_date_f
AND		ID_DATE 								<= :as_date_t
AND		nvl(ROOM_CODE,' ')           	LIKE :as_room_code
AND		NVL(TO_CHAR(REVENUE_OPT),0)   LIKE :as_revenue_opt
AND		NVL(TO_CHAR(OPER_OPT),0)      LIKE :as_oper_opt
AND		NVL(ACCT_CODE,' ')  				LIKE :as_acct_code
AND		NVL(manager,' ')              LIKE :as_manager
AND		NVL(USEFUL,' ')               LIKE :as_useful||'%'
AND      Acct_code							not in '131201'
AND		NVL(TO_CHAR(OPER_OPT),0) <> SUBSTR('3%',decode(:as_oper_opt,'%',1,2),1)
AND		NVL(TO_CHAR(OPER_OPT),0) <> SUBSTR('5%',decode(:as_oper_opt,'%',1,2),1)
order by  A.ITEM_NO, A.ID_NO" arguments=(("as_id_no", string),("as_item_no", string),("as_item_name", string),("as_dept_code", string),("as_date_f", string),("as_date_t", string),("as_room_code", string),("as_revenue_opt", string),("as_oper_opt", string),("as_acct_code", string),("as_manager", string),("as_useful", string))  sort="item_no A id_no A " )
