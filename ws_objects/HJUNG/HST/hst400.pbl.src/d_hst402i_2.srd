			A.ITEM_CLASS,
			A.APPLY_DATE,
         A.SEQ_NO,
			A.APPLY_MEMBER_NO,
			A.MOVE_PROC_DATE,
			A.MOVE_BEF_GWA,
			A.MOVE_BEF_ROOM,
			A.GWA,
			A.ROOM_CODE,
			A.IN_ITEM_NAME,
			A.JIKWI_NAME,
			A.MOVE_OPT,
			A.REMARK,
			B.ITEM_NO,
			D.ITEM_NAME,
			B.ITEM_STAND_SIZE,
			B.MODEL,
			C.ROOM_NAME,
			A.MOVE_PROC_MEMBER_NO,
         B.PURCHASE_QTY,
         B.PURCHASE_AMT,
         B.REVENUE_OPT,
         B.PURCHASE_PRICE,
         B.NATION_AMT,
         B.SCHOOL_AMT,
         B.PREPARE_AMT,
         B.REPLACE_AMT,
         B.OPER_OPT,
         B.PURCHASE_OPT,
         B.IN_NO,
         B.TOOL_CLASS,
			A.WORKER,
			A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE
FROM		STDB.HST034H	A,
			STDB.HST027M	B,
			STDB.HST242M	C,
         STDB.HST004M   D
WHERE		A.ID_NO      = B.ID_NO
AND		A.ITEM_CLASS = B.ITEM_CLASS
AND		A.ROOM_CODE  = C.ROOM_CODE(+)
AND      B.ITEM_NO    = D.ITEM_NO(+)
AND		A.ID_NO     LIKE :as_id_no
AND		nvl(B.ITEM_NO,' ')   LIKE  :as_item_no
AND		nvl(D.ITEM_NAME,' ') LIKE  :as_item_name
AND		TO_CHAR(A.ITEM_CLASS) LIKE :as_item_class
AND		A.APPLY_DATE   >= :as_date_f
AND		A.APPLY_DATE   <= :as_date_t
AND		nvl(A.MOVE_BEF_GWA,' ') LIKE :as_dept_code
AND		A.MOVE_OPT = 1" update="STDB.HST034H" updatewhere=0 updatekeyinplace=no arguments=(("as_id_no", string),("as_item_no", string),("as_item_name", string),("as_item_class", string),("as_date_f", string),("as_date_t", string),("as_dept_code", string))  sort="move_proc_date A move_bef_gwa A " )
