 retrieve="SELECT	A.ID_NO,
			A.ITEM_NO,
			B.ITEM_NAME,
			A.ITEM_CLASS,
			A.PURCHASE_DATE,
         A.PURCHASE_QTY,
         A.PURCHASE_AMT,
			A.limit_term,
			A.ITEM_STAND_SIZE,
			A.MODEL,
			A.SERIAL_NO,
			A.MAKER,
			A.GWA,
			A.ROOM_CODE,
			C.ROOM_NAME,
			A.PURCHASE_AMT
FROM		STDB.HST027M A,
         STDB.HST004M B,
         STDB.HST242M C
WHERE    A.ITEM_NO = B.ITEM_NO(+)
AND      A.ROOM_CODE = C.ROOM_CODE(+)
AND      A.ID_NO     LIKE :as_id_no
AND		nvl(A.ITEM_NO,' ')   LIKE :as_item_code
AND		nvl(B.ITEM_NAME,' ') LIKE :as_item_name
AND		nvl(TO_CHAR(A.ITEM_CLASS),' ') LIKE :as_item_class
AND		nvl(A.GWA,' ')       LIKE :as_dept_code
AND		NVL(C.ROOM_NAME,0) LIKE :as_room_name
AND		nvl(A.OPER_OPT,0)  in (0,1,2,5)
" arguments=(("as_id_no", string),("as_item_code", string),("as_item_name", string),("as_item_class", string),("as_dept_code", string),("as_room_name", string))  sort="id_no A " )
