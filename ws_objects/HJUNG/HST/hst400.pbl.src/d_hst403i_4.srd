 retrieve="SELECT	A.ID_NO,
			A.ITEM_CLASS,
			B.ITEM_NO,
			D.ITEM_NAME,
			B.ITEM_STAND_SIZE,
			A.APPLY_DATE,
			A.RETURN_BEF_GWA,
			A.RETURN_BEF_ROOM,
			A.GWA,
			A.ROOM_CODE,
			A.IN_ITEM_NAME,
			A.REMARK MEMO,
			A.RETURN_PROC_DATE,
			C.ROOM_NAME,
         B.PURCHASE_QTY,
         B.PURCHASE_AMT
FROM		STDB.HST038H	A,
			STDB.HST027M	B,
			STDB.HST242M	C,
			STDB.HST004M     D
WHERE	A.ID_NO      = B.ID_NO
AND		A.ITEM_CLASS = B.ITEM_CLASS
AND		A.ROOM_CODE	 = C.ROOM_CODE(+)
AND      B.ITEM_NO    = D.ITEM_NO(+)
AND		nvl(C.ROOM_NAME,' ')	 LIKE	:as_room_name	  
AND		A.ID_NO     LIKE :as_id_no
AND		nvl(B.ITEM_NO,' ')   LIKE :as_item_no
AND		nvl(D.ITEM_NAME,' ') LIKE :as_item_name
AND		nvl(TO_CHAR(A.ITEM_CLASS),' ') LIKE :as_item_class
AND		A.APPLY_DATE BETWEEN  :as_date_f  AND :as_date_t
AND		nvl(A.RETURN_BEF_GWA,' ') LIKE :as_dept_code
AND		A.RETURN_OPT    = :ai_move_opt
" arguments=(("as_id_no", string),("as_item_no", string),("as_item_name", string),("as_item_class", string),("as_date_f", string),("as_date_t", string),("as_dept_code", string),("ai_move_opt", number),("as_room_name", string)) )
