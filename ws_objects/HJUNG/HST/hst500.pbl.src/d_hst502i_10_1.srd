 retrieve="SELECT	A.ID_NO,
			A.ITEM_CLASS,
			A.APPLY_MEMBER_NO,
			A.LOSS_REMARK,
			A.GWA,
			A.REMARK,
			A.APPLY_DATE,
			B.ITEM_NO,
			C.ITEM_NAME,
			B.ITEM_STAND_SIZE,
			B.MODEL,
         B.PURCHASE_QTY,
         B.PURCHASE_AMT
FROM		STDB.HST034H	A,
			STDB.HST027M	B,
			STDB.HST004M    C
WHERE    A.ID_NO      = B.ID_NO
AND		A.ITEM_CLASS = B.ITEM_CLASS
AND      B.ITEM_NO    = C.ITEM_NO(+)
AND		A.ID_NO      LIKE :as_id_no
AND		nvl(B.ITEM_NO,' ')   LIKE  :as_item_no
AND		nvl(C.ITEM_NAME,' ') LIKE  :as_item_name
AND		nvl(TO_CHAR(A.ITEM_CLASS),' ') LIKE :as_item_class
AND		A.APPLY_DATE 			>= :as_date_f
AND		A.APPLY_DATE 			<= :as_date_t
AND		nvl(A.GWA,' ') 		LIKE :as_dept_code" arguments=(("as_id_no", string),("as_item_no", string),("as_item_name", string),("as_item_class", string),("as_date_f", string),("as_date_t", string),("as_dept_code", string)) )
