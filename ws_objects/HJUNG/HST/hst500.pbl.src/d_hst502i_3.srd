 retrieve="SELECT	A.ID_NO,
			A.ITEM_CLASS,
			A.APPLY_DATE,
         A.SEQ_NO,
			A.APPLY_MEMBER_NO,
			A.MOVE_PROC_DATE,
			A.MOVE_BEF_GWA,
			A.MOVE_BEF_ROOM,
			A.GWA,
			A.ROOM_CODE,
			A.MOVE_ROOM,
			A.IN_ITEM_NAME,
			A.JIKWI_NAME,
			A.MOVE_OPT,
			B.ITEM_NO,
			D.ITEM_NAME,
			B.ITEM_STAND_SIZE,
			B.MODEL,
			B.PURCHASE_QTY,
			B.PURCHASE_AMT,
			C.ROOM_NAME,
			A.MOVE_PROC_MEMBER_NO,
         A.REMARK,
			A.LOSS_REMARK,
			A.WORKER,
			A.WORK_DATE,
			A.IPADDR,
			A.JOB_UID,
			A.JOB_ADD,
			A.JOB_DATE
FROM		STDB.HST034H	A,
			STDB.HST027M	B,
			STDB.HST242M	C,
         STDB.HST004M   D
WHERE		A.ID_NO      = B.ID_NO
AND		A.ITEM_CLASS = B.ITEM_CLASS
AND		A.ROOM_CODE  = C.ROOM_CODE(+)
AND      B.ITEM_NO    = D.ITEM_NO(+)
AND		A.ID_NO   LIKE :as_id_no
AND		NVL(D.ITEM_NAME,' ') LIKE :as_item_name
AND		nvl(TO_CHAR(A.ITEM_CLASS),' ') LIKE :as_item_class
AND		A.APPLY_DATE     >= :as_date_f
AND		A.APPLY_DATE     <= :as_date_t
AND		nvl(A.GWA,' ')       LIKE :as_dept_code
AND		NVL(C.ROOM_NAME,' ')	LIKE	:as_room_name
AND		A.MOVE_OPT        = 8
" arguments=(("as_id_no", string),("as_item_name", string),("as_item_class", string),("as_date_f", string),("as_date_t", string),("as_dept_code", string),("as_room_name", string))  sort="id_no A item_class A item_no A " )
