 retrieve="  SELECT A.STD_YM,   
         A.MEMBER_NO,   
         B.NAME,   
         A.MNG_NO,   
         A.SCJNL_DVS_CD,   
         A.OVRS_PBLC_YN,   
         A.KRF_PBLC_YN,   
         A.SCJNL_NM,   
         A.PBLC_PLC_NM,   
         A.PBLC_NTN_CD,   
         A.ISSN_NO,   
         A.PBLC_YM,   
         A.PBLC_BK_NO,   
         A.PBLC_VOL_NO,   
         A.STT_PAGE,   
         A.END_PAGE,   
         A.ORG_LANG_PPR_NM,   
         A.DIFF_LANG_PPR_NM,   
         A.TOTAL_ATHR_CNT,   
         A.PPR_LANG_DVS_CD,   
         A.IMPCT_FCTR,   
         A.SPHE_CD,   
         A.SBJT_NO,   
         A.IRB_NO,   
         A.VRFC_PPR_ID,   
         A.VRFC_DVS_CD,   
         A.ABST_CNTN,   
         A.APPR_DVS_CD,   
         A.APPR_DTTM,   
         A.APPR_CNCL_DTTM,   
         A.APPR_RTRN_CNCL_RSN_CNTN,   
         A.GUS_POINT,   
         A.EVL_SLT_YN,   
         A.EVL_STAT,   
         A.KRI_LINK_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
    FROM YGDB.HYD301T A,   
         INDB.HIN001M B  
   WHERE ( A.MEMBER_NO = B.MEMBER_NO ) and  
         ( ( A.STD_YM BETWEEN  :as_ym AND :as_ym_to) AND  
         ( :as_gwa = '%' OR B.GWA = :as_gwa ) AND  
         ( :as_member_no = '%' OR A.MEMBER_NO = :as_member_no ) AND  
         ( :as_yn = '%' OR  A.APPR_DVS_CD = :as_yn ) )    
" arguments=(("as_ym", string),("as_ym_to", string),("as_gwa", string),("as_member_no", string),("as_yn", string)) )
