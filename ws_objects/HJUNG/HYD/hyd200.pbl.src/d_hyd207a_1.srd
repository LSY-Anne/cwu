 retrieve="  SELECT A.STD_YM,   
         A.MEMBER_NO,
         B.NAME,   
         A.MNG_NO,   
         A.ANCM_ACPS_DVS_CD,   
         A.DTL_DVS_NM,   
         A.ANCM_YM,   
         A.ANCM_NTN_CD,   
         A.ANCM_PLC_NM,   
         A.PRFMC_TIMES_PRDT_CNT,   
         A.PLAN_MNG_CRP_NM,   
         A.ORG_LANG_XHBT_ANCM_NM,   
         A.DIFF_LANG_XHBT_ANCM_NM,   
         A.PRDT_NM,   
         A.XHBT_CNTN,   
         A.ANUC_PE_CNT,   
         A.APPR_DVS_CD,   
         A.APPR_DTTM,   
         A.APPR_CNCL_DTTM,   
         A.APPR_RTRN_CNCL_RSN_CNTN,   
         A.GUS_POINT,   
         A.EVL_SLT_YN,   
         A.EVL_STAT,   
         A.KRI_LINK_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
    FROM YGDB.HYD305T   A,   
         INDB.HIN001M B  
   WHERE ( A.MEMBER_NO = B.MEMBER_NO ) and  
         ( ( A.STD_YM BETWEEN :as_ym  AND :as_ym_to ) AND  
         ( :as_gwa = '%' OR B.GWA = :as_gwa ) AND  
         ( :as_member_no = '%' OR A.MEMBER_NO = :as_member_no ) AND  
         ( :as_yn = '%' OR  A.APPR_DVS_CD = :as_yn ) )    
" arguments=(("as_ym", string),("as_ym_to", string),("as_gwa", string),("as_member_no", string),("as_yn", string)) )
