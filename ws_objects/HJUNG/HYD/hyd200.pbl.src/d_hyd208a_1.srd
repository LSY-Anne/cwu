         A.CFMT_AGC_NM,   
         A.CFMT_NTN_CD,   
         A.AWRD_DVS_CD,   
         A.APPR_DTTM,   
         A.APPR_DVS_CD,   
         A.APPR_CNCL_DTTM,   
         A.APPR_RTRN_CNCL_RSN_CNTN,   
         A.GUS_POINT,   
         A.EVL_SLT_YN,   
         A.EVL_STAT,   
         A.KRI_LINK_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
    FROM YGDB.HYD306T   A,   
         INDB.HIN001M B  
   WHERE ( A.MEMBER_NO = B.MEMBER_NO ) and  
         ( ( A.STD_YM BETWEEN :as_ym AND :as_ym_to ) AND  
         ( :as_gwa = '%' OR B.GWA = :as_gwa ) AND  
         ( :as_member_no = '%' OR A.MEMBER_NO = :as_member_no ) AND  
         ( :as_yn = '%' OR  A.APPR_DVS_CD = :as_yn ) )    
