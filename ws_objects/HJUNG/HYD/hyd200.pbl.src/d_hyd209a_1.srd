 retrieve="  SELECT A.STD_YM,   
         A.MEMBER_NO,   
  B.NAME,
         A.MNG_NO,   
         A.CFRC_DVS_CD,   
         A.CFRC_NM,   
         A.CFRC_HCTR_CD,   
         A.ORG_LANG_PPR_NM,   
         A.DIFF_LANG_PPR_NM,   
         A.SCTF_CFRC_ANCM_CLCT_PPR_NM,   
         A.STT_PAGE,   
         A.END_PAGE,   
         A.HLD_AGC_NM,   
         A.ANCM_PLC_NM,   
         A.HLD_STT_DATE,   
         A.HLD_END_DATE,   
         A.ANCM_DATE,   
         A.ATHR_DVS_CD,   
         A.TOTAL_ATHR_CNT,   
         A.ASSO_ATHR_NM,   
         A.RSRCHACPS_STDY_SPHE_CD,   
         A.SBJT_NO,   
         A.APPR_DTTM,   
         A.APPR_DVS_CD,   
         A.APPR_CNCL_DTTM,   
         A.APPR_RTRN_CNCL_RSN_CNTN,   
         A.GUS_POINT,   
         A.EVL_SLT_YN,   
         A.EVL_STAT,   
         A.KRI_LINK_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  
    FROM YGDB.HYD304T   A,   
         INDB.HIN001M B  
   WHERE ( A.MEMBER_NO = B.MEMBER_NO ) and  
         ( ( A.STD_YM BETWEEN :as_ym AND :as_ym_to ) AND  
         ( :as_gwa = '%' OR B.GWA = :as_gwa ) AND  
         ( :as_member_no = '%' OR A.MEMBER_NO = :as_member_no ) AND  
         ( :as_yn = '%' OR  A.APPR_DVS_CD = :as_yn ) )    
" arguments=(("as_ym", string),("as_ym_to", string),("as_gwa", string),("as_member_no", string),("as_yn", string)) )
