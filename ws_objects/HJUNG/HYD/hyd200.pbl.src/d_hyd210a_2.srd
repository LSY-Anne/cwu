       ,(SELECT FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'HYD1430' AND  CODE = A.ACQS_DTL_DVS_CD  )  AS ACQS_DTL_DVS_CD
       ,(SELECT FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'HYD1710' AND  CODE = A.PAT_CLS_CD  ) AS PAT_CLS_CD
       ,A.ITL_PPR_RGT_NM
       ,A.SMMR_CNTN
       ,A.APPL_REG_DATE
       ,A.APPL_REG_NO
       ,A.APPL_REG_NTN_CD
       ,A.PCT_EPO_APPL_NTN_CNT
       ,A.INVT_NM
       ,A.INVT_CNT
       ,A.APPL_REGT_NM
       ,A.SBJT_NO
       ,A.APPR_DVS_CD
       ,A.APPR_DTTM
       ,A.APPR_CNCL_DTTM
       ,A.APPR_RTRN_CNCL_RSN_CNTN
       ,A.GUS_POINT
       ,A.EVL_SLT_YN
       ,A.EVL_STAT
       ,A.KRI_LINK_YN
       ,A.WORKER
       ,A.IPADDR
       ,A.WORK_DATE
       ,A.JOB_UID
       ,A.JOB_ADD
       ,A.JOB_DATE
, 'N' AS CHK
 FROM YGDB.HYD303T A
 ,INDB.HIN001M B
 WHERE A.MEMBER_NO = B.MEMBER_NO
 AND A.STD_YM = :as_std_ym
