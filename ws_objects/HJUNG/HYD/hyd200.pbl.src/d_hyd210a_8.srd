         A.MNG_NO,   
         (SELECT FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'HYD1140' AND  CODE = A.CFRC_DVS_CD  )  as CFRC_DVS_CD,   
         A.CFRC_NM,   
         A.CFRC_HCTR_CD,   
         A.ORG_LANG_PPR_NM,   
         A.DIFF_LANG_PPR_NM,   
         A.SCTF_CFRC_ANCM_CLCT_PPR_NM,   
         A.STT_PAGE,   
         A.END_PAGE,   
         A.HLD_AGC_NM,   
         A.ANCM_PLC_NM,   
         A.HLD_STT_DATE,   
         A.HLD_END_DATE,   
         A.ANCM_DATE,   
         A.ATHR_DVS_CD,   
         A.TOTAL_ATHR_CNT,   
         A.ASSO_ATHR_NM,   
         A.RSRCHACPS_STDY_SPHE_CD,   
         A.SBJT_NO,   
         A.APPR_DTTM,   
         A.APPR_DVS_CD,   
         A.APPR_CNCL_DTTM,   
         A.APPR_RTRN_CNCL_RSN_CNTN,   
         A.GUS_POINT,   
         A.EVL_SLT_YN,   
         A.EVL_STAT,   
         A.KRI_LINK_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE  ,
'N' As CHK
    FROM YGDB.HYD304T   A,   
         INDB.HIN001M B  
   WHERE ( A.MEMBER_NO = B.MEMBER_NO ) and  
         ( ( A.STD_YM = :as_ym ) AND  
         ( :as_gwa = '%' OR B.GWA = :as_gwa ) AND  
            (   A.APPR_DVS_CD = :as_yn ) )    
