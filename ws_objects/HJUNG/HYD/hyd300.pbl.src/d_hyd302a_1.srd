				B.GWA,   
				B.JIKWI_CODE,
				C.FNAME AS GWA_NM,
				D.PRINT_NM 
    FROM 	YGDB.HYD402T A,   
         		YGDB.HYD401T B,
				CDDB.KCH003M C,
				YGDB.HYD106T D  
   WHERE ( A.EVL_YEAR = B.EVL_YEAR ) and  
				( A.MEMBER_NO = B.MEMBER_NO ) and
