         B.EVL_YEAR,   
         B.HAKGI,   
         SUM(TO_NUMBER(B.HAKJUM)) AS HAKJUM,   
         SUM(TO_NUMBER(B.SCH_TIME)) AS SCH_TIME,   
         SUM(TO_NUMBER(B.WEEK_TIME)) AS WEEK_TIME
