         A.EVL_ITEM,   
         A.EVL_NM,   
         A.STD_POINT,   
         A.EVL_GRADE,   
         A.EVL_POINT
    FROM YGDB.HYK203T  A, YGDB.HYK201T B
   WHERE ( A.EVL_YM(+) = :as_ym ) AND  
         ( A.APPOINT_GB(+) = :as_gb ) AND  
A.MEMBER_NO (+) = B.MEMBER_NO AND
         ( B.MEMBER_NO = :as_member_no )   AND
          A.EVL_AREA(+) = :as_area

            
