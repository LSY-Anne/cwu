         A.APPOINT_GB,   
         B.MEMBER_NO,   
         (SELECT TO_CHAR(ORDER_SEQ) || '. ' ||FNAME FROM CDDB.KCH102D WHERE UPPER(CODE_GB) = 'HYK01' AND  CODE = :AS_AREA) 
