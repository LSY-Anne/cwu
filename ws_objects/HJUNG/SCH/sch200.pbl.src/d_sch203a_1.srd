 retrieve="  SELECT A.HOUSE_GB,   
         A.STD_YEAR,   
         A.HOUSE_REQ_NO,   
         A.RECRUIT_NO,   
         A.HOUSE_REQ_DT,   
         A.HAKBUN,   
         A.HNAME,   
         A.PER_EMAIL,   
         A.SEX,   
         A.GWA,   
         A.HAKYUN,   
         A.JUMIN_NO,   
         A.BIRTH_DT,   
         A.RELIGION_CD,   
         A.NEIS_CODE,   
         A.JOLUP_YY,   
         A.ZIP_CD,   
         A.ZIP_ADDR,   
         A.ADDR,   
         A.AREA_GB,   
         A.NATION_CD,   
         A.CELL_NO,   
         A.HOME_NO,   
         A.PARENT_NM,   
         A.PARENT_CELL,   
         A.PARENT_EMAIL,   
         A.BANK_CD,   
         A.DEPOSIT_NO,   
         A.DEPOSIT_NM,   
         A.CMS_BANK_CD,   
         A.CMS_DEPOSIT_NO,   
         A.CMS_DEPOSIT_NM,   
         A.DOOR_GB,   
         A.EXPERI_YN,   
         A.EXPERI_NO,   
         A.ENTER_TERM,   
         A.ZONE1_GB,   
         A.ZONE2_GB,   
         A.ZONE3_GB,   
         A.HABIT_GB,   
         A.SMKING_YN,   
         A.SMKING_GB,   
         A.SICK_YN,   
         A.SICK_GB,   
         A.SMS_GB,   
         A.REMARK,   
         A.DISCOUNT_YN,   
         A.ALLOCATE_NO,   
         A.CLOSE_YN,   
         A.WORKER,   
         A.IPADDR,   
         A.WORK_DATE,   
         A.JOB_UID,   
         A.JOB_ADD,   
         A.JOB_DATE,
			' ' PIC,
			B.HOUSE_CD,
			(SELECT C.HOUSE_NM FROM SCH.SAZ110M C WHERE C.HOUSE_GB = B.HOUSE_GB AND C.HOUSE_CD = B.HOUSE_CD) AS HOUSE_NM,
			B.ROOM_CD,
			B.DOOR_GB,
			B.DOOR_NO
    FROM SCH.SAZ220T A,
			SCH.SAZ250T B
   WHERE ( A.HOUSE_GB = B.HOUSE_GB (+)) AND
			( A.STD_YEAR = B.STD_YEAR (+)) AND
			( A.HOUSE_REQ_NO = B.HOUSE_REQ_NO (+)) AND
			( A.HOUSE_GB = :as_house_gb ) AND  
         ( A.STD_YEAR = :as_std_year ) AND  
         ( A.HAKBUN = :as_hakbun )    
" update="SCH.SAZ220T" updatewhere=1 updatekeyinplace=no arguments=(("as_house_gb", string),("as_std_year", string),("as_hakbun", string)) )
