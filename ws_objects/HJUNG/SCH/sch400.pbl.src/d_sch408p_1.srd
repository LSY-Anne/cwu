 retrieve="SELECT   (CASE WHEN SEX = 1 THEN '남'
             WHEN SEX = 2 THEN '여'
             END) SEX,
          TRIM(AVL_PER) AS AVL_PER,
          SUM(BUL) AS A_BUL,
		 0 AS BUL_PER,
          SUM(SANG) AS A_SANG,
		 0 AS SANG_PER
FROM     (SELECT    A.SEX,
                    A.AVL_PER,
                    0 AS BUL,
                    0 AS SANG
          FROM      SCH.SAZ120D A,
                    SCH.SAZ370T B,
                    SCH.SAZ250T C
          WHERE     C.HOUSE_GB = A.HOUSE_GB
          AND       C.STD_YEAR = A.STD_YEAR
          AND       C.HOUSE_CD = A.HOUSE_CD
          AND       C.ROOM_CD = A.ROOM_CD
          AND       C.DOOR_GB = A.DOOR_GB
          AND       B.HOUSE_GB = C.HOUSE_GB
          AND       B.STD_YEAR = C.STD_YEAR
          AND       B.ALLOCATE_NO = C.ALLOCATE_NO
          AND       B.HAKBUN = C.HAKBUN
          AND       B.HOUSE_GB = :as_house_gb
          AND       B.STD_YEAR = :as_std_year
          GROUP BY  A.SEX,
                    A.AVL_PER
          UNION ALL
          SELECT    A.SEX,
                    A.AVL_PER,
                    COUNT(*) AS BUL,
                    0 AS SANG
          FROM      SCH.SAZ120D A,
                    SCH.SAZ370T B,
                    SCH.SAZ250T C
          WHERE     C.HOUSE_GB = A.HOUSE_GB
          AND       C.STD_YEAR = A.STD_YEAR
          AND       C.HOUSE_CD = A.HOUSE_CD
          AND       C.ROOM_CD = A.ROOM_CD
          AND       C.DOOR_GB = A.DOOR_GB
          AND       B.HOUSE_GB = C.HOUSE_GB
          AND       B.STD_YEAR = C.STD_YEAR
          AND       B.ALLOCATE_NO = C.ALLOCATE_NO
          AND       B.HAKBUN = C.HAKBUN
          AND       TRIM(B.PP_GB) = '2'
          AND       B.HOUSE_GB = :as_house_gb
          AND       B.STD_YEAR = :as_std_year
          GROUP BY  A.SEX,
                    A.AVL_PER        
          UNION ALL
          SELECT    A.SEX,
                    A.AVL_PER,
                    0 AS BUL,
                    COUNT(*) AS SANG
          FROM      SCH.SAZ120D A,
                    SCH.SAZ370T B,
                    SCH.SAZ250T C
          WHERE     C.HOUSE_GB = A.HOUSE_GB
          AND       C.STD_YEAR = A.STD_YEAR
          AND       C.HOUSE_CD = A.HOUSE_CD
          AND       C.ROOM_CD = A.ROOM_CD
          AND       C.DOOR_GB = A.DOOR_GB
          AND       B.HOUSE_GB = C.HOUSE_GB
          AND       B.STD_YEAR = C.STD_YEAR
          AND       B.ALLOCATE_NO = C.ALLOCATE_NO
          AND       B.HAKBUN = C.HAKBUN
          AND       TRIM(B.PP_GB) = '1'
          AND       B.HOUSE_GB = :as_house_gb
          AND       B.STD_YEAR = :as_std_year
          GROUP BY  A.SEX,
                    A.AVL_PER) D
GROUP BY SEX, AVL_PER" arguments=(("as_house_gb", string),("as_std_year", string))  sort="sex A avl_per A " )
